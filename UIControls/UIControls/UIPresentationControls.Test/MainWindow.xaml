<mainControl:UIWindow x:Class="UIPresentationControls.Test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mainControl="clr-namespace:UIControls.MainControl;assembly=UIControls"
        Style="{DynamicResource VSUIWindowStyle}" Title="Test"
        Height="200" Width="400" Icon="overwolf.ico" >
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/UIControls;component/MainControl/UIWindowStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>

    <Grid>
        <ProgressBar IsIndeterminate="True" VerticalAlignment="Bottom"></ProgressBar>
    </Grid>
</mainControl:UIWindow>