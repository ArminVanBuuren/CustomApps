<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:ser="http://schemas.microsoft.com/2003/10/Serialization/" xmlns:tns="http://schemas.sitels.ru/marti/cdm/common/financial/v001" elementFormDefault="qualified" targetNamespace="http://schemas.sitels.ru/marti/cdm/common/financial/v001" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="http://msk-dev-foris:9245/EquipmentOperations?xsd=xsd1" namespace="http://schemas.microsoft.com/2003/10/Serialization/" />
  <xs:import schemaLocation="http://msk-dev-foris:9245/EquipmentOperations?xsd=xsd11" namespace="http://schemas.sitels.ru/marti/cdm/common/v001" />
  <xs:complexType name="ArrayOfOneTimeServicePrice">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="OneTimeServicePrice" nillable="true" type="tns:OneTimeServicePrice" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ArrayOfOneTimeServicePrice" nillable="true" type="tns:ArrayOfOneTimeServicePrice" />
  <xs:complexType name="OneTimeServicePrice">
    <xs:complexContent mixed="false">
      <xs:extension base="tns:ProductPriceBase">
        <xs:sequence>
          <xs:element minOccurs="0" name="OneTimeServiceCode" nillable="true" type="xs:string" />
          <xs:element minOccurs="0" name="OneTimeServiceId" nillable="true" type="xs:long" />
          <xs:element minOccurs="0" name="OneTimeServiceName" nillable="true" type="xs:string" />
          <xs:element minOccurs="0" name="TarifficationRules" nillable="true" type="tns:TarifficationRules" />
          <xs:element minOccurs="0" name="UserPrice" nillable="true" type="tns:UserSpecifiedPrice" />
          <xs:element minOccurs="0" name="ExternalDataId" type="ser:guid" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="OneTimeServicePrice" nillable="true" type="tns:OneTimeServicePrice" />
  <xs:complexType name="ProductPriceBase">
    <xs:sequence>
      <xs:element minOccurs="0" name="Price" nillable="true" type="tns:Price" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ProductPriceBase" nillable="true" type="tns:ProductPriceBase" />
  <xs:complexType name="Price">
    <xs:sequence>
      <xs:element minOccurs="0" name="ActualPrice" type="xs:decimal" />
      <xs:element minOccurs="0" name="BasePrice" type="xs:decimal" />
      <xs:element minOccurs="0" name="CurrencyCode" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="Discount" type="xs:decimal" />
      <xs:element minOccurs="0" name="FullPrice" type="xs:decimal" />
      <xs:element minOccurs="0" name="MetricUnitNumber" nillable="true" type="xs:int" />
      <xs:element xmlns:q1="http://schemas.sitels.ru/marti/cdm/common/v001" minOccurs="0" name="MetricUnitType" type="q1:MetricUnitType" />
      <xs:element minOccurs="0" name="Tax" type="xs:decimal" />
      <xs:element minOccurs="0" name="TaxRate" type="xs:decimal" />
      <xs:element xmlns:q2="http://schemas.sitels.ru/marti/cdm/common/v001" minOccurs="0" name="TimeUnitType" type="q2:TimeUnitType" />
      <xs:element minOccurs="0" name="UnitPrice" nillable="true" type="xs:decimal" />
      <xs:element minOccurs="0" name="ClearBasePrice" type="xs:decimal" />
      <xs:element minOccurs="0" name="BaseTax" type="xs:decimal" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Price" nillable="true" type="tns:Price" />
  <xs:complexType name="PeriodicalPrice">
    <xs:complexContent mixed="false">
      <xs:extension base="tns:ProductPriceBase">
        <xs:sequence>
          <xs:element xmlns:q3="http://schemas.sitels.ru/marti/cdm/common/v001" minOccurs="0" name="Period" nillable="true" type="q3:TimePeriod" />
          <xs:element minOccurs="0" name="PeriodicalProductCode" nillable="true" type="xs:string" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="PeriodicalPrice" nillable="true" type="tns:PeriodicalPrice" />
  <xs:complexType name="TarifficationRules">
    <xs:sequence>
      <xs:element minOccurs="0" name="BillType" type="tns:BillType" />
      <xs:element minOccurs="0" name="DoNotUpdateBalance" type="xs:boolean" />
      <xs:element minOccurs="0" name="IsFree" type="xs:boolean" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="TarifficationRules" nillable="true" type="tns:TarifficationRules" />
  <xs:simpleType name="BillType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="None" />
      <xs:enumeration value="RegularBill" />
      <xs:enumeration value="HotBill" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="BillType" nillable="true" type="tns:BillType" />
  <xs:complexType name="UserSpecifiedPrice">
    <xs:sequence>
      <xs:element minOccurs="0" name="IsTaxIncluded" type="xs:boolean" />
      <xs:element minOccurs="0" name="Price" type="xs:decimal" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="UserSpecifiedPrice" nillable="true" type="tns:UserSpecifiedPrice" />
</xs:schema>