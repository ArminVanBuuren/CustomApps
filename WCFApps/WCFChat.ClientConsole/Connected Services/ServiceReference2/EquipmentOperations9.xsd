<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.sitels.ru/marti/resourcelocking/entities" elementFormDefault="qualified" targetNamespace="http://schemas.sitels.ru/marti/resourcelocking/entities" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="ResourceAcquisitionResult">
    <xs:sequence>
      <xs:element minOccurs="0" name="ConflictingLocks" nillable="true" type="tns:ArrayOfResourceLockInfo" />
      <xs:element minOccurs="0" name="Succeeded" type="xs:boolean" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ResourceAcquisitionResult" nillable="true" type="tns:ResourceAcquisitionResult" />
  <xs:complexType name="ArrayOfResourceLockInfo">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="ResourceLockInfo" nillable="true" type="tns:ResourceLockInfo" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ArrayOfResourceLockInfo" nillable="true" type="tns:ArrayOfResourceLockInfo" />
  <xs:complexType name="ResourceLockInfo">
    <xs:complexContent mixed="false">
      <xs:extension base="tns:ResourceLock">
        <xs:sequence>
          <xs:element minOccurs="0" name="DateOfLocking" type="xs:dateTime" />
          <xs:element minOccurs="0" name="Owner" nillable="true" type="xs:string" />
          <xs:element minOccurs="0" name="OwnerType" nillable="true" type="xs:string" />
          <xs:element minOccurs="0" name="RollbackDate" nillable="true" type="xs:dateTime" />
          <xs:element minOccurs="0" name="ValidUntil" nillable="true" type="xs:dateTime" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="ResourceLockInfo" nillable="true" type="tns:ResourceLockInfo" />
  <xs:complexType name="ResourceLock">
    <xs:sequence>
      <xs:element minOccurs="0" name="LockMode" type="tns:ResourceLockMode" />
      <xs:element minOccurs="0" name="Resource" nillable="true" type="xs:string" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ResourceLock" nillable="true" type="tns:ResourceLock" />
  <xs:simpleType name="ResourceLockMode">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Shared" />
      <xs:enumeration value="Exclusive" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="ResourceLockMode" nillable="true" type="tns:ResourceLockMode" />
</xs:schema>