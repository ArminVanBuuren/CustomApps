<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.sitels.ru/marti/cdm/common/financial/v002" elementFormDefault="qualified" targetNamespace="http://schemas.sitels.ru/marti/cdm/common/financial/v002" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="PeriodicalProductInfo">
    <xs:sequence>
      <xs:element minOccurs="0" name="ChargeType" type="tns:PeriodicalPriceType" />
      <xs:element minOccurs="0" name="IsDiscrete" type="xs:boolean" />
      <xs:element minOccurs="0" name="IsFirstPeriodPaid" type="xs:boolean" />
      <xs:element minOccurs="0" name="IsFullOnly" type="xs:boolean" />
      <xs:element minOccurs="0" name="IsIrpaService" type="xs:boolean" />
      <xs:element minOccurs="0" name="NextTarifficationDate" nillable="true" type="xs:dateTime" />
      <xs:element minOccurs="0" name="TarifficationState" type="tns:CurrentTarifficationState">
        <xs:annotation>
          <xs:appinfo>
            <DefaultValue EmitDefaultValue="false" xmlns="http://schemas.microsoft.com/2003/10/Serialization/" />
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="PeriodicalProductInfo" nillable="true" type="tns:PeriodicalProductInfo" />
  <xs:simpleType name="PeriodicalPriceType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Primary" />
      <xs:enumeration value="Alternative" />
      <xs:enumeration value="WillBeSetOnTDActivation" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="PeriodicalPriceType" nillable="true" type="tns:PeriodicalPriceType" />
  <xs:simpleType name="CurrentTarifficationState">
    <xs:restriction base="xs:string">
      <xs:enumeration value="None" />
      <xs:enumeration value="Paid" />
      <xs:enumeration value="Unpaid" />
      <xs:enumeration value="InProcess" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="CurrentTarifficationState" nillable="true" type="tns:CurrentTarifficationState" />
</xs:schema>