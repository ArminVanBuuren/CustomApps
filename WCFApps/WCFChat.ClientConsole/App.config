<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_IMainService">
                    <reliableSession inactivityTimeout="00:20:00" />
                    <security mode="None" />
                </binding>
                <binding name="WSDualHttpBinding_IChatService">
                    <reliableSession inactivityTimeout="00:20:00" />
                    <security mode="None" />
                </binding>
            </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8040/MainServer" binding="wsDualHttpBinding"
                bindingConfiguration="WSDualHttpBinding_IMainService" contract="ServiceReference1.IMainService"
                name="WSDualHttpBinding_IMainService" />
            <endpoint address="http://localhost:8040/ChatServer" binding="wsDualHttpBinding"
                bindingConfiguration="WSDualHttpBinding_IChatService" contract="ServiceReference1.IChatService"
                name="WSDualHttpBinding_IChatService" />
        </client>
    </system.serviceModel>
</configuration>