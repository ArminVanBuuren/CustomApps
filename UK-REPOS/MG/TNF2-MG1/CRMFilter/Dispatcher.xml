		<plugin typeName="XSLTPlugin" fileName="Plugins.dll" config="XSLTPlugin\XSLTPlugin_out2.xml"  />
		<plugin typeName="LMSplitterPlugin" fileName="Plugins.dll" config="LMSplitterPlugin\LMSplitterPlugin.xml" />
		<plugin typeName="RouterPlugin" fileName="Plugins.dll" config="RouterPlugin\RouterPlugin_CheckInput.xml"  />
	</filter>
	<filter input=".\crmfilter_output2a" default=".\crmfilter_output" alternative=".\crmfilter_output" name="OUT_02" >
		<logging level="normal" buffer="200" />
		<plugin typeName="XSLTPlugin" fileName="Plugins.dll" config="XSLTPlugin\XSLTPlugin_out2a.xml"  />
	</filter>

</configuration>