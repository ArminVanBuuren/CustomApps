<configuration cleanerPeriod="2" remotingPort="1209" logDirectoryPath="c:\FORISLOG\CRMFilter">
	<filter input=".\crmfilter_in" default=".\crmfilter_input2" alternative=".\telcrm_output" name="IN_02" >
		<logging level="debug" buffer="200" />
		<plugin typeName="DBAdapter" fileName="DBAdapter.dll" config="DBAdapter\DBAdapter.xml" />
	</filter>

	<filter input=".\crmfilter_input2" default=".\crmfilter_input2_5" alternative=".\crmfilter_input2_5" name="Garbage_collector" >
		<logging level="debug" buffer="200" />
		<plugin typeName="RouterPlugin" fileName="Plugins.dll" config="RouterPlugin\RouterPlugin_move_to_trash.xml"  />
	</filter>

	<filter input=".\crmfilter_input2_5" default=".\crmfilter_input3" alternative=".\crmfilter_input3" name="IN_03" >
		<logging level="debug" buffer="200" />
		<plugin typeName="XSLTPlugin" fileName="Plugins.dll" config="XSLTPlugin\XSLTPlugin_mail.xml"  />
	</filter>

	<filter input=".\crmfilter_input3" default=".\telcrm_input_deliveryresult" alternative=".\telcrm_output" name="IN_04" >
		<logging level="debug" buffer="200" />
		<plugin typeName="SlowFastPlugin" fileName="SlowFast.dll" config="SlowFastPlugin\SlowFastPlugin.xml"/>
	</filter>



	<filter input=".\telcrm_output" default=".\crmfilter_output2a" alternative=".\crmfilter_output2a" name="OUT_01" >
		<logging level="debug" buffer="200" />
		<plugin typeName="ReplacePlugin" fileName="Plugins.dll" config="ReplacePlugin\ReplacePlugin_out.xml" />
