	<template procedureName="Activate_SMARTSMS" sessionStorage="Memory" sessionLifetime="180">
		<condition>
			<test item="${Recipient}" matches="^9610$"/>
			<test item="${Content}" matches="(.*)"/>
		</condition>
		<receiveTemplate>
			<Message type="serviceinterrogation" initiator="smscon" id="@{newGuid}" session_id="@{setSessionId(${customer}ACTPOPSMARTSMS)}">
				<Item customer="${Sender}">
					<Service id="POPTBSMS" action="interrogation"/>
				</Item>
				@{store(sRecipient,${Recipient})}
				@{store(sContent,${Content})}
			</Message>
		</receiveTemplate>
		<deliveryTemplate conditionEx="#{'${dbresult}'='0' and '${subscription}'='1'}" call="Activate_SMARTSMS_2">
			<set item="Content" value="${sContent}"/>
			<set item="Sender" value="${customer}"/>
			<set item="Recipient" value="9610"/>
			<set item="Encoding" value="unicodefffe"/>
		</deliveryTemplate>
		<deliveryTemplate conditionEx="#{'${dbresult}'='0' and '${subscription}'='0' and '${language_id}'='1'}" >
			<set item="Content" value="Your number can not take part in Campaign" />
			<set item="Sender" value="9610"/>
			<set item="Recipient" value="${customer}"/>
			<set item="Encoding" value="unicodefffe"/>
			<set conditionEx="0@{freeSession}"/>
		</deliveryTemplate>
		<deliveryTemplate conditionEx="#{'${dbresult}'='0' and '${subscription}'='0' and '${language_id}'='3'}" >
			<set item="Content" value="Vash nomer ne uchavstvuet v Akcii" />
			<set item="Sender" value="9610"/>
			<set item="Recipient" value="${customer}"/>
			<set item="Encoding" value="unicodefffe"/>
			<set conditionEx="0@{freeSession}"/>
		</deliveryTemplate>
		<deliveryTemplate conditionEx="#{'${dbresult}'='0' and '${subscription}'='0'}" >
			<set item="Content" value="Vash nomer ne bere uchasti v Aktsii" />
			<set item="Sender" value="9610"/>
			<set item="Recipient" value="${customer}"/>
			<set item="Encoding" value="unicodefffe"/>
			<set conditionEx="0@{freeSession}"/>
		</deliveryTemplate>
		<deliveryTemplate>
			<set item="Content" value="Oshibka podklyucheniya uslugi. Poprobuite pozdnee." />
			<set item="Sender" value="9610"/>
			<set item="Recipient" value="${customer}"/>
			<set item="Encoding" value="unicodefffe"/>
			<set conditionEx="0@{freeSession}"/>
		</deliveryTemplate>
	</template>
	<template procedureName="Activate_SMARTSMS_2" sessionStorage="Memory" sessionLifetime="180">
		<condition>
			<test item="0" matches="1"/>
		</condition>
		<receiveTemplate>
			<Message type="serviceorder" initiator="smscon" id="@{newGuid}" session_id="${session_id}">
				<Item customer="${Sender}">
					<Service id="POPSMARTSMS" action="2"/>
				</Item>
			</Message>
		</receiveTemplate>
		<deliveryTemplate conditionEx="#{${dbresult} != 0}" forward="ServiceOrderErrorHandler.SendProvisionErrorMessage" terminating="false" /><deliveryTemplate conditionEx="#{${dbresult}=document('..\..\config\SelfDescriptiveCodes.xml')/dbresults/dbresult}" forward="ServiceOrderErrorHandler.SendCRMErrorMessage" />
		<deliveryTemplate conditionEx="#{'${dbresult}'!='0' and '${dbresult}'!='320205'}">
			<set item="Content" value="Oshibka podklyucheniya uslugi. Poprobuite pozdnee."/>
			<set item="Content" condition="//Item/@dbresult=320203" value="Operaciya ne mozhet byt vypolnenna, poka nezavershitsya predydushaya."/>
			<set item="Content" condition="//Item/@dbresult=320191" value="Nedostatochno sredstv dlya vypolneniya dannoi operacii. Popolnite svoi balans i povtorite operaciyu."/>
			<set item="Content" condition="//Item/@dbresult=320204" value="Usluga &quot;Smart SMS&quot; ne predostavlyaetsya dlya Vashego tarifnogo plana."/>
			<set item="Encoding" value="unicodefffe"/>
			<set item="Sender" value="9610"/>
			<set item="Recipient" value="${customer}"/>
			<set conditionEx="0@{freeSession}"/>
		</deliveryTemplate>
		<deliveryTemplate conditionEx="#{'${language_id}'='1'}">
			<set item="Content" condition="//Item/@dbresult=0" value="Your activation request is received. The request will be processed within 24 hours of which you will be additionally informed by SMS." />
			<set item="Content" condition="//Item/@dbresult=320205" value="Service &quot;Smart SMS&quot; is already activated"/>
			<set item="Encoding" value="unicodefffe"/>
			<set item="Sender" value="9610"/>
			<set item="Recipient" value="${customer}"/>
		</deliveryTemplate>
		<deliveryTemplate conditionEx="#{'${language_id}'='3'}">
			<set item="Content" condition="//Item/@dbresult=0" value="Vash zakaz na podklyuchenie uslugi prinyat. Zapros budet obrabotan v techenie sutok, o chem dopolnitel'no Vy budete uvedomleny po SMS" />
			<set item="Content" condition="//Item/@dbresult=320205" value="Usluga &quot;Smart SMS&quot; aktivna"/>
			<set item="Encoding" value="unicodefffe"/>
			<set item="Sender" value="9610"/>
			<set item="Recipient" value="${customer}"/>
		</deliveryTemplate>
		<deliveryTemplate>
			<set item="Content" condition="//Item/@dbresult=0" value="Vashe zamovlennya na pidklyuchennya poslugy pryjnyato. Zapyt bude obrobleno protyagom doby, pro shcho Vam bude dodatkovo povidomleno za dopomogoyu SMS." />
			<set item="Content" condition="//Item/@dbresult=320205" value="Posluga &quot;Smart SMS&quot; aktyvna"/>
			<set item="Encoding" value="unicodefffe"/>
			<set item="Sender" value="9610"/>
			<set item="Recipient" value="${customer}"/>
		</deliveryTemplate>
	</template>


