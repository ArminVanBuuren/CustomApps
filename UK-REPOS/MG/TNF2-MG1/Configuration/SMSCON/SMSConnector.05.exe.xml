<?xml version="1.0" encoding="utf-8"?>
<Application configClass="FORIS.MessagingGateway.SMSConnector.SMSConnectorConfiguration" type="SMSCON">
	<SMSCenters>
		<Connection host="localhost" port="9005" systemType="502" systemId="admin" pass="admin" />
		<!--Connection host="172.20.162.150" port="2777" systemType="502" systemId="fomgt2" pass="GZ4aTMjV" /--> 
	</SMSCenters>
	<Settings>
		<Item name="ModuleType" value="SMSCON"/>
		<Item name="ModuleName" value="SMSCON_05"/>
		<Item name="DefaultCustomerSource" value="7031"/>
		<Item name="DirectDeliveryTemplate" value="23"/>
		<Item name="CRMCacheCleanInterval" value="80"/>
		<Item name="StoringInterval" value="20"/>               <!-- MG Cache. Save to DB after sec --> 
		<Item name="StoringHWM" value="50"/>                    <!-- MG Cache. Save DB after count records -->
		<Item name="HealthParam" value="500"/>
		<Item name="Window" value="500" />                      <!-- width of buffer where messages wait submit_resp -->
		<Item name="WindowExceededDelay" value="10" />          <!-- if window is full after this number of seconds message will be rescheduled MSG_STATE = RT -->
		<Item name="FetchLimit" value="0"/>                     <!-- number of Messages to fetch from DB. If zero, than all -->
		<Item name="ResponseTimeout" value="5000"/>             <!-- message will be dropped from buffer after this number of miliseconds of submit_resp waiting -->
		<Item name="SuccessResponseCodes" value="0"/>
		<Item name="StatusToResend" value="20, 88" />           <!-- resend if submit_resp status code-->
		<Item name="IntervalToResent" value="1000" />
		<Item name="IntervalIncreaseToResent" value="2000" />
		<Item name="MaxIntervalToResent" value="60000" />
		<Item name="ErrorDelay" value="30"/>
		<Item name="Protocol" value="smpp"/>
		<Item name="EnquireLinkInterval" value="10"/>
		<Item name="SMSCBindRetryDelay" value="20"/>
		<Item name="SynchroMode" value="false"/>
		<Item name="RegisteredDelivery" value="true"/>
		<Item name="SrcTon" value="0"/>
		<Item name="SrcNpi" value="0"/>
		<Item name="DstTon" value="1"/>
		<Item name="DstNpi" value="1"/>
		<Item name="SubmitEsmClass" value="0"/>
		<Item name="SubmitUssdServiceOp" value="0"/>
		<Item name="EnableUdhSplitting" value="false"/>         <!-- prevent message splitting -->
		<Item name="SplittingType" value="UDH"/>                <!-- None, UDH -->
		<Item name="ContentField" value="Payload"/>             <!-- Payload, ShortMessage  -->
		<Item name="SplitBy" value="134"/>
		<Item name="SplitAfter" value="140"/>
		<Item name="UtcOffset" value="08+"/>
		<Item name="SendSpeedLimit" value="200" />
		<Item name="ThreadMultiplifier" value="4"/>
	</Settings>
	<Anumbertransformations>
	</Anumbertransformations>
	<TemplateProcessors defaultConfig="C:\FORIS\Messaging Gateway\Configuration\SMSCON\SMSTemplateProcessor.05.xml">
		<Processor dll="DefaultTemplates.dll" class="SMSTemplateProcessor" config="C:\FORIS\Messaging Gateway\Configuration\SMSCON\SMSTemplateProcessor.05.xml"/>
	</TemplateProcessors>
	<BlackList propertyName="Sender" regExpBan="^88">
		<Ban>603221981</Ban>
		<Ban>555715</Ban>
	</BlackList>
	<Encodings>
		<Outgoing>
			<Table name="iso-8859-1" encoding="0" special="" />
			<Table name="ascii" encoding="1" special="" />
			<Table name="ascii" encoding="2" special="" />
			<Table name="iso-8859-3" encoding="3" special="" />
			<Table name="ascii" encoding="4" special="" />
			<Table name="windows-1251" encoding="4"/>
			<Table name="shift-jis" encoding="5" special="" />
			<Table name="iso-8859-5" encoding="6" special=""/>
			<Table name="iso-8859-8" encoding="7" special="" />
			<Table name="unicodefffe" encoding="8" special="" />
		</Outgoing>
		<Incoming>
			<Table name="iso-8859-1" encoding="0" special="" />
			<Table name="ascii" encoding="1" special="" />
			<Table name="ascii" encoding="2" special="" />
			<Table name="iso-8859-3" encoding="3" special="" />
			<Table name="windows-1251" encoding="4"/>
			<Table name="ascii" encoding="4" special="" />
			<Table name="shift-jis" encoding="5" special="" />
			<Table name="iso-8859-5" encoding="6" special="" />
			<Table name="iso-8859-8" encoding="7" special="" />
			<Table name="unicodefffe" encoding="8" special=""/>
		</Incoming>
	</Encodings>
</Application>