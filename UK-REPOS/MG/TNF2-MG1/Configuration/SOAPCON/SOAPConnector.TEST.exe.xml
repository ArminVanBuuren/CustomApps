<?xml version="1.0" encoding="utf-8" ?> 
<Application>
	<Settings>
		<Item name="ModuleType" value="SOAPCON" />
		<Item name="ModuleName" value="SOAPCON_TEST_01" />
		<Item name="ListeningPort" value="3432"/>  <!-- remoting interface listens this port -->
		<Item name="ConnectionsLimit" value="400"/> <!-- quantity of simultaneous connections is limited -->
		<Item name="ConnectionTTL" value="60"/>    <!-- remote invocation's time-to-live -->  
		<Item name="URI" value="SOAPTEST"/>            <!-- URI of service. Must be unique within the host -->  
		<Item name="SessionCleanInterval" value="300" />
		<Item name="CRMCacheCleanInterval" value="1000"/>
		<Item name="StoringInterval" value="20" />
		<Item name="StoringHWM" value="200" />
		<Item name="FetchLimit" value="200" /> 
		<Item name="HealthAlgo" value="Simple" />    <!-- Simple, Adaptive , Delta-->
		<Item name="HealthParam" value="300" />
	</Settings>
	<TemplateProcessors defaultConfig="C:\FORIS\Messaging Gateway\Configuration\SOAPCON\SOAPTemplateProcessor.TEST.xml">
		<Processor dll="DefaultTemplates.dll" class="SOAPTemplateProcessor" config="C:\FORIS\Messaging Gateway\Configuration\SOAPCON\SOAPTemplateProcessor.TEST.xml" />
	</TemplateProcessors>
</Application>
