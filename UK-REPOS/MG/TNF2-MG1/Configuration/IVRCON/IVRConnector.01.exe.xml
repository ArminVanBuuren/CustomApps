<?xml version="1.0" encoding="utf-8"?>
<Application>
	<IVRServers>
		<MedioListener host="" port="9990" scp="0x311" remoteHost=""/>
	</IVRServers>
	<Settings>
		<Item name="ModuleType" value="IVRCON"/>
		<Item name="ModuleName" value="IVRCON_01"/>
		<Item name="CRMCacheCleanInterval" value="10"/>
		<Item name="StoringInterval" value="20"/>
		<Item name="StoringHWM" value="200"/>
		<Item name="ThreadMultiplifier" value="1"/>
		<Item name="FetchLimit" value="150"/>
		<Item name="IgnoredProtocols" value="A4"/>
	</Settings>
	<TemplateProcessors defaultConfig="C:\FORIS\Messaging Gateway\Configuration\IVRCON\IVRTemplateProcessor.01.xml">
		<Processor dll="DefaultTemplates.dll" class="IVRTemplateProcessor" config="C:\FORIS\Messaging Gateway\Configuration\IVRCON\IVRTemplateProcessor.01.xml"/>
	</TemplateProcessors>
	<Replace/>
	<Monitors>
		<HeartBeat periodSec="10"/>
		<ProcessTime periodSec="60"/>
		<NoActivity defaultThresholdSec="10">
			<MonitoredInterval thresholdSec="20">
				<day from="1" to="5"/>
				<hour from="2" to="6"/>
			</MonitoredInterval>
			<MonitoredInterval thresholdSec="40">
				<day from="1" to="5"/>
				<hour from="16" to="20"/>
			</MonitoredInterval>
			<MonitoredInterval thresholdSec="5">
				<day from="3" to="4"/>
				<hour from="16" to="17"/>
			</MonitoredInterval>
		</NoActivity>
		<NoDBConnection periodSec="5"/>
		<NoDeviceConnection periodSec="5"/>
		<NoDiscSpace periodSec="10">
			<warning threshold="20"/>
			<minor threshold="30"/>
			<major threshold="40"/>
			<critical threshold="50"/>
		</NoDiscSpace>
	</Monitors>
</Application>
