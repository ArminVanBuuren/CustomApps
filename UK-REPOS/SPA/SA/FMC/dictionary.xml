<?xml version="1.0" encoding="utf-8"?>
<Dictionary>
	<MacroList>
	    <Macro Name="[SRC_BGS_LIST]" Value="SubscribersDataList/SubscribersData/BGSCollection"/>
	    <Macro Name="[DST_BGS_LIST]" Value="ExtendedRequestMessage/AdditionalData/BGSCollection"/>
	    <Macro Name="[DST_CURRENT_DATA]" Value="ExtendedRequestMessage/AdditionalData/CURRENT_FMC_DATA/BG"/>
	    <Macro Name="[GET_MSISDN]" Value="ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN"/>
	    <Macro Name="[BGS_URL]" Value="sa/rest/v1/prov/bgs"/>
	    <Macro Name="[DNS_URL]" Value="sa/rest/v1/prov/dns"/>
	</MacroList>
	<WellknownsList>
		<Wellknown Name="MSISDN" Value="xmlsubstr(&quot;ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN&quot;)"/>
		<Wellknown Name="IMSI" Value="xmlsubstr(&quot;ExtendedRequestMessage/IdentifiersList/Identifier/IMSI&quot;)"/>
	</WellknownsList>
	<ErrorsList CodePage="windows-1251">
		<Error Name="200 OK;"                        Value="SUCCESS"                      Expression="$(Error)" />
		<Error Name="400 Bad Request;"               Value="CommandConstructionError"     Expression="$(Error)" />
		<Error Name="403 Forbidden;"                 Value="AccessDenied"                 Expression="$(Error)" />
		<Error Name="404 Not Found;"                 Value="NotSupportedCommandOrAction"  Expression="$(Error)" />
		<Error Name="405 Method not allowed;"        Value="InvalidObjectState"           Expression="$(Error)" />
		<Error Name="409 Conflict;"                  Value="ExecutionError"               Expression="$(Error)" />
		<Error Name="415 Unsupported Media Type;"    Value="NotSupportedCommandOrAction"  Expression="$(Error)" />
		<Error Name="500 Internal Server Error;"     Value="HardwareSpecificError"        Expression="$(Error)" />
		<Error Name="" Value="UnregisteredError" />
	</ErrorsList>
	<RegexpsList>
		<Regexp Name="Error" Value="HTTP/1.1\s*(?&lt;ERROR&gt;[A-z0-9 ]+)" />
	</RegexpsList>
	<ConstantsList>
		<Constant Name="SYMBOL_START" Value="{"/>
		<Constant Name="SYMBOL_END" Value="}"/>
	</ConstantsList>
	<ParametersList>
		<Parameter Name="BG_ID">
			<Source Path="[DST_CURRENT_DATA]/bgid"/>
			<PossibleValues StringToKeyTable="true"/>
		</Parameter>
		<Parameter Name="BG_CompanyName">
			<Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList//UserGroupCode"/>
			<PossibleValues StringToKeyTable="true"/>
		</Parameter>
		<Parameter Name="BG_ExternalID">
			<Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList//UserGroupId"/>
			<PossibleValues StringToKeyTable="true"/>
		</Parameter>
		<Parameter Name="DN_CSNode">
			<Source Path="ExtendedRequestMessage/AdditionalData/DeviceInfo/TerminalDeviceClassId"/>
			<PossibleValues StringToKeyTable="true"/>
		</Parameter>
	</ParametersList>
	<LogicalGroupParametersList/>
	<CommandsList Root="Commands" Mask="*.xml">
	</CommandsList>
	<DecodersList Root="Decoders">
		<Decoder Path="BGSCollection.xml"/>
		<Decoder Path="FMCDescriptionError.xml"/>
	</DecodersList>
</Dictionary>