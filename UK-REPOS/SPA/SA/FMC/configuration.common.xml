<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Application Path="..\SA.config\FMC\configuration.application.xml"/>
	<Settings>
		<Add Key="ProfilerConfiguration" Value="Profiler_debug.xml" />
		<Add Key="ReportTimeout" Value="300"/>
		<Add Key="Requests.MaxQueuedPerHost" Value="100"/>
		<Add Key="Requests.PollPeriod" Value="5"/>
		<Add Key="Requests.PollIdlePeriod" Value="2"/>
		<Add Key="Responses.WritersCount" Value="5"/>
		<Add Key="InfiniteLoopWarningThreshold" Value="1000" />
		<Add Key="InfiniteLoopInterruptThreshold" Value="10000" />
	</Settings>
	<DatabaseObjects Remotable="false">
		<Add Name="FORIS.SPA.CBP" ConnectionDataType="connectionStringKey" ConnectionData="FORIS.SPA.CBP"/>
		<Add Name="FORIS.SPA.ARCHIVE" ConnectionDataType="connectionStringKey" ConnectionData="FORIS.SPA.ARCHIVE"/>
		<Add Name="FORIS.SPA.ROBP" ConnectionDataType="connectionStringKey" ConnectionData="FORIS.SPA.ROBP"/>
		<Add Name="FORIS.SPA.STATE" ConnectionDataType="connectionStringKey" ConnectionData="FORIS.SPA.STATE"/>
		<Add Name="FORIS.RI" ConnectionDataType="uri" ConnectionData="UA"/>
		<Add Name="FORIS.SPA.SRI" ConnectionDataType="connectionStringKey" ConnectionData="FORIS.SPA.SRI"/>
	</DatabaseObjects>
</Configuration>
