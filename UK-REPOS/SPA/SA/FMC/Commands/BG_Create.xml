<?xml version="1.0" encoding="UTF-8"?>
<Command Name="BG_Create" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
	<Description>It creates new business group (BG).</Description>
	<Author Name="Khovanskiy Vladimir" Date="2017-11-14+10:00"/>
	<Request>
		<RequestTemplate>
<![CDATA[
{0}
	"name" : "{2}",
	"ctxLength" : 3,
	"extId" : "{3}",
	"softphoneLicMobile" : null,
	"softphoneLicDesktop" : null
{1}
]]>
		</RequestTemplate>
		<EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
		<Item Name="SYMBOL_START" Type="Constant"/>
		<Item Name="SYMBOL_END" Type="Constant"/>
		<Item Name="BG_CompanyName" Type="Parameter"/>
		<Item Name="BG_ExternalID" Type="Parameter"/>
	</Request>
	<ErrorsList CodePage="ascii"/>
</Command>