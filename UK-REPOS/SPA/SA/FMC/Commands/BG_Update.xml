<?xml version="1.0" encoding="UTF-8"?>
<Command Name="BG_Update" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
	<Description>It updates BG with values set in request body. In case of success, 200 Ok is returned with parameters in body.</Description>
	<Author Name="Khovanskiy Vladimir" Date="2017-11-14+10:00"/>
	<Request>
		<RequestTemplate>
<![CDATA[
{0}
	"bgId" : {2}, 
	"name" : "{3}",
	"ctxLenght" : 3,
	"extId" : "{4}",
	"softphoneLicMobile" : null,
	"softphoneLicDesktop" : null
{1}
]]>
		</RequestTemplate>
		<EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
		<Item Name="SYMBOL_START" Type="Constant"/>
		<Item Name="SYMBOL_END" Type="Constant"/>
		<Item Name="BG_ID" Type="Parameter"/>
		<Item Name="BG_CompanyName" Type="Parameter"/>
		<Item Name="BG_ExternalID" Type="Parameter"/>
	</Request>
	<ErrorsList CodePage="ascii"/>
</Command>