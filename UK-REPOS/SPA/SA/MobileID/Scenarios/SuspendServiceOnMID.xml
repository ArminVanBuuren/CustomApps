<?xml version="1.0" encoding="UTF-8"?>
<scenario name="SuspendServiceOnMID">
	<objectList>
		<object name="BySubscribersSplitter" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.RequestSplitterBySubscribers" assembly="FORIS.SPA.MmlProcessing.dll"/>
		<object name="RequestDequeuer" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.RequestDequeuer" assembly="FORIS.SPA.MmlProcessing.dll"/>
		<object name="Commander" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.Processors.RequestProcessor" assembly="FORIS.SPA.MmlProcessing.dll"/>
		<object name="ResponseAssembler" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.ResponseAssembler" assembly="FORIS.SPA.MmlProcessing.dll"/>
		<object name="IfMultipleXmlConditionsResolver" class="FORIS.ScenarioProcessing.CommonUnits.IfMultipleXmlConditionsResolver" assembly="FORIS.SPA.SPF.dll"/>
		<object name="ScenarioProcessor" class="FORIS.ScenarioProcessing.Base.SPF.SubscenarioProcessingUnit" assembly="FORIS.SPA.SPF.dll" />
		<object name="DummyResponder" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.DummyResponder" assembly="FORIS.SPA.MmlProcessing.dll"/>
		<object name="IdentifiersGenerator" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.IdentifiersGenerator" assembly="FORIS.SPA.MmlProcessing.dll" />
		<object name="DataBaseProcessor" class="FORIS.ScenarioProcessing.CommonUnits.DataBaseProcessor" assembly="FORIS.SPA.SPF.dll"/>
		<object name="Switch" class="FORIS.ScenarioProcessing.CommonUnits.ParametersSwitch" assembly="FORIS.SPA.SPF.dll" />
	</objectList>
	<automat>
		<node name="BySubscribersSplitter" object="BySubscribersSplitter">
			<resultlist>
				<result value="0" node="BySubscriberRequestDequeuer"/>
			</resultlist>
			<parameterslist>
				<param name="queueName" value="requestsSplittedBySubscriberQueue"/>
			</parameterslist>
		</node>
		<node name="BySubscriberRequestDequeuer" object="RequestDequeuer">
			<resultlist>
				<result value="0" node="CheckPoPSubscriber"/>
				<result value="1" node=""/>
			</resultlist>
			<parameterslist>
				<param name="queueName" value="requestsSplittedBySubscriberQueue"/>
			</parameterslist>
		</node>
		<node name="CheckPoPSubscriber" object="IfMultipleXmlConditionsResolver">
			<resultlist>
				<result value="0" node="IdentifiersGenerator"/>
				<result value="1" node="CrutchGetBlockStatus"/>
			</resultlist>
			<parameterslist>
				<param name="ObjectNameInContext" value="xmlrequest"/>
				<param name="IfXmlPath:0" value="//RegisteredList/POPSERVMOBID"/>
			</parameterslist>
		</node>
		<node name="CrutchGetBlockStatus" object="DataBaseProcessor">
			<resultlist>
				<result value="0" node="TerminalSwitch"/>
				<result value=""  node="ResponseAssembler"/>
			</resultlist>
			<parameterslist>
				<param name="DataBaseName" value="FORIS.TelCRM.CustomerManagement"/>
				<param name="FunctionName" value="select /*+ index(tdp I_TDS_TD_ID_DF_DT_TP_ID)*/
					tdp.is_blocked
					from cust.td_product tdp, rd.service ser
					where tdp.terminal_device_id = '{0}'
					and tdp.service_id = ser.service_id
					and ser.service_code = 'SERVMOBID'
					and tdp.date_to is null"/>							
				<param name="FunctionType" value="customQuery"/>
				<param name="OutXml" value="db_unit_result"/>
				<param name="DbParameterName:0" value="b_msisdn"/>
				<param name="DbParameterDirection:0" value="Input"/>
				<param name="DbParameterType:0" value="Varchar2"/>				
				<param name="DbParameterXpathValue:0" value="ExtendedRequestMessage/IdentifiersList/Identifier/TerminalDeviceId"/>
			</parameterslist>
		</node>
		<node  name="TerminalSwitch" object="Switch">
			<resultlist>
				<result value="0" node="dummySUCCESS" />
				<result value="1" node="dummySUCCESS" />
				<result value="2" node="IdentifiersGenerator" />
			</resultlist>
			<parameterslist>
				<param name="ObjectNameInContext" value="db_unit_result" />
				<param name="XmlPath" value="root/return/record/column[@name='IS_BLOCKED']" />
				<param name="Value:2" value="0" />
			</parameterslist>
		</node>
		<node name="IdentifiersGenerator" object="IdentifiersGenerator">
			<parameterslist>
				<param name="WellknownName" value="GlobalUid" />
			</parameterslist>
			<resultlist>
				<result value="0" node="SuspendService" />
			</resultlist>
		</node>
		<node name="SuspendService" object="Commander">
			<resultlist>
				<result value="" node="ResponseAssembler"/>
			</resultlist>
			<parameterslist>
				<param name="Command" value="SuspendService"/>
				<param name="Script" value="Command"/>
				<param name="ErrorDecodingType" value="ByRegexp"/>
				<param name="CommandSource" value="Dictionary"/>
			</parameterslist>
		</node>
		<node name="dummySUCCESS" object="DummyResponder">
			<resultlist>
				<result value="" node="ResponseAssembler"/>
			</resultlist>
			<parameterslist>
				<param name="OverallResult" value="SUCCESS"/>
				<param name="DetailedResult" value="SUCCESS"/>
				<param name="Delay" value="0"/>
			</parameterslist>
		</node>
		<node name="ResponseAssembler" object="ResponseAssembler">
			<resultlist>
				<result value="" node="BySubscriberRequestDequeuer"/>
			</resultlist>
		</node>
	</automat>
</scenario>
