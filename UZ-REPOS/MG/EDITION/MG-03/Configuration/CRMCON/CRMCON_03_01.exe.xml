<Application>
  <Queues>
    <InputQueue name=".\crmfilter_output_1" />
    <OutputQueue name=".\crmfilter_input_1" timeToBeReceived="1000" timeToReachQueue="10" recoverable="false">
      <Override connector="IVRCON_03_02" timeToBeReceived="20" recoverable="true" />
      <Override connector="SMSCON_03_01" timeToBeReceived="20" />
    </OutputQueue>
  </Queues>
  <Settings>
    <Item name="ModuleName" value="CRMCON_03_01" />
    <Item name="ModuleType" value="CRMCON" />
    <Item name="CRMCacheCleanInterval" value="1200" />
    <Item name="StoringInterval" value="60" />
    <Item name="StoringHWM" value="1200" />
    <Item name="XmlEncoding" value="utf-16" />
    <Item name="MSMQLimit" value="4194304" />
    <Item name="ReportTemplates" value="^Delivery" />
    <Item name="SupportLargeMessages" value="true" />
    <Item name="ThreadMultiplifier" value="8" />
	<Item name="ExposeMessageSendingService" value="false" /> 
  </Settings>
  <TemplateToConnector>
    <Map template_id="sms -2" connector="SMSCON_03_01" template_name="DeliveryRequest" />
    <Map template_id="SMS-spam" connector="SMSCON_03_01" template_name="DeliveryRequest" />
    <Map template_id="AutoInformationOfBalance" connector="SMSCON_03_01" template_name="DeliveryRequestAutoInformation" />
    <Map template_id="SMSAutoInformation" connector="SMSCON_03_01" template_name="DeliveryRequestAutoInformation" />
    <Map template_id="SMSMonitoring" connector="SMSCON_03_01" template_name="SMSMonitoring" />
    <Map template_id="SMSlocal" connector="SMSCON_03_01" template_name="SMSlocal" />
    <Map template_id="AddToGroup_onClean" connector="SMSCON_03_01" template_name="AddToGroup_onClean_MTSGROUP" processor="SMSTemplateProcessor" />
    <Map template_id="SMSNewCustomer" connector="SMSCON_03_01" template_name="SMSNewCustomer" />
  </TemplateToConnector>
</Application>