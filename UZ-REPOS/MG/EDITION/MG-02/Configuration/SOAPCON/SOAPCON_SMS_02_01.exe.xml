<Application>
   <Settings>
      <Item name="ModuleType" value="SOAPCON" />
      <Item name="ModuleName" value="SOAP_SMSCON_02_01" />
      <Item name="ListeningPort" value="6681" />
      <!-- remoting interface listens this port -->
      <Item name="ConnectionsLimit" value="50" />
      <!-- quantity of simultaneous connections is limited -->
      <Item name="ConnectionTTL" value="60" />
      <!-- remote invocation's time-to-live -->
      <Item name="URI" value="SOAP" />
      <!-- URI of service. Must be unique within the host -->
      <Item name="SessionCleanInterval" value="120" />
      <Item name="CRMCacheCleanInterval" value="120" />
      <Item name="StoringInterval" value="20" />
      <Item name="StoringHWM" value="200" />
      <Item name="FetchLimit" value="200" />
      <Item name="HealthAlgo" value="Simple" />
      <!-- Simple, Adaptive , Delta-->
      <Item name="HealthParam" value="300" />
      <Item name="ThreadMultiplifier" value="1" />
   </Settings>
   <TemplateProcessors defaultConfig="SOAPTemplateProcessor_SMS.xml">
      <Processor dll="DefaultTemplates.dll" class="SOAPTemplateProcessor" config="SOAPTemplateProcessor_SMS.xml" />
   </TemplateProcessors>
</Application>