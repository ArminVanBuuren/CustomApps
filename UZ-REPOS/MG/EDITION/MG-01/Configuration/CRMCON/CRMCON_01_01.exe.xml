<Application>
  <Queues>
    <InputQueue name=".\crmfilter_output_1" />
    <OutputQueue name=".\crmfilter_input_1" timeToBeReceived="1000" timeToReachQueue="10" recoverable="false">
      <Override connector="IVRCON_02_02" timeToBeReceived="20" recoverable="true" />
      <Override connector="SMSCON_01_01" timeToBeReceived="20" />
    </OutputQueue>
  </Queues>
  <Settings>
    <Item name="ModuleName" value="CRMCON_01_01" />
    <Item name="ModuleType" value="CRMCON" />
    <Item name="CRMCacheCleanInterval" value="1200" />
    <Item name="StoringInterval" value="20" />
    <Item name="StoringHWM" value="1200" />
    <Item name="XmlEncoding" value="utf-16" />
    <Item name="MSMQLimit" value="4194304" />
    <Item name="ReportTemplates" value="^Delivery" />
    <Item name="SupportLargeMessages" value="true" />
    <Item name="ThreadMultiplifier" value="8" />
    <Item name="ExposeMessageSendingService" value="true" />
  </Settings>
  <TemplateToConnector>
    <Map template_id="DirectSendEmail" connector="EMAILCON_01_01" template_name="DeliveryRequest" />
    <Map template_id="fax -3" connector="FAXCON_01_01" template_name="DeliveryRequest" />
    <Map template_id="email -2" connector="EMAILCON_01_01" template_name="DeliveryRequest" />
    <Map template_id="sms -2" connector="SMSCON_01_01" template_name="DeliveryRequest" />
    <Map template_id="SMS-spam" connector="SMSCON_01_01" template_name="DeliveryRequest" />
    <Map template_id="AutoInformationOfBalance" connector="SMSCON_01_01" template_name="DeliveryRequestAutoInformation" />
    <Map template_id="SMSAutoInformation" connector="SMSCON_01_01" template_name="DeliveryRequestAutoInformation" />
    <Map template_id="SMSMonitoring" connector="SMSCON_01_01" template_name="SMSMonitoring" />
    <Map template_id="SMSInform" connector="SMSCON_01_INFORM" template_name="SMSInform" />
    <Map template_id="Template222" connector="SMSCON_01_INFORM" template_name="Template222" />
    <Map template_id="SMSInformRus" connector="SMSCON_01_INFORM" template_name="SMSInformRus" />
    <Map template_id="SMSlocal" connector="SMSCON_01_01" template_name="SMSlocal" />
    <Map template_id="AddToGroup_onClean" connector="SMSCON_01_01" template_name="AddToGroup_onClean_MTSGROUP" processor="SMSTemplateProcessor" />
    <Map template_id="SMSmnformat" connector="SMSCON_01_ISSA" template_name="SMSmnformat" />
    <Map template_id="DirectSendSMS" connector="SMSCON_01_ISSA" template_name="DeliveryRequestFromISSA" />
    <Map template_id="Email_Test" connector="EMAILCON_01_01" template_name="Email_Test" />
    <Map template_id="SMSNewCustomer" connector="SMSCON_01_01" template_name="SMSNewCustomer" />
  </TemplateToConnector>
</Application>