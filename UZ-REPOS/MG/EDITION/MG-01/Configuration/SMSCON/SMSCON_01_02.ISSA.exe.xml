<Application configClass="FORIS.MessagingGateway.SMSConnector.SMSConnectorConfiguration" type="SMSCON">
  <Settings>
    <Item name="ModuleType" value="SMSCON" />
    <Item name="ModuleName" value="SMSCON_01_02" />
    <!--Item name="MessagesLimit" value="10"/-->
    <Item name="DefaultCustomerSource" value="1771" />
    <Item name="ShortMessageMaxLength" value="0" />
    <Item name="DirectDeliveryTemplate" value="23" />
    <Item name="CRMCacheCleanInterval" value="1200" />
    <Item name="Window" value="1" />
    <!-- width of buffer where messages wait submit_resp -->
    <Item name="WindowExceededDelay" value="30" />
    <!-- if window is full after this number of seconds message will be rescheduled MSG_STATE = RT -->
    <Item name="FetchLimit" value="0" />
    <!--  number of Messages to fetch from DB. If zero, than all -->
    <Item name="ResponseTimeout" value="4000" />
    <!-- message will be dropped from buffer after this number of miliseconds of submit_resp waiting -->
    <Item name="ErrorDelay" value="30" />
    <Item name="SMSCBindRetryDelay" value="5" />
    <Item name="Protocol" value="smpp" />
    <Item name="EnquireLinkInterval" value="10" />
    <Item name="DefaultEncoding" value="unicodefffe" />
    <!--Item name="SessionCleanInterval" value="3" /-->
    <Item name="SuccessResponseCodes" value="0" />
    <Item name="StatusToResend" value="20, 88" />
    <!-- resend if submit_resp status code-->
    <Item name="SynchroMode" value="false" />
    <Item name="RegisteredDelivery" value="true" />
    <!--Item name="IntervalToResent" value="1000" />
    <Item name="IntervalIncreaseToResent" value="2000" />
    <Item name="MaxIntervalToResent" value="15000" /-->
    <Item name="SrcTon" value="1" />
    <Item name="SrcNpi" value="1" />
    <Item name="DstTon" value="1" />
    <Item name="DstNpi" value="1" />
    <Item name="SubmitEsmClass" value="0" />
    <Item name="SubmitUssdServiceOp" value="0" />
    <Item name="EnableUdhSplitting" value="false" />
    <!-- prevent message splitting -->
    <Item name="SplittingType" value="UDH" />
    <!--  None, UDH -->
    <Item name="ContentField" value="PayLoad" />
    <!--  Payload, ShortMessage  -->
    <Item name="SplitBy" value="134" />
    <Item name="SplitAfter" value="140" />
    <Item name="StoringInterval" value="10" />
    <!-- MG Cache. Save to DB after sec -->
    <Item name="StoringHWM" value="50" />
    <!-- MG Cache. Save DB after count records -->
    <Item name="HealthParam" value="500" />
    <Item name="ThreadMultiplifier" value="10" />
    <Item name="UtcOffset" value="04+" />
    <Item name="SendSpeedLimit" value="200" />
  </Settings>
  <SMSCenters>
    <!-- 10.160.32.108 900 -->
    <Connection host="10.160.32.108" port="900" systemType="" systemId="Internethelp" pass="mediatel" />
  </SMSCenters>
  <Anumbertransformations />
  <TemplateProcessors defaultConfig="SMSTemplateProcessor.02.xml">
    <Processor dll="DefaultTemplates.dll" class="SMSTemplateProcessor" config="SMSTemplateProcessor.02.xml" />
  </TemplateProcessors>
  <BlackList propertyName="Sender" regExpBan="^88">
    <Ban>603221981</Ban>
    <Ban>555715</Ban>
  </BlackList>
  <Encodings>
    <Outgoing>
      <Table name="iso-8859-1" encoding="0" special="7bit" />
      <Table name="ascii" encoding="1" special="" />
      <Table name="ascii" encoding="2" special="" />
      <Table name="iso-8859-3" encoding="3" special="" />
      <Table name="ascii" encoding="4" special="" />
      <Table name="windows-1251" encoding="4" />
      <Table name="shift-jis" encoding="5" special="" />
      <Table name="iso-8859-5" encoding="6" special="" />
      <Table name="iso-8859-8" encoding="7" special="" />
      <Table name="unicodefffe" encoding="8" special="" />
    </Outgoing>
    <Incoming>
      <Table name="gsm 03.38" encoding="0" special="" />
      <Table name="ascii" encoding="1" special="" />
      <Table name="ascii" encoding="2" special="" />
      <Table name="iso-8859-3" encoding="3" special="" />
      <Table name="windows-1251" encoding="4" />
      <Table name="ascii" encoding="4" special="" />
      <Table name="shift-jis" encoding="5" special="" />
      <Table name="iso-8859-5" encoding="6" special="" />
      <Table name="iso-8859-8" encoding="7" special="" />
      <Table name="unicodefffe" encoding="8" special="" />
    </Incoming>
  </Encodings>
</Application>