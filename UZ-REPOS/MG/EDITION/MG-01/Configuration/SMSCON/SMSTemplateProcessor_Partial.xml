<template procedureName="DeliveryRequestNotify">
  <condition>
    <test item="0" matches="1" />
  </condition>
  <deliveryTemplate>
    <set item="Content" value="#{//Content/text()}" />
    <set item="Sender" value="MTC" />
    <set item="Recipient" value="${customer}" />
    <set item="Encoding" value="unicodefffe" />
  </deliveryTemplate>
</template>