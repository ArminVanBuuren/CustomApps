<?xml version="1.0" encoding="utf-8"?>
<Application>
	<Settings>
		<Item name="ModuleType" value="SOAPCON" />
		<Item name="ModuleName" value="SOAP_USSDCON_02_02" />
		<Item name="ListeningPort" value="6682" />
		<!-- remoting interface listens this port -->
		<Item name="ConnectionsLimit" value="150"/> <!-- quantity of simultaneous connections is limited -->
		<Item name="ConnectionTTL" value="60"/>    <!-- remote invocation's time-to-live -->  
		<Item name="URI" value="SOAP"/>            <!-- URI of service. Must be unique within the host -->  
		<Item name="SessionCleanInterval" value="6" />
		<Item name="CRMCacheCleanInterval" value="1200"/>
		<Item name="StoringInterval" value="20" />
		<Item name="StoringHWM" value="200" />
		<Item name="FetchLimit" value="200" />
		<Item name="HealthAlgo" value="Delta" />    <!-- Simple, Adaptive , Delta-->
		<Item name="HealthParam" value="2000" />
		<Item name="ThreadMultiplifier" value="4" />
                <Item name="NumberOfWorkThreads" value="8" />
	</Settings>
	<TemplateProcessors defaultConfig="Configuration\SOAPCON\SOAPTemplateProcessor_USSD.xml">
		<Processor dll="DefaultTemplates.dll" class="SOAPTemplateProcessor" config="Configuration\SOAPCON\SOAPTemplateProcessor_USSD.xml" />  
	</TemplateProcessors>  
</Application>