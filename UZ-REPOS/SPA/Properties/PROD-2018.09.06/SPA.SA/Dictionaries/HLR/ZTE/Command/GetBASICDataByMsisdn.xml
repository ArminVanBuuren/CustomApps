<Command Name="GetBASICDataByMsisdn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
   <Description>Запрос основных сервисов абонента</Description>
   <Author Name="Timur Khamzin" Date="2015-11-16+15:00" />
   <Request Template="Qry User:MSISDN={0},Item=BASIC;^M">
      <Item Name="msisdn" Type="Wellknown" />
   </Request>
   <ErrorsList CodePage="windows-1251">
      <Error Name="ERR3001" Value="NoSuchObjectInstance" />
      <Error Name="" Value="SUCCESS" />
   </ErrorsList>
</Command>