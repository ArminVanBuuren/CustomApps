<Command Name="CreateSubscriberNEWIMSI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
   <Description>Создание абонента с новым IMSI</Description>
   <Author Name="Timur Khamzin" Date="2015-11-16+15:00" />
   <Request Template="ADD User:IMSI={1},MSISDN={0},Profile={2};^M">
      <Item Name="msisdn" Type="Wellknown" />
      <Item Name="NewIMSI" Type="Parameter" />
      <Item Name="HLR_IMSI_Profile" Type="LogicalGroupParameter" />
   </Request>
   <ErrorsList CodePage="windows-1251">
      <Error Name="101041" Value="SUCCESS" />
      <!--MSISDN already exist-->
   </ErrorsList>
</Command>