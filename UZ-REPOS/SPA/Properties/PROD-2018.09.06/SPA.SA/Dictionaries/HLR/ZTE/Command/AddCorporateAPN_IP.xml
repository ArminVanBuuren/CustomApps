<Command Name="AddCorporateAPN_IP" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
   <Description>Добавление услуги 'Корпоративный APN'Статический IP</Description>
   <Author Name="Timur Khamzin" Date="2016-04-29+15:00" />
   <Request Template="MOD PDP:MSISDN={0},OAPN={1},OPDPTYPE=0,PDPAddr={2};^M">
      <Item Name="msisdn" Type="Wellknown" />
      <Item Name="apn" Type="Parameter" />
      <Item Name="IP" Type="Parameter" />
   </Request>
   <ErrorsList CodePage="windows-1251">
      <Error Name="110703" Value="SUCCESS" />
      <!--The format of the entered IP address is illegal-->
   </ErrorsList>
</Command>