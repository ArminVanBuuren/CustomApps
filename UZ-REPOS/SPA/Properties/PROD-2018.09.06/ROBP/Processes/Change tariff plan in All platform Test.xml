<BusinessProcessData name="Change tariff plan in All platform Test">
   <businessprocess>
      <scenario>
         <objectlist>
            <object name="ChangeTPOnFINPlatform" class="FORIS.ServiceProvisioning.BPM.ImplicitHostRedirectingProcessor" assembly="FORIS.SPA.BPMProcessingUnit.dll" />
            <object name="ChangeTPOnFINPlatform2" class="FORIS.ServiceProvisioning.BPM.BPMProcessingUnitConditional" assembly="FORIS.SPA.BPMProcessingUnit.dll" />
            <object name="OperationOnSA" class="FORIS.ServiceProvisioning.BPM.BPMProcessingUnit" assembly="FORIS.SPA.BPMProcessingUnit.dll" />
            <object name="DummyOnHLR" class="FORIS.ServiceProvisioning.BPM.BPMProcessingUnit" assembly="FORIS.SPA.BPMProcessingUnit.dll" />
         </objectlist>
         <automat>
            <node name="ChangeTPOnFINPlatform" object="ChangeTPOnFINPlatform">
               <parameterslist>
                  <param name="operation" value="ChangeTPOnFINPlatform" />
                  <param name="SrcXml" value="$XmlRequest" />
                  <param name="MultiLevelCondition" value="(//NewPlatform/HostTypeCode != //PreviousPlatform/HostTypeCode)" />
                  <param name="CodeTemplate" value="SC:{//NewPlatform/HostCode}" />
               </parameterslist>
               <resultlist>
                  <result value="" node="ChangeTPOnFINPlatform2" />
               </resultlist>
            </node>
            <node name="ChangeTPOnFINPlatform2" object="ChangeTPOnFINPlatform2">
               <parameterslist>
                  <param name="operation" value="ChangeTPOnFINPlatform" />
                  <param name="SrcXml" value="$XmlRequest" />
                  <param name="MultiLevelCondition" value="(//NewPlatform/HostTypeCode == //PreviousPlatform/HostTypeCode) AND (//PreviousTariffPlanId != //NewTariffPlanId)" />
               </parameterslist>
               <resultlist>
                  <result value="" node="" />
               </resultlist>
            </node>
         </automat>
      </scenario>
   </businessprocess>
</BusinessProcessData>