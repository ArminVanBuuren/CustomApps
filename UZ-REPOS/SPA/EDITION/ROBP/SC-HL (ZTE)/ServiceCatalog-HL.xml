<?xml version="1.0" encoding="utf-8"?>
<Configuration markers="*" scenarioPrefix="SC." mainIdentities="MSISDN,PersonalAccountNumber,ContractNumber" SICreation="" formatVersion="1">
  <HostTypeList>
    <HostType name="HL" nriType="RI" allowTerminalDeviceType="mobile" description="Home Location Register" />
  </HostTypeList>
  <IdentityList>
    <Identity name="ContractNumber" description="Номер контракта" />
    <Identity name="ICCID" description="Номер ICCID" />
    <Identity name="IMSI" description="Номер IMSI" />
    <Identity name="KI" path="//AdditionalData//EncryptedKi" description="Ключ шифрования" />
    <Identity name="MSISDN" description="Номер MSISDN" />
  </IdentityList>
  <AttributeList>
    <Attribute name="A3A8" path="//AdditionalData//VersionsA3A8" description="Алгоритм шифрования" />
    <Attribute name="PersonalAccountNumber" path="//IdentifiersList/Identifier/PersonalAccountNumber" description="Номер ЛС" />
    <Attribute name="TerminalDeviceId" path="//IdentifiersList/Identifier/TerminalDeviceId" description="Идентификатор ПО" />
  </AttributeList>
  <ResourceList>
    <Resource name="RES_HL_INTERNET_TEMPLATE" description="APN профиль абонента на HLR" />
  </ResourceList>
  <RFSParameterList>
    <RFSParameter name="CallForwardType" description="Тип установки переадресаций" />
    <RFSParameter name="CallForwardMode" description="Режим переадресации" />
    <RFSParameter name="ForwardedToNumber" description="Номер переадресации" />
    <RFSParameter name="CallForwardingTimer" description="Таймер переадресации" />
  </RFSParameterList>
  <FlagList />
  <RFSList>
    <RFS name="RFS_HL_CF_SERVICE" hostType="HL" description="Call forwarding service"/>
    <RFS name="RFS_HL_CF" hostType="HL" description="Активация переадресаций">
      <RFSParameter name="CallForwardType">
        <Value condition="(//ActionNameOriginal[text()='Update Call Forwarding'])" value="Provision" />
      </RFSParameter>
    </RFS>
    <RFS name="RFS_HL_CFB" hostType="HL" description="Call forwarding on mobile subscriber busy - TS10">
      <RFSParameter name="CallForwardMode">
        <Value condition="(//ProvisionList/CallForwarding/CFB)" value="Activate" />
        <Value condition="(//WithdrawalList/CallForwarding/CFB)" value="Deactivate" />
      </RFSParameter>
      <RFSParameter name="ForwardedToNumber" path="//CFB/ForwardingToAddress" />
    </RFS>
    <RFS name="RFS_HL_CFU" hostType="HL" description="Call forwarding unconditional - TS10">
      <RFSParameter name="CallForwardMode">
        <Value condition="(//ProvisionList/CallForwarding/CFU)" value="Activate" />
        <Value condition="(//WithdrawalList/CallForwarding/CFU)" value="Deactivate" />
      </RFSParameter>
      <RFSParameter name="ForwardedToNumber" path="//CFU/ForwardingToAddress" />
    </RFS>
    <RFS name="RFS_HL_CFNRC" hostType="HL" description="Call forwarding on mobile subscriber not reachable - TS10">
      <RFSParameter name="CallForwardMode">
        <Value condition="(//ProvisionList/CallForwarding/CFNRC)" value="Activate" />
        <Value condition="(//WithdrawalList/CallForwarding/CFNRC)" value="Deactivate" />
      </RFSParameter>
      <RFSParameter name="ForwardedToNumber" path="//CFNRC/ForwardingToAddress" />
    </RFS>
    <RFS name="RFS_HL_CFNRY" hostType="HL" description="Call forwarding on no reply - TS10">
      <RFSParameter name="CallForwardMode">
        <Value condition="(//ProvisionList/CallForwarding/CFNRY)" value="Activate" />
        <Value condition="(//WithdrawalList/CallForwarding/CFNRY)" value="Deactivate" />
      </RFSParameter>
      <RFSParameter name="ForwardedToNumber" path="//CFNRY/ForwardingToAddress" />
      <RFSParameter name="CallForwardingTimer" path="//CFNRY/NoReplyTimer" />
    </RFS>
    <RFS name="RFS_HL_CFD" hostType="HL" description="Call forwarding (to MCA) on mobile subscriber busy">
      <RFSParameter name="CallForwardMode">
        <Value condition="(//ProvisionList)" value="Activate" />
      </RFSParameter>
      <RFSParameter name="ForwardedToNumber" value="998970000007" />
    </RFS>
    <RFS name="RFS_HL_RBT" hostType="HL" description="GOODOK"/>
    <RFS name="RFS_HL_SERVICE_INTERNET" hostType="HL" description="Мобильный интернет GPRS"/>
    <RFS name="RFS_HL_SERVICE_LTE" hostType="HL" description="Мобильный интернет LTE"/>
  </RFSList>
  <CFSList>
    <CFS name="Subscriber" type="ManagedObject" description="Абонент" />
    <CFS name="CallForwarding" description="Активация пользовательской переадресации вызова">
      <RFS name="RFS_HL_CF" linkType="Add" />
      <RFS name="RFS_HL_CFB" linkType="Info" />
      <RFS name="RFS_HL_CFU" linkType="Info" />
      <RFS name="RFS_HL_CFNRC" linkType="Info" />
      <RFS name="RFS_HL_CFNRY" linkType="Info" />
    </CFS>
    <CFS name="CB532" description="Переадресация вызова (периодическая)">
      <RFS name="RFS_HL_CF_SERVICE" linkType="Add" />
      <RFS name="RFS_HL_CFB" linkType="Info" />
      <RFS name="RFS_HL_CFU" linkType="Info" />
      <RFS name="RFS_HL_CFNRC" linkType="Info" />
      <RFS name="RFS_HL_CFNRY" linkType="Info" />
    </CFS>
    <CFS name="FRMCA" description="Вам звонили / Я на связи">
      <RFS name="RFS_HL_CF_SERVICE" linkType="Add" />
      <RFS name="RFS_HL_CFD" linkType="Info" />
    </CFS>
    <CFS name="FRCRBT" description="GOODOK">
      <RFS name="RFS_HL_RBT" linkType="Add" />
    </CFS>
    <CFS name="CB406278" description="Мобильный интернет">
      <RFS name="RFS_HL_SERVICE_INTERNET" linkType="Add" />
    </CFS>
    <CFS name="FRUNLIMLTE" description="Доступ к сети 4G LTE">
      <RFS name="RFS_HL_SERVICE_LTE" linkType="Add" />
    </CFS>
  </CFSList>
  <CFSGroupList>
  </CFSGroupList>
  <RFSGroupList />
  <RFSDependencyList />
  <HandlerList >
  </HandlerList>
  <ScenarioList>
    <Scenario name="HL.CallForwarding" description="Добавление-удаление, активация-деактивация переадресаций">
      <RFS name="RFS_HL_CF" scenarioType="Add, Remove" />
      <RFS name="RFS_HL_CF_SERVICE" scenarioType="Add, Remove" />
      <Append name="RFS_HL_CFB" listType="All" />
      <Append name="RFS_HL_CFU" listType="All" />
      <Append name="RFS_HL_CFNRC" listType="All" />
      <Append name="RFS_HL_CFNRY" listType="All" />
      <Append name="RFS_HL_CFD" listType="All" />
    </Scenario>
    <Scenario name="HL.AssignServices" description="Добавление услуг">
      <RFS name="RFS_HL_RBT" scenarioType="Add" />
      <RFS name="RFS_HL_SERVICE_INTERNET" scenarioType="Add" />
    </Scenario>
    <Scenario name="HL.RemoveServices" description="Удаление услуг">
      <RFS name="RFS_HL_RBT" scenarioType="Remove" />
      <RFS name="RFS_HL_SERVICE_INTERNET" scenarioType="Remove" />
    </Scenario>
  </ScenarioList>
  <WfmScenarioList />
</Configuration>