<?xml version="1.0" encoding="utf-8"?>
<Configuration markers="*" scenarioPrefix="SC." mainIdentities="MSISDN,PersonalAccountNumber,ContractNumber" SICreation="" formatVersion="1">
  <HostTypeList>
    <HostType name="HL" nriType="RI" allowTerminalDeviceType="mobile" description="Home Location Register" />
  </HostTypeList>
  <IdentityList>
  </IdentityList>
  <AttributeList>
  </AttributeList>
  <ResourceList>
    <Resource name="RES_HL_REDUCE_SPEED_APN" description="APN ограничения скорости GPRS до 128кбс для LTE" />
  </ResourceList>
  <RFSParameterList>
    <RFSParameter name="CallForwardType" description="Тип установки переадресаций" />
    <RFSParameter name="CallForwardMode" description="Режим переадресации" />
    <RFSParameter name="ForwardedToNumber" description="Номер переадресации" />
    <RFSParameter name="CallForwardingTimer" description="Таймер переадресации" />
  </RFSParameterList>
  <FlagList />
  <RFSList>
    <RFS name="RFS_HL_CFB" hostType="HL" description="Call forwarding on mobile subscriber busy - TS10"/>
    <RFS name="RFS_HL_CFB_ACT" base="RFS_HL_CFB" hostType="HL" description="Activate Call forwarding on mobile subscriber busy - TS10">
      <RFSParameter name="CallForwardType">
        <Value condition="(//ActionNameOriginal[text()='Update Call Forwarding'])" value="Update" />
      </RFSParameter>
      <RFSParameter name="CallForwardMode">
        <Value condition="(//ProvisionList/CallForwarding/CFB)" value="Activate" />
        <Value condition="(//WithdrawalList/CallForwarding/CFB)" value="Deactivate" />
      </RFSParameter>
      <RFSParameter name="ForwardedToNumber" path="//CFB/ForwardingToAddress" />
    </RFS>
    <RFS name="RFS_HL_CFB_PRV" base="RFS_HL_CFB" hostType="HL" description="Provision Call forwarding on mobile subscriber busy - TS10">
      <RFSParameter name="CallForwardType" value="Provision"/>
    </RFS>
    <RFS name="RFS_HL_CFU" hostType="HL" description="Call forwarding unconditional - TS10"/>
    <RFS name="RFS_HL_CFU_ACT" base="RFS_HL_CFU" hostType="HL" description="Activate Call forwarding unconditional - TS10">
      <RFSParameter name="CallForwardType">
        <Value condition="(//ActionNameOriginal[text()='Update Call Forwarding'])" value="Update" />
      </RFSParameter>
      <RFSParameter name="CallForwardMode">
        <Value condition="(//ProvisionList/CallForwarding/CFU)" value="Activate" />
        <Value condition="(//WithdrawalList/CallForwarding/CFU)" value="Deactivate" />
      </RFSParameter>
      <RFSParameter name="ForwardedToNumber" path="//CFU/ForwardingToAddress" />
    </RFS>
    <RFS name="RFS_HL_CFU_PRV" base="RFS_HL_CFU" hostType="HL" description="Provision Call forwarding unconditional - TS10">
      <RFSParameter name="CallForwardType" value="Provision"/>
    </RFS>
    <RFS name="RFS_HL_CFNRC" hostType="HL" description="Call forwarding on mobile subscriber not reachable - TS10"/>
    <RFS name="RFS_HL_CFNRC_ACT" base="RFS_HL_CFNRC" hostType="HL" description="Activate Call forwarding on mobile subscriber not reachable - TS10">
      <RFSParameter name="CallForwardType">
        <Value condition="(//ActionNameOriginal[text()='Update Call Forwarding'])" value="Update" />
      </RFSParameter>
      <RFSParameter name="CallForwardMode">
        <Value condition="(//ProvisionList/CallForwarding/CFNRC)" value="Activate" />
        <Value condition="(//WithdrawalList/CallForwarding/CFNRC)" value="Deactivate" />
      </RFSParameter>
      <RFSParameter name="ForwardedToNumber" path="//CFNRC/ForwardingToAddress" />
    </RFS>
    <RFS name="RFS_HL_CFNRC_PRV" base="RFS_HL_CFNRC" hostType="HL" description="Provision Call forwarding on mobile subscriber not reachable - TS10">
      <RFSParameter name="CallForwardType" value="Provision"/>
    </RFS>
    <RFS name="RFS_HL_CFNRY" hostType="HL" description="Call forwarding on no reply - TS10"/>
    <RFS name="RFS_HL_CFNRY_ACT" base="RFS_HL_CFNRY" hostType="HL" description="Activate Call forwarding on no reply - TS10">
      <RFSParameter name="CallForwardType">
        <Value condition="(//ActionNameOriginal[text()='Update Call Forwarding'])" value="Update" />
      </RFSParameter>
      <RFSParameter name="CallForwardMode">
        <Value condition="(//ProvisionList/CallForwarding/CFNRY)" value="Activate" />
        <Value condition="(//WithdrawalList/CallForwarding/CFNRY)" value="Deactivate" />
      </RFSParameter>
      <RFSParameter name="ForwardedToNumber" path="//CFNRY/ForwardingToAddress" />
      <RFSParameter name="CallForwardingTimer" path="//CFNRY/NoReplyTimer" />
    </RFS>
    <RFS name="RFS_HL_CFNRY_PRV" base="RFS_HL_CFNRY" hostType="HL" description="Provision Call forwarding on no reply - TS10">
      <RFSParameter name="CallForwardType" value="Provision"/>
    </RFS>
    <RFS name="RFS_HL_CFD" hostType="HL" description="Call forwarding (to MCA) on mobile subscriber busy">
      <RFSParameter name="CallForwardType" value="Provision"/>
      <RFSParameter name="CallForwardMode" value="Activate" />
      <RFSParameter name="ForwardedToNumber" value="998970000007" />
    </RFS>
    
    
    <RFS name="RFS_HL_RBT" hostType="HL" description="Caller Ring Back Tone. GOODOK"/>
    <RFS name="RFS_HL_INET_GPRS" hostType="HL" description="Мобильный интернет GPRS"/>
    <RFS name="RFS_HL_INET_LTE" hostType="HL" description="Мобильный интернет LTE">
      <Resource name="RES_HL_REDUCE_SPEED_APN" value="NET.UMS.UZ" />
    </RFS>
    <RFS name="RFS_HL_ROAMING" hostType="HL" description="Роуминг"/>
  </RFSList>
  <CFSList>
    <CFS name="Subscriber" type="ManagedObject" description="Абонент" />
    <CFS name="CallForwarding" description="Активация пользовательской переадресации вызова">
      <RFS name="RFS_HL_CFB_ACT" linkType="Add" />
      <RFS name="RFS_HL_CFU_ACT" linkType="Add" />
      <RFS name="RFS_HL_CFNRC_ACT" linkType="Add" />
      <RFS name="RFS_HL_CFNRY_ACT" linkType="Add" />
    </CFS>
    <CFS name="CB532" description="Переадресация вызова (периодическая)">
      <RFS name="RFS_HL_CFB_PRV" linkType="Add" />
      <RFS name="RFS_HL_CFU_PRV" linkType="Add" />
      <RFS name="RFS_HL_CFNRC_PRV" linkType="Add" />
      <RFS name="RFS_HL_CFNRY_PRV" linkType="Add" />
    </CFS>
    <CFS name="FRMCA" description="Вам звонили / Я на связи">
      <RFS name="RFS_HL_CFD" linkType="Add" />
    </CFS>
    
    
    <CFS name="FRCRBT" description="Caller Ring Back Tone. GOODOK">
      <RFS name="RFS_HL_RBT" linkType="Add" />
    </CFS>
    <CFS name="CB406278" description="Мобильный интернет">
      <RFS name="RFS_HL_INET_GPRS" linkType="Add" />
    </CFS>
    <CFS name="FRUNLIMLTE" description="Доступ к сети 4G LTE">
      <RFS name="RFS_HL_INET_LTE" linkType="Add" />
    </CFS>
    <CFS name="CB123" description="Международный роуминг">
      <RFS name="RFS_HL_ROAMING" linkType="Add" />
    </CFS>
    <CFS name="BSNSSRMNG" description="Business Роуминг">
      <RFS name="RFS_HL_ROAMING" linkType="Add" />
    </CFS>
  </CFSList>
  <CFSGroupList>
  </CFSGroupList>
  <RFSGroupList />
  <RFSDependencyList />
  <HandlerList >
  </HandlerList>
  <ScenarioList>
    <Scenario name="HL.CallForwardingAssign" description="Добавление, активация-деактивация переадресаций">
      <RFS name="RFS_HL_CFB" scenarioType="Add" />
      <RFS name="RFS_HL_CFU" scenarioType="Add" />
      <RFS name="RFS_HL_CFNRC" scenarioType="Add" />
      <RFS name="RFS_HL_CFNRY" scenarioType="Add" />
      <RFS name="RFS_HL_CFD" scenarioType="Add" />
    </Scenario>
    <Scenario name="HL.CallForwardingRemove" description="Удаление переадресаций">
      <RFS name="RFS_HL_CFB" scenarioType="Remove" />
      <RFS name="RFS_HL_CFU" scenarioType="Remove" />
      <RFS name="RFS_HL_CFNRC" scenarioType="Remove" />
      <RFS name="RFS_HL_CFNRY" scenarioType="Remove" />
      <RFS name="RFS_HL_CFD" scenarioType="Remove" />
    </Scenario>
    <Scenario name="HL.BasicServicesAssign" description="Добавление услуг">
      <RFS name="RFS_HL_RBT" scenarioType="Add" />
      <RFS name="RFS_HL_ROAMING" scenarioType="Add" />
    </Scenario>
    <Scenario name="HL.BasicServicesRemove" description="Удаление услуг">
      <RFS name="RFS_HL_RBT" scenarioType="Remove" />
      <RFS name="RFS_HL_ROAMING" scenarioType="Remove" />
    </Scenario>
    <Scenario name="HL.InternetServiceAssign" description="Добавление интернет услуг">
      <RFS name="RFS_HL_INET_GPRS" scenarioType="Add" />
      <RFS name="RFS_HL_INET_LTE" scenarioType="Add" />
      <Append name="RFS_HL_INET_LTE" listType="EndList" />
      <Append name="RFS_HL_ROAMING" listType="EndList" />
    </Scenario>
    <Scenario name="HL.InternetServiceRemove" description="Удаление интернет услуг">
      <RFS name="RFS_HL_INET_GPRS" scenarioType="Remove" />
      <RFS name="RFS_HL_INET_LTE" scenarioType="Remove" />
    </Scenario>
  </ScenarioList>
  <WfmScenarioList />
</Configuration>