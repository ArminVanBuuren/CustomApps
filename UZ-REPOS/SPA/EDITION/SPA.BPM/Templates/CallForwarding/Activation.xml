<ExtendedRequestMessage>
  <Source>crm</Source>
  <SourceAddress>CRM</SourceAddress>
  <Destination>bpm</Destination>
  <DestinationAddress>BPM</DestinationAddress>
  <ID>#MessageId#</ID>
  <OriginalID>#MessageId#</OriginalID>
  <OriginalSource>CRM</OriginalSource>
  <Priority>Idle</Priority>
  <AdditionalData Executable="True">
    <ProvisionList>
      <CallForwarding>
        #CallForwardingParams#
      </CallForwarding>
    </ProvisionList>
    <ProcessingOptions>
      <SynchronizationRequest />
    </ProcessingOptions>
  </AdditionalData>
  <MustBeConfirmed>false</MustBeConfirmed>
  <ActionNameAlias>Активирование услуги Переадресация вызова</ActionNameAlias>
  <ActionName>Activate Call Forwarding Sync</ActionName>
  <IdentifiersList>
    <Identifier>
      <MSISDN>#MSISDN#</MSISDN>
      <IMSI>#IMSI#</IMSI>
    </Identifier>
  </IdentifiersList>
  <ContinueByDefault>true</ContinueByDefault>
</ExtendedRequestMessage>