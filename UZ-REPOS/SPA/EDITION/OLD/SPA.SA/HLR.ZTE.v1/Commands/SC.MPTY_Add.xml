<Command Name="SC.MPTY_Add" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>Добавление услуги 'Конференц-связь'</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[MOD SS: MSISDN=##msisdn##,MPTY=1;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate>
      <![CDATA[]]>
    </EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown" />
  </Request>
  <ErrorsList CodePage="windows-1251" />
</Command>