<Command Name="SC.NAM_Add" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>Добавление сервиса интернет NAM</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[MOD NAM:MSISDN=##msisdn##,NAM=0,Keep=1,KEEPEPS=1;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate>
      <![CDATA[]]>
    </EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown" />
  </Request>
  <ErrorsList CodePage="windows-1251">
    <Error Name="110109" Value="SUCCESS" />
    <!--Subscriber does not exist-->
  </ErrorsList>
</Command>