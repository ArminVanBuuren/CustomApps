<Command Name="CounterReinitialization" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
   <Description>CounterReinitialization</Description>
   <Author Name="Aleksandr Zaytsev" Date="2016-10-20" />
   <Request>
      <RequestTemplate>
         <![CDATA[ 
				<ChangeCounterProfiles>
					<changes>##changes##</changes>
					<requestId>##org.id##</requestId>
					<clientId>SPA</clientId>
				</ChangeCounterProfiles>
			]]>
      </RequestTemplate>
      <Item Name="changes" Type="LocalDynamicParameter" />
      <Item Name="org.id" Type="Wellknown" />
   </Request>
   <DynamicParametersList>
      <DynamicParameter Name="changes">
         <Source Path="//string" />
         <DynamicObject Name="Services" />
      </DynamicParameter>
   </DynamicParametersList>
</Command>