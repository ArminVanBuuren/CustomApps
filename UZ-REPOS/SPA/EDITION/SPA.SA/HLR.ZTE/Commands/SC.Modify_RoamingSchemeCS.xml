<Command Name="SC.Modify_RoamingSchemeCS">
  <Description>Изменение управляющей схемы для роуминга по CS части</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[Mod BscEx:MSISDN=##msisdn##,ROAMSCH=##param##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="param" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="param">
      <Source Path="//AddList/RFS_HLR_ROAMING" PresenceValue="STEERING" AbsenceValue="*" />
    </Parameter>
  </ParametersList>
</Command>