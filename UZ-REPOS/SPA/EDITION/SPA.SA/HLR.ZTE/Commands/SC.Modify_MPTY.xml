<Command Name="SC.Modify_MPTY">
  <Description>Изменение услуги 'Конференц-связь'</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[MOD SS: MSISDN=##msisdn##,MPTY=##ACTION##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="ACTION" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="ACTION" IsChoice="true" AbsenceValue="0">
      <Source Path="//AddList/RFS_HLR_MPTY" PresenceValue="1" />
      <Source Path="//EndList/RFS_HLR_MPTY" PresenceValue="1" />
    </Parameter>
  </ParametersList>
</Command>