<Command Name="SC.Modify_CLIP">
  <Description>Изменение сервиса 'Определитель номера'</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[MOD SS: MSISDN=##msisdn##,##SERVICE##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="SERVICE" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="SERVICE" IsChoice="true" AbsenceValue="CLIP=0">
      <Source Path="//AddList/RFS_HLR_CLIP" PostFormat="CLIP=1,CLIPOpt=1" />
      <Source Path="//EndList/RFS_HLR_CLIP" PostFormat="CLIP=1,CLIPOpt=1" />
    </Parameter>
  </ParametersList>
</Command>