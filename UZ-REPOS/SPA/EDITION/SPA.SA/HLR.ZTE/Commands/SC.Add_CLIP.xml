   <Author Name="system" Date="2018-09-20+09:00" />
   <Request>
      <RequestTemplate>
         <![CDATA[MOD SS: MSISDN=##msisdn##,CLIP=1,CLIPOpt=1;^M]]>
      </RequestTemplate>
      <Item Name="msisdn" Type="Wellknown" />
   </Request>
</Command>