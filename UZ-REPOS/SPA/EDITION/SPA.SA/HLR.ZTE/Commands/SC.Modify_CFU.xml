<Command Name="SC.Modify_CFU">
  <Description>Modify call forwarding unconditional</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[MOD SS:MSISDN=##msisdn##,##CFU_SERVICE##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="CFU_SERVICE" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="CFU_SERVICE" IsChoice="true" AbsenceValue="CFU=0">
      <Source Path="//AddList/RFS_HLR_CFU" PostFormat="CFU=1,CFUNTC=1" />
      <Source Path="//EndList/RFS_HLR_CFU" PostFormat="CFU=1,CFUNTC=1" />
    </Parameter>
  </ParametersList>
</Command>