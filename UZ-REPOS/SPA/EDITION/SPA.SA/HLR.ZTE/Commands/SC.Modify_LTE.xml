<Command Name="SC.Modify_LTE">
  <Description>Изменение LTE</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[Mod BscEx:MSISDN=##msisdn##,SubType=##LTE_TYPE##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="LTE_TYPE" Type="LogicalGroupParameter" />
    <!--Item Name="ACTION" Type="LocalParameter" /-->
  </Request>
  <ParametersList>
    <Parameter Name="ACTION">
      <Source Path="//AddList/RFS_HLR_LTE" PresenceValue="2" AbsenceValue="1" />
    </Parameter>
  </ParametersList>
</Command>