<Command Name="SC.Modify_LTE">
  <Description>Изменение LTE</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[Mod BscEx:MSISDN=##msisdn##,SubType=##LTE_TYPE##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="LTE_TYPE" Type="LocalLogicalGroupParameter" />
  </Request>
  <LogicalGroupParametersList>
    <LogicalGroupParameter Name="LTE_TYPE" AbsenceValue="2">
      <SourcesList PresenceValue="1">
        <Source Type="OrXmlPath:0" Path="//RemoveList/RFS_HLR_LTE"/>
        <Source Type="OrXmlPath:1" Path="//RES_HLR_PROFILE/Value[text()='DEALERS']"/>
        <Source Type="OrXmlPath:2" Path="//RES_HLR_PROFILE/Value[text()='DGU']"/>
        <Source Type="OrXmlPath:3" Path="//RES_HLR_PROFILE/Value[text()='AIISKUE']"/>
      </SourcesList>
    </LogicalGroupParameter>
  </LogicalGroupParametersList>
</Command>