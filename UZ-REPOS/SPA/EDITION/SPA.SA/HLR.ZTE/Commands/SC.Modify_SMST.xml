<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.Modify_SMST">
	<Description>Изменение услуги 'Служба коротких сообщений'Входящие</Description>
	<Author Name="system" Date="2018-09-20+09:00"/>
	<Request>
		<RequestTemplate>
			<![CDATA[MOD TELE: MSISDN=##msisdn##,SMMT=##SMSBarring##;^M]]>
		</RequestTemplate>
		<Item Name="msisdn" Type="Wellknown"/>
		<Item Name="SMSBarring" Type="LocalParameter"/>
	</Request>
	<ParametersList>
		<Parameter Name="SMSBarring">
			<Source Path="//AddList/RFS_HLR_SMS_MT/RES_HLR_SMS_MT/Value | //ModifyList/RFS_HLR_SMS_MT/RES_HLR_SMS_MT/Value" AbsenceValue="0"/>
			<PossibleValues StringToKeyTable="true">
				<Item Key="0" StringValue="Deny"/>
				<Item Key="1" StringValue="Allow"/>
			</PossibleValues>
		</Parameter>
	</ParametersList>
</Command>