<Command Name="SC.Modify_DataTransfer">
  <Description>Изменение мобильной передачи данных</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[Mod BscEx:MSISDN=##msisdn##,CDA9600=##ACTION##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="ACTION" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="ACTION" IsChoice="true" AbsenceValue="0">
      <Source Path="//AddList/RFS_HLR_DATATRANSFER" PresenceValue="1" />
      <Source Path="//EndList/RFS_HLR_DATATRANSFER" PresenceValue="1" />
    </Parameter>
  </ParametersList>
</Command>