<Command Name="SC.Add_CF">
   <Description>Добавление услуги переадресация</Description>
   <Author Name="system" Date="2018-09-20+09:00" />
   <Request>
      <RequestTemplate>
         <![CDATA[MOD SS:MSISDN=##msisdn##,##CallForwardName##;^M]]>
      </RequestTemplate>
      <Item Name="CallForwardName" Type="LocalParameter" />
      <Item Name="msisdn" Type="Wellknown" />
   </Request>
   <ParametersList>
      <Parameter Name="CallForwardName" IsChoice="true" AbsenceValue="">
         <Source Path="//RFS_HLR_CFB" PostFormat="CFB=1,CFBNTC=1,CFBNTF=1" />
         <Source Path="//RFS_HLR_CFU" PostFormat="CFU=1,CFUNTC=1" />
         <Source Path="//RFS_HLR_CFNRC" PostFormat="CFNRC=1,CFNRCNTC=1" />
         <Source Path="//RFS_HLR_CFNRY" PostFormat="CFNRY=1,CFNRYNTC=1,CFNRYNTF=1" />
         <Source Path="//RFS_HLR_CFD" PostFormat="CFD=1,CFDNTC=1,CFDNTF=1" />
      </Parameter>
   </ParametersList>
</Command>