<Command Name="SC.Modify_NAM">
  <Description>Изменение сервиса интернет NAM</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[MOD NAM:MSISDN=##msisdn##,##NAM_STATE##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="NAM_STATE" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="NAM_STATE" IsChoice="true" AbsenceValue="NAM=1">
      <Source Path="//AddList/RFS_HLR_NET/RES_HLR_NET/Value[text()='Allow']" PostFormat="NAM=0,Keep=1,KEEPEPS=1" />
      <Source Path="//AddList/RFS_HLR_NET/RES_HLR_NET/Value[text()='Deny']" PostFormat="NAM=1,Keep=1,KEEPEPS=1" />
      <Source Path="//EndList/RFS_HLR_NET/RES_HLR_NET/Value[text()='Allow']" PostFormat="NAM=0,Keep=1,KEEPEPS=1" />
      <Source Path="//EndList/RFS_HLR_NET/RES_HLR_NET/Value[text()='Deny']" PostFormat="NAM=1,Keep=1,KEEPEPS=1" />
      <Source Path="//ModifyList/RFS_HLR_NET/RES_HLR_NET/Value[text()='Allow']" PostFormat="NAM=0,Keep=1,KEEPEPS=1" />
      <Source Path="//ModifyList/RFS_HLR_NET/RES_HLR_NET/Value[text()='Deny']" PostFormat="NAM=1,Keep=1,KEEPEPS=1" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="windows-1251">
    <Error Name="110109" Value="SUCCESS" />
    <!--Subscriber does not exist-->
  </ErrorsList>
</Command>