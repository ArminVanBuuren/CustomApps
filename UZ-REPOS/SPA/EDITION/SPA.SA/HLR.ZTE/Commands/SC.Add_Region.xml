<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.Add_Region">
	<Description>Определение региона для Переадресации</Description>
	<Author Name="system" Date="2018-09-20+09:00"/>
	<Request>
		<RequestTemplate>
			<![CDATA[Mod BscEx:MSISDN=##msisdn##,ORIGINFAID=##REGION##;^M]]>
		</RequestTemplate>
		<Item Name="msisdn" Type="Wellknown"/>
		<Item Name="REGION" Type="LocalParameter"/>
	</Request>
	<ParametersList>
		<Parameter Name="REGION" AbsenceValue="">
			<Source Path="//RES_HLR_REGION/Value"/>
		</Parameter>
	</ParametersList>
</Command>