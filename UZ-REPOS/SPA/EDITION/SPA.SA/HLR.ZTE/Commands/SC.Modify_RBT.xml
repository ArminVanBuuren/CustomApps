<Command Name="SC.Modify_RBT">
  <Description>Изменение сервиса RBT</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[MOD SS: MSISDN=##msisdn##,PLSS_5=##action##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="action" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="action">
      <Source Path="//AddList/RFS_HLR_RBT" PresenceValue="1" AbsenceValue="0" />
    </Parameter>
  </ParametersList>
</Command>