<Command Name="SC.Modify_CFB">
  <Description>Modify call forwarding on mobile subscriber busy</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[MOD SS:MSISDN=##msisdn##,##CFB_SERVICE##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="CFB_SERVICE" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="CFB_SERVICE" IsChoice="true" AbsenceValue="CFB=0">
      <Source Path="//AddList/RFS_HLR_CFB" PostFormat="CFB=1,CFBNTC=1,CFBNTF=1" />
      <Source Path="//EndList/RFS_HLR_CFB" PostFormat="CFB=1,CFBNTC=1,CFBNTF=1" />
    </Parameter>
  </ParametersList>
</Command>