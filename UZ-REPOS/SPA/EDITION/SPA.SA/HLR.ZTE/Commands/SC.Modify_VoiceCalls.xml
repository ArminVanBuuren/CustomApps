<Command Name="SC.Modify_VoiceCalls">
  <Description>Изменение голосовой связи</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[Mod Tele:MSISDN=##msisdn##,Tele=##ACTION##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="ACTION" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="ACTION">
      <Source Path="//AddList/RFS_HLR_CALLS_BAN" PresenceValue="0" AbsenceValue="1" />
    </Parameter>
  </ParametersList>
</Command>