<Command Name="SC.Add_LTE_TPLAPNCP">
   <Description>Добавление услуги 'LTE 4G'</Description>
   <Author Name="system" Date="2018-09-20+09:00" />
   <Request>
      <RequestTemplate>
         <![CDATA[Set TPLAPNCP:MSISDN=##msisdn##,EPCAPNCPTPL=51;^M]]>
      </RequestTemplate>
      <Item Name="msisdn" Type="Wellknown" />
   </Request>
   <ErrorsList CodePage="windows-1251">
      <Error Name="222007" Value="SUCCESS" />
      <!--Non-EPC users can't include the parameters only for EPC users-->
   </ErrorsList>
</Command>