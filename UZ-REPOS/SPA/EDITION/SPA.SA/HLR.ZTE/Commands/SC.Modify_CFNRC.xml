<Command Name="SC.Modify_CFNRC">
  <Description>Modify call forwarding on mobile subscriber not reachable</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[MOD SS:MSISDN=##msisdn##,##CFNRC_SERVICE##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="CFNRC_SERVICE" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="CFNRC_SERVICE" IsChoice="true" AbsenceValue="CFNRC=0">
      <Source Path="//AddList/RFS_HLR_CFNRC" PostFormat="CFNRC=1,CFNRCNTC=1" />
      <Source Path="//EndList/RFS_HLR_CFNRC" PostFormat="CFNRC=1,CFNRCNTC=1" />
    </Parameter>
  </ParametersList>
</Command>