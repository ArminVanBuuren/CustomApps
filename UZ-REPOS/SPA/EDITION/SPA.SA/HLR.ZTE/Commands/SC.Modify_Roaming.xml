<Command Name="SC.Modify_Roaming">
  <Description>Изменение международного роуминга</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[MOD ODB:MSISDN=##msisdn##,BR=##ACTION##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="ACTION" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="ACTION">
      <Source Path="//AddList/RFS_HLR_ROAMING" PresenceValue="0" AbsenceValue="2" />
    </Parameter>
  </ParametersList>
</Command>