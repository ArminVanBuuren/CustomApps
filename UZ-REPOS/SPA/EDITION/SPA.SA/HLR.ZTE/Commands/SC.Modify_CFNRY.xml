<Command Name="SC.Modify_CFNRY">
  <Description>Modify call forwarding on no reply</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[MOD SS:MSISDN=##msisdn##,##CFNRY_SERVICE##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="CFNRY_SERVICE" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="CFNRY_SERVICE" IsChoice="true" AbsenceValue="CFNRY=0">
      <Source Path="//AddList/RFS_HLR_CFNRY" PostFormat="CFNRY=1,CFNRYNTC=1,CFNRYNTF=1" />
      <Source Path="//EndList/RFS_HLR_CFNRY" PostFormat="CFNRY=1,CFNRYNTC=1,CFNRYNTF=1" />
    </Parameter>
  </ParametersList>
</Command>