<Command Name="SC.Modify_VideoCalls">
  <Description>Изменение асинхронной и синхронной видео связи</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[Mod BscEx:MSISDN=##msisdn##,GENCDA=##ACTION##,GENCDS=##ACTION##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="ACTION" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="ACTION">
      <Source Path="//AddList/RFS_HLR_CALLS_BAN" PresenceValue="0" AbsenceValue="1" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="windows-1251">
    <Error Name="ERR3001" Value="NoSuchObjectInstance" />
  </ErrorsList>
</Command>