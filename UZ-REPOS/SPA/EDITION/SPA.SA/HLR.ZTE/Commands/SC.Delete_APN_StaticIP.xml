<Command Name="SC.Delete_APN_StaticIP">
  <Description>Удалить статический IP</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[MOD PDP:MSISDN=##msisdn##,OAPN=##NET_APN##,OPDPTYPE=0,OaddrOption=1,PDPAddr=0.0.0.0;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="NET_APN" Type="Parameter" />
  </Request>
  <ErrorsList CodePage="windows-1251">
    <Error Name="104080" Value="SUCCESS" />
    <!--Can not find the corresponding PDP information-->
  </ErrorsList>
</Command>