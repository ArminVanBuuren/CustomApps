<Command Name="SC.Delete_APN_StaticIP_Dealer">
   <Description>Удалить дилерский статический IP</Description>
   <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
   <Request>
      <RequestTemplate>
         <![CDATA[MOD PDP:MSISDN=##msisdn##,OAPN=##DEALER_APN##,OPDPTYPE=0,OaddrOption=1,PDPAddr=0.0.0.0;^M]]>
      </RequestTemplate>
      <Item Name="DEALER_APN" Type="Parameter" />
      <Item Name="msisdn" Type="Wellknown" />
   </Request>
   <ErrorsList CodePage="windows-1251">
      <Error Name="104080" Value="SUCCESS" />
      <!--Can not find the corresponding PDP information-->
   </ErrorsList>
</Command>