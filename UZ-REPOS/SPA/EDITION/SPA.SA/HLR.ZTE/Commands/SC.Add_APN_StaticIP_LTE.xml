<Command Name="SC.Add_APN_StaticIP_LTE">
  <Description>Добавить статический IP LTE</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[Mod APNPROF:MSISDN=##msisdn##,OAPNCFGID=1,EPCPDNTYPE1=0,EPCPDNAddr1=##IP##;^M]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="IP" Type="Parameter" />
  </Request>
  <ErrorsList CodePage="windows-1251">
    <Error Name="ERR3045" Value="FAILURE" />
    <Error Name="104080" Value="SUCCESS" />
  </ErrorsList>
</Command>