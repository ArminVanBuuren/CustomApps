<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.Add_APN_StaticIP">
	<Description>Добавить статический IP</Description>
	<Author Name="system" Date="2018-09-20+09:00"/>
	<Request>
		<RequestTemplate>
			<![CDATA[MOD PDP:MSISDN=##msisdn##,OAPN=##NET_APN##,OPDPTYPE=0,PDPAddr=##IP##;^M]]>
		</RequestTemplate>
		<Item Name="IP" Type="Parameter"/>
		<Item Name="msisdn" Type="Wellknown"/>
		<Item Name="NET_APN" Type="Parameter"/>
	</Request>
	<ErrorsList CodePage="windows-1251">
		<Error Name="ERR3045" Value="FAILURE"/>
		<Error Name="104080" Value="SUCCESS"/>
		<Error Name="110703" Value="SUCCESS"/>
		<!--The format of the entered IP address is illegal-->
	</ErrorsList>
</Command>