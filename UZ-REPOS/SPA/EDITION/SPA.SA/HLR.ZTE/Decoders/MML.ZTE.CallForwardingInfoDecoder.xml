<Decoder Name="CallForwardingInfo" Type="FORIS.ServiceProvisioning.MmlProcessing.Dictionary.NestedIdentifiersDecoder, FORIS.SPA.MmlProcessing">
   <AttributesList>
      <Attribute Key="BasicService" RegexpPattern="((?&lt;parameter&gt;(CFB|CFU|CFNRY|CFNRC))_R=\d)" RegexpOptions="Singleline, ExplicitCapture">
         <PossibleValues StringToKeyTable="false">
            <Item Key="CFB" StringValue="TeleService" />
            <Item Key="CFU" StringValue="TeleService" />
            <Item Key="CFNRC" StringValue="TeleService" />
            <Item Key="CFNRY" StringValue="TeleService" />
         </PossibleValues>
      </Attribute>
      <Attribute Key="AdditionalInfo/Number" RegexpPattern="(CFB_R=1.*CFBNum|CFU_R=1.*CFUNum|CFNRY_R=1.*CFNRYNum|CFNRC_R=1.*CFNRCNum)=(?&lt;parameter&gt;\d+)" RegexpOptions="Singleline, ExplicitCapture" />
      <Attribute Key="GeneralInfo/NoReplyTimer" RegexpPattern="(CFBTime|CFUTime|CFNRYTime|CFNRCTime)=(?&lt;parameter&gt;\d+)" RegexpOptions="Singleline, ExplicitCapture" />
      <Attribute Key="ActivationStatus" RegexpPattern="(?&lt;parameter&gt;(CFB_R=0|CFU_R=0|CFNRY_R=0|CFNRC_R=0|CFB_R=1|CFU_R=1|CFNRY_R=1|CFNRC_R=1))" RegexpOptions="Singleline, ExplicitCapture">
         <PossibleValues StringToKeyTable="false">
            <Item Key="CFB_R=0" StringValue="Provisioned" />
            <Item Key="CFU_R=0" StringValue="Provisioned" />
            <Item Key="CFNRY_R=0" StringValue="Provisioned" />
            <Item Key="CFNRC_R=0" StringValue="Provisioned" />
            <Item Key="CFB_R=1" StringValue="Activated" />
            <Item Key="CFU_R=1" StringValue="Activated" />
            <Item Key="CFNRY_R=1" StringValue="Activated" />
            <Item Key="CFNRC_R=1" StringValue="Activated" />
         </PossibleValues>
      </Attribute>
   </AttributesList>
</Decoder>