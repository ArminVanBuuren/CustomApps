<Command Name="AddMST" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
   <Description>Создание абонента на MST</Description>
   <Author Name="Timur Khamzin" Date="2015-11-27+15:00" />
   <Request>
      <RequestTemplate>
         <![CDATA[msisdn=##msisdn##&command=add&service=##ServiceId##]]>
      </RequestTemplate>
      <Item Name="msisdn" Type="Wellknown" />
      <Item Name="ServiceId" Type="Parameter" />
   </Request>
   <ErrorsList CodePage="ASCII" />
</Command>