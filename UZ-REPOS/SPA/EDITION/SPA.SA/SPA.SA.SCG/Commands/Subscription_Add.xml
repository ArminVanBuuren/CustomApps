<?xml version="1.0" encoding="UTF-8"?>
<Command Name="Subscription_Add" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
	<Description>Добавление подписки услуги на платформу SCG</Description>
  <Author Name="Vladimir Khovanskiy" Date="2018-10-01+11:09"/>
	<Request>
		<RequestTemplate>
			<![CDATA[<soapenv:Envelope
xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/"
xmlns:bas="http://basicservices.ib.sdp.huawei.com"
xmlns:req="http://request.basicservices.ib.sdp.huawei.com"
xmlns:sch="http://schema.ib.sdp.huawei.com">
   <soapenv:Header/>
   <soapenv:Body>
      <bas:in0>
         <req:channelID>1</req:channelID>
         <req:subInfoList>
            <bas:item>               
               <sch:payType>0</sch:payType>
               <sch:productID>##productID##</sch:productID>
               <sch:subType>0</sch:subType>
            </bas:item>
         </req:subInfoList>
         <req:userID>
            <sch:ID>##msisdn##</sch:ID>
            <sch:type>0</sch:type>
         </req:userID>
      </bas:in0>
   </soapenv:Body>
</soapenv:Envelope>]]>
		</RequestTemplate>
		<EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
		<Item Name="msisdn" Type="Wellknown"/>	
		<Item Name="productID" Type="Parameter"/>	
	</Request>
	<ErrorsList CodePage="windows-1251" />
</Command>