<?xml version="1.0" encoding="UTF-8"?>
<scenario name="SC.SCG.ChangeMSISDN">
  <include>
    <scenario name="SubDeleteSubscriberOnSCG" />
  </include>
  <objectList>
    <object name="RequestCutterBySubscribers" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.RequestCutterBySubscribers" assembly="FORIS.SPA.MmlProcessing.dll"/>
    <object name="Commander" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.Processors.RequestProcessor" assembly="FORIS.SPA.MmlProcessing.dll"/>
    <object name="ResponseAssembler" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.ResponseAssembler" assembly="FORIS.SPA.MmlProcessing.dll"/>
    <object name="Switch" class="FORIS.ScenarioProcessing.CommonUnits.ParametersSwitch" assembly="FORIS.SPA.SPF.dll"/>
    <object name="ScenarioProcessor" class="FORIS.ScenarioProcessing.Base.SPF.SubscenarioProcessingUnit" assembly="FORIS.SPA.SPF.dll" />
    <object name="DummyResponder" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.DummyResponder" assembly="FORIS.SPA.MmlProcessing.dll"/>
    <object name="WellknownsReplacer" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.WellknownsReplacer" assembly="FORIS.SPA.MmlProcessing.dll"/>
    <object name="WellknownsRestorer" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.WellknownsRestorer" assembly="FORIS.SPA.MmlProcessing.dll"/>
  </objectList>
  <automat>
    <node name="RequestCutterBySubscribers" object="RequestCutterBySubscribers">
      <resultlist>
        <result value="0" node="SubDeleteSubscriberOnSCGOld"/>
        <result value="1" node=""/>
      </resultlist>
    </node>
    <node  name="SubDeleteSubscriberOnSCGOld" object="ScenarioProcessor">
      <resultlist>
        <result value="0" node="WellknownsReplacerNode" />
        <result value="" node="ResponseAssembler" />
      </resultlist>
      <parameterslist>
        <param name="Scenario" value="SubDeleteSubscriberOnSCG" />
      </parameterslist>
    </node>
    <node name="WellknownsReplacerNode" object="WellknownsReplacer">
      <resultlist>
        <result value="" node="ReCreateSubscriberOnSCGNew"/>
      </resultlist>
      <parameterslist>
        <param name="msisdn" value="xmlsubstr(&quot;//AdditionalData//NewMSISDN&quot;)"/>
      </parameterslist>
    </node>
    <node  name="ReCreateSubscriberOnSCGNew" object="ScenarioProcessor">
      <resultlist>
        <result value="0" node="ADD_MAPSERVER_OSC" />
        <result value="" node="ResponseAssembler" />
      </resultlist>
      <parameterslist>
        <param name="Scenario" value="SubDeleteSubscriberOnSCG" />
      </parameterslist>
    </node>
    <node name="ADD_MAPSERVER_OSC" object="Commander">
      <resultlist>
        <result value="" node="ResponseAssembler"/>
      </resultlist>
      <parameterslist>
        <param name="Command" value="ADD_MAPSERVER_OSC"/>
        <param name="Script" value="Command"/>
        <param name="ErrorDecodingType" value="ByRegexp"/>
        <param name="CommandSource" value="Dictionary"/>
      </parameterslist>
    </node>
    <node name="ResponseAssembler" object="ResponseAssembler">
      <resultlist>
        <result value="" node="RequestCutterBySubscribers"/>
      </resultlist>
    </node>
  </automat>
</scenario>