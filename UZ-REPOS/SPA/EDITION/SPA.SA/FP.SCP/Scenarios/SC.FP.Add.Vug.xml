<scenario name="SC.FP.Add.Vug">
   <objectList>
      <object name="BySubscribersCutter" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.RequestCutterBySubscribers" assembly="FORIS.SPA.MmlProcessing.dll" />
      <object name="Commander" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.Processors.RequestProcessor" assembly="FORIS.SPA.MmlProcessing.dll" />
      <object name="ByServicesCutter" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.RequestCutterByXmlNodes" assembly="FORIS.SPA.MmlProcessing.dll" />
      <object name="IfCommander" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.Processors.RequestProcessorConditional" assembly="FORIS.SPA.MmlProcessing.dll" />
      <object name="RiParametersReader" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.RiLinkedDataInitializer" assembly="FORIS.SPA.MmlProcessing.dll" />
      <object name="ResponseAssembler" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.ResponseAssembler" assembly="FORIS.SPA.MmlProcessing.dll" />
      <object name="ConditionsResolver" class="FORIS.ScenarioProcessing.CommonUnits.IfMultipleXmlConditionsResolver" assembly="FORIS.SPA.SPF.dll" />
      <object name="DummyResponder" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.DummyResponder" assembly="FORIS.SPA.MmlProcessing.dll" />
   </objectList>
   <automat>
      <!-- первый цикл по группам -->
      <node name="ByServicesCutterCreateGroup-Node" object="ByServicesCutter">
         <resultlist>
            <result value="0" node="ChangeCallingCircle-Node" />
            <result value="" node="BySubscribersCutter" />
         </resultlist>
         <parameterslist>
            <param name="Queue" value="requestsSplittedByServicesQueue" />
            <param name="XmlPath:0" value="//AdditionalData/AddList/RFS_FP_SERVICE_VUG" />
         </parameterslist>
      </node>
      <node name="ChangeCallingCircle-Node" object="IfCommander">
         <parameterslist>
            <param name="IfXmlPath:0" value="//AdditionalData/AddList/RFS_FP_SERVICE_VUG/CugAvailableOnFP/Value[text()='1']" />
            <param name="OrXmlPath:0" value="//AdditionalData/AddList/RFS_FP_SERVICE_VUG/CreateGroup/Value[text()='1']" />
            <param name="Command" value="SC.ChangeCallingCircle" />
         </parameterslist>
         <resultlist>
            <result value="0" node="ByServicesCutterCreateGroup-Node" />
            <result value="1538" node="ByServicesCutterCreateGroup-Node" />
            <result value="" node="FinalResponseAssembler" />
         </resultlist>
      </node>
      <!-- второй цикл по абонам и услугам -->
      <node name="BySubscribersCutter" object="BySubscribersCutter">
         <parameterslist>
            <param name="ObjectInContext" value="request" />
         </parameterslist>
         <resultlist>
            <result value="0" node="ByServicesCutter-Node" />
            <result value="1" node="" />
         </resultlist>
      </node>
      <node name="ByServicesCutter-Node" object="ByServicesCutter">
         <resultlist>
            <result value="0" node="AddCallingCircleMembers-Node" />
            <result value="" node="CycledResponseAssembler" />
         </resultlist>
         <parameterslist>
            <param name="Queue" value="requestsSplittedByServicesQueue" />
            <param name="XmlPath:0" value="//AdditionalData/AddList/RFS_FP_SERVICE_VUG" />
         </parameterslist>
      </node>
      <node name="AddCallingCircleMembers-Node" object="IfCommander">
         <resultlist>
            <result value="0" node="CheckForeignSubscriber" />
            <result value="1876" node="CreateCallingCircle" />
            <result value="1538" node="ByServicesCutter-Node" />
            <result value="" node="FinalResponseAssembler" />
         </resultlist>
         <parameterslist>
            <param name="IfXmlPath:0" value="//AdditionalData/AddList/RFS_FP_SERVICE_VUG/CugAvailableOnFP/Value[text()='1']" />
            <param name="Command" value="SC.AddCallingCircleMembers" />
         </parameterslist>
      </node>
      <node name="CreateCallingCircle" object="Commander">
         <resultlist>
            <result value="0" node="AddCallingCircleMembersRepeat-Node" />
            <result value="1877" node="AddCallingCircleMembersRepeat-Node" />
            <result value="" node="ByServicesCutter-Node" />
         </resultlist>
         <parameterslist>
            <param name="Command" value="SC.ChangeCallingCircle" />
            <param name="AppendSubscribersData" value="true" />
         </parameterslist>
      </node>
      <node name="AddCallingCircleMembersRepeat-Node" object="Commander">
         <resultlist>
            <result value="0" node="CheckForeignSubscriber" />
            <result value="" node="FinalResponseAssembler" />
         </resultlist>
         <parameterslist>
            <param name="Command" value="SC.AddCallingCircleMembers" />
         </parameterslist>
      </node>
      <node name="CheckForeignSubscriber" object="ConditionsResolver">
         <resultlist>
            <result value="0" node="RiParametersReader" />
            <result value="" node="ByServicesCutter-Node" />
         </resultlist>
         <parameterslist>
            <param name="MultiLevelCondition" value="(//IdentifiersList/Identifier//IMSI[text()!='0']) AND NOT (//AddList/RFS_FP_SERVICE_VUG/SkipFP/Value[text()='1'])" />
         </parameterslist>
      </node>
      <node name="RiParametersReader" object="RiParametersReader">
         <resultlist>
            <result value="0" node="CheckHostTypeCode" />
            <result value="" node="ByServicesCutter-Node" />
         </resultlist>
         <parameterslist>
            <param name="IdentityName" value="IMSI" />
            <param name="ResultContextKey" value="RiParametersList" />
            <param name="DataType" value="GetBalanceStorageByImsiList" />
            <param name="IdentityXmlPath" value="//IdentifiersList/Identifier/IMSI" />
         </parameterslist>
      </node>
      <node name="CheckHostTypeCode" object="ConditionsResolver">
         <resultlist>
            <result value="0" node="AssignService" />
            <result value="1" node="ByServicesCutter-Node" />
         </resultlist>
         <parameterslist>
            <param name="ObjectNameInContext" value="RiDataXml" />
            <param name="IfXmlPath:0" value="//Host_x0020_code[contains('contextXpathParam(xmlrequest,//Destination)',.)]" />
         </parameterslist>
      </node>
      <node name="AssignService" object="Commander">
         <parameterslist>
            <param name="Command" value="SC.AssignCugService" />
            <param name="Asynchronous" value="True" />
         </parameterslist>
         <resultlist>
            <result value="0" node="ByServicesCutter-Node" />
            <result value="" node="FinalResponseAssembler" />
         </resultlist>
      </node>
      <node name="CycledResponseAssembler" object="ResponseAssembler">
         <parameterslist />
         <resultlist>
            <result value="" node="BySubscribersCutter" />
         </resultlist>
      </node>
      <node name="FinalResponseAssembler" object="ResponseAssembler">
         <resultlist>
            <result value="" node="" />
         </resultlist>
         <parameterslist>
            <param name="FinalStepCheck" value="true" />
         </parameterslist>
      </node>
   </automat>
</scenario>