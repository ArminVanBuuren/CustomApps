<scenario  name="SC.FP.Change.Msisdn.Vug">
  <objectList>
    <object name="BySubscribersSplitter" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.RequestSplitterBySubscribers" assembly="FORIS.SPA.MmlProcessing.dll" />
    <object name="Dequeuer" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.RequestDequeuer" assembly="FORIS.SPA.MmlProcessing.dll" />
    <object name="Commander" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.Processors.RequestProcessor" assembly="FORIS.SPA.MmlProcessing.dll" />
    <object name="ResponseAssembler" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.ResponseAssembler" assembly="FORIS.SPA.MmlProcessing.dll" />
    <object name="DummyResponder" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.DummyResponderConditional" assembly="FORIS.SPA.MmlProcessing.dll" />
    <object name="ConditionsResolver" class="FORIS.ScenarioProcessing.CommonUnits.IfMultipleXmlConditionsResolver" assembly="FORIS.SPA.SPF.dll"/>
    <object name="ByCUGSplitter" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.RequestSplitterByXmlNodes" assembly="FORIS.SPA.MmlProcessing.dll"/>
  </objectList>
  <automat>
    <node  name="BySubscribersSplitter" object="BySubscribersSplitter">
      <resultlist>
        <result value="0" node="SingleSubscriberRequestDequeuer" />
      </resultlist>
    </node>
    <node  name="SingleSubscriberRequestDequeuer" object="Dequeuer">
      <resultlist>
        <result value="0" node="ByCUGSplitter-Node" />
        <result value="1" node="" />
      </resultlist>
      <parameterslist>
        <param name="queueName" value="requestsSplittedBySubscriberQueue" />
      </parameterslist>
    </node>
    <node name="ByCUGSplitter-Node" object="ByCUGSplitter">
      <resultlist>
        <result value="0" node="SingleSubscriberRequestDequeuer"/>
        <result value="" node="SingleCUGRequestDequeuer"/>
      </resultlist>
      <parameterslist>
        <param name="Queue" value="requestsSplittedByCUGQueue"/>
        <param name="XmlPath:0" value="//AdditionalData/EndList/RFS_FP_SERVICE_VUG"/>
      </parameterslist>
    </node>
    <node name="SingleCUGRequestDequeuer" object="Dequeuer">
      <resultlist>
        <result value="0" node="ChechHostType-Node"/>
        <result value="" node="SingleSubscriberRequestDequeuer"/>
      </resultlist>
      <parameterslist>
        <param name="queueName" value="requestsSplittedByCUGQueue"/>
      </parameterslist>
    </node>
    <node name="ChechHostType-Node" object="ConditionsResolver">
      <resultlist>
        <result value="0" node="DeleteCugByRegisteredMsisdn"/>
        <result value="" node="DummyResponder-Node"/>
      </resultlist>
      <parameterslist>
        <param name="IfXmlPath:0" value="//CugAvailableOnFP/Value[text()='1']"/>
      </parameterslist>
    </node>
    <node name="DummyResponder-Node" object="DummyResponder">
      <parameterslist>
        <param name="OverallResult" value="SUCCESS" />
        <param name="Delay" value="0" />
        <param name="DetailedResult" value="SUCCESS" />
        <param name="DetailedResultDescription" value="Command not sending" />
      </parameterslist>
      <resultlist>
        <result value="" node="ResponseAssembler" />
      </resultlist>
    </node>
    <node  name="DeleteCugByRegisteredMsisdn" object="Commander">
      <resultlist>
        <result value="0" node="AddCugWithNewMsisdn" />
        <result value="1876" node="CreateCallingCircle" />
        <result value="" node="ResponseAssembler" />
      </resultlist>
      <parameterslist>
        <param name="AppendSubscribersData" value="false" />
        <param name="Command" value="SC.DeleteCallingCircleMembers" />
      </parameterslist>
    </node>
    <!-- если ВГП не существует - создаём -->
    <node name="CreateCallingCircle" object="Commander">
      <resultlist>
        <result value="0" node="AddCugWithNewMsisdn" />
        <result value="1877" node="AddCugWithNewMsisdn" />
        <result value="" node="ResponseAssembler" />
      </resultlist>
      <parameterslist>
        <param name="Command" value="SC.ChangeCallingCircle" />
        <param name="AppendSubscribersData" value="true" />
        <param name="ErrorDecodingType" value="ByString" />
      </parameterslist>
    </node>
    <node  name="AddCugWithNewMsisdn" object="Commander">
      <resultlist>
        <result value="" node="ResponseAssembler" />
      </resultlist>
      <parameterslist>
        <param name="AppendSubscribersData" value="false" />
        <param name="Command" value="SC.AddCugWithNewMsisdn" />
      </parameterslist>
    </node>
    <node  name="ResponseAssembler" object="ResponseAssembler">
      <resultlist>
        <result value="" node="SingleCUGRequestDequeuer" />
      </resultlist>
    </node>
  </automat>
</scenario>