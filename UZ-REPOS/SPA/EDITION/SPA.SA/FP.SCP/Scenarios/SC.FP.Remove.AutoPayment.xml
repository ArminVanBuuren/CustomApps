   </objectList>
   <automat>
      <!-- Начало Dummy проверок -->
      <node name="CheckSingleSCPEnabled" object="ConditionsResolver">
         <resultlist>
            <result value="0" node="SingleSCPDummyResponder" />
            <result value="" node="BySubscribersCutter" />
         </resultlist>
         <parameterslist>
            <param name="IfXmlPath:0" value="*['processingFlag(SingleSCPEnabled)'='True']" />
            <param name="IfXmlPath:1" value="//PreviousPlatform/HostCode" />
            <param name="IfNotXmlPath:0" value="//PreviousPlatform/HostCode[contains('[HOST_CODE_BS]',.)]" />
         </parameterslist>
      </node>
      <node name="SingleSCPDummyResponder" object="DummyResponder">
         <resultlist>
            <result value="" node="SkipResponseAssembler" />
         </resultlist>
         <parameterslist>
            <param name="OverallResult" value="SUCCESS" />
            <param name="DetailedResult" value="SUCCESS" />
            <param name="DetailedResultDescription" value="Command not sending because of old destination was SCP and Single SCP is enabled" />
         </parameterslist>
      </node>
      <node name="SkipResponseAssembler" object="ResponseAssembler">
         <resultlist>
            <result value="" node="" />
         </resultlist>
      </node>
      <!-- Окончание Dummy проверок -->
      <node name="BySubscribersCutter" object="BySubscribersCutter">
         <resultlist>
            <result value="0" node="IdentifiersGenerator-Node" />
            <result value="1" node="" />
         </resultlist>
      </node>
      <node name="IdentifiersGenerator-Node" object="IdentifiersGenerator">
         <parameterslist>
            <param name="WellknownName" value="GlobalUid" />
         </parameterslist>
         <resultlist>
            <result value="0" node="RemoveAutopayment" />
         </resultlist>
      </node>
      <node name="DummyResponderNode" object="DummyResponder">
         <resultlist>
            <result value="" node="ResponseAssembler" />
         </resultlist>
         <parameterslist>
            <param name="OverallResult" value="SUCCESS" />
            <param name="DetailedResult" value="SUCCESS" />
            <param name="DetailedResultDescription" value="Command not sending" />
         </parameterslist>
      </node>
      <node name="RemoveAutopayment" object="IfCommander">
         <resultlist>
            <result value="0" node="ResponseAssembler" />
            <result value="1538" node="DummyResponderNode" />
            <result value="" node="ResponseAssembler" />
         </resultlist>
         <parameterslist>
            <param name="IfXmlPath:0" value="//AdditionalData//RFS_FP_SERVICE_AUTO_PAYMENT/AutoPaymentThreshold" />
            <param name="Command" value="SC.RemoveAutopayment" />
         </parameterslist>
      </node>
      <node name="ResponseAssembler" object="ResponseAssembler">
         <resultlist>
            <result value="" node="BySubscribersCutter" />
         </resultlist>
      </node>
   </automat>
</scenario>