<Command Name="SC.SM.AddSubscriber">
  <Description>Создание абонента на SMSC</Description>
  <Author Name="Cherneckiy A." Date="2018-03-20+10:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[OPERATION=SUBSCRIBER-PROV-ADD&MSISDN=##MSISDN##&IMSI=##IMSI##&ID=##Profile##]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate>
      <![CDATA[]]>
    </EmulationPrintoutTemplate>
    <Item Name="MSISDN" Type="Wellknown" />
    <Item Name="IMSI" Type="Wellknown" />
    <Item Name="Profile" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="Profile">
      <Source Path="//RES_SM_PROFILE/Value[text()!='']" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="ascii" />
</Command>