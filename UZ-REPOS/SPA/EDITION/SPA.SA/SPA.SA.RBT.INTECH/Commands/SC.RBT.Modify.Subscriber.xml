<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.RBT.Modify.Subscriber" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
  <Description>Создание и удаление пользователя на платформе RBT</Description>
  <Author Name="A.Branov" Date="2018-09-25+15:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA["cmd=##Action##&msisdn=##msisdn##"]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown"/>
    <Item Name="Action" Type="LocalParameter"/>
  </Request>
  <ParametersList>
    <Parameter Name="Action" IsChoice="true">
      <Source Path="//AddList/RFS_RBT_SUBSCRIPTION" PostFormat="new"/>
      <Source Path="//AddList/RFS_RBT_SERVICE" PostFormat="on"/>
      <Source Path="//RemoveList/RFS_RBT_SUBSCRIPTION" PostFormat="remove"/>
      <Source Path="//RemoveList/RFS_RBT_SERVICE" PostFormat="of"/>
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="windows-1251">
    <Error Name="4" Value="SUCCESS"/>
    <Error Name="9" Value="SUCCESS"/>
  </ErrorsList>
</Command>