<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.NAM_Modify" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>Изменение сервиса интернет NAM</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA[MOD NAM:MSISDN=##msisdn##,NAM=##NET_STATUS##,Keep=1,KEEPEPS=1;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown"/>
    <Item Name="NET_STATUS" Type="Parameter"/>
  </Request>
  <ErrorsList CodePage="windows-1251">
    <Error Name="110109" Value="SUCCESS"/> <!--Subscriber does not exist-->
  </ErrorsList>
</Command>