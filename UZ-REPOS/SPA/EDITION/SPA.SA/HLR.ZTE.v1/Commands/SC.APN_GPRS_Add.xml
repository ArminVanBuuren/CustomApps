<Command Name="SC.APN_GPRS_Add" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>Добавление услуги 'GPRS'APN</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[SET TPLGPRS:MSISDN=##msisdn##,GPRSTPL=##NET_APN##;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate>
      <![CDATA[]]>
    </EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="NET_APN" Type="Parameter" />
  </Request>
  <ErrorsList CodePage="windows-1251" />
</Command>