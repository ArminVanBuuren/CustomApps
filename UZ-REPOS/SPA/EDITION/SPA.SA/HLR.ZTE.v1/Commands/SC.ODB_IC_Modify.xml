<Command Name="SC.ODB_IC_Modify" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>Модификация баррингов входящей связи</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[MOD ODB:MSISDN=##msisdn##,BIC=##IncomingCallsBarring##;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate>
      <![CDATA[]]>
    </EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="IncomingCallsBarring" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="IncomingCallsBarring" IsChoice="true" AbsenceValue="1">
      <Source Path="//AddList/RFS_HL_ODB_IC/RES_HL_ODB_IC/Value[text()='AllCalls']" PostFormat="0" />
      <Source Path="//ModifyList/RFS_HL_ODB_IC/RES_HL_ODB_IC/Value[text()='AllCalls']" PostFormat="0" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="windows-1251">
    <Error Name="110109" Value="SUCCESS" />
    <!--Subscriber does not exist-->
  </ErrorsList>
</Command>