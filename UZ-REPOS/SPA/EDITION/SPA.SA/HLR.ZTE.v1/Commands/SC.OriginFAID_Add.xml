<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.OriginFAID_Add" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>Определение региона для Переадресации</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA[Mod BscEx:MSISDN=##msisdn##,ORIGINFAID=##REGION##;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown"/>
    <Item Name="REGION" Type="LocalParameter"/>
  </Request>
  <ParametersList>
    <Parameter Name="REGION" AbsenceValue="">
      <Source Path="//RES_HL_REGION/Value" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="windows-1251" />
</Command>