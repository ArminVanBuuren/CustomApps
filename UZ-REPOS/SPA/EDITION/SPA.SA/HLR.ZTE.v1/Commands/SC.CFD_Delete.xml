<Command Name="SC.CFD_Delete" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>Delete call forwarding (to MCA) on mobile subscriber busy</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[MOD SS:MSISDN=##msisdn##,CFD=0;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate>
      <![CDATA[]]>
    </EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown" />
  </Request>
  <ErrorsList CodePage="windows-1251" />
</Command>