<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.LTEsReduceSpeed_Add" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
  <Description>Добавление ограничения скорости GPRS до 128кбс для LTE</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA[Mod APNPROF:MSISDN=##msisdn##,OAPNCFGID=1,NEWEPCAPN=##ReduceSpeedApn##,EPSQOSID=107;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown"/>
    <Item Name="ReduceSpeedApn" Type="LocalParameter"/>
  </Request>
  <ParametersList>
    <Parameter Name="ReduceSpeedApn">
      <Source Path="//RES_HL_REDUCE_SPEED_APN/Value"/>
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="windows-1251" />
</Command>