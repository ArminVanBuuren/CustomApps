<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.LTEs2_Add" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
  <Description>Добавление услуги 'LTE 4G' 2</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA[Set TPLAPNCP:MSISDN=##msisdn##,EPCAPNCPTPL=51;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown"/>
  </Request>
  <ErrorsList CodePage="windows-1251" >
    <Error Name="222007" Value="SUCCESS"/> <!--Non-EPC users can't include the parameters only for EPC users-->
  </ErrorsList>
</Command>