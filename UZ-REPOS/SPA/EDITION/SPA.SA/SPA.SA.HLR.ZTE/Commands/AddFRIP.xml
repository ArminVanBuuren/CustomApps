<?xml version="1.0" encoding="UTF-8"?>
<Command Name="AddFRIP" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
	<Description>AddFRIP</Description>
	<Author Name="Timur Khamzin" Date="2015-11-16+15:00"/>
	<Request Template='MOD PDP:MSISDN={0},OAPN=NET.UMS.UZ,OPDPTYPE=0,PDPAddr={1};^M'>                           
		<Item Name="msisdn" Type="Wellknown"/>
                <Item Name="IP" Type="Parameter"/>
	</Request>

	<ErrorsList CodePage="windows-1251" >
          <Error Name="ERR3045" Value="FAILURE"/>
          <Error Name="104080" Value="SUCCESS"/>
        </ErrorsList>
</Command>
