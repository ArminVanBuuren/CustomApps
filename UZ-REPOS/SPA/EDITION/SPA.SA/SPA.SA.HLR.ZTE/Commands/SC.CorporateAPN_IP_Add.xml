<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.CorporateAPN_IP_Add" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description></Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA[MOD PDP:MSISDN=##msisdn##,OAPN=##CORPORATE_APN##,OPDPTYPE=0,PDPAddr=##IP##;^M]]>
