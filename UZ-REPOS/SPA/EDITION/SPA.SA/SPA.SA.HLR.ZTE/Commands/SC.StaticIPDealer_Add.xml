<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.StaticIPDealer_Add" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>Добавить дилерский статический IP</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA[MOD PDP:MSISDN=##msisdn##,OAPN=##DEALER_APN##,OPDPTYPE=0,PDPAddr=##IP##;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown"/>
    <Item Name="DEALER_APN" Type="Parameter"/>
    <Item Name="IP" Type="Parameter"/>
  </Request>
  <ErrorsList CodePage="windows-1251" >
    <Error Name="ERR3045" Value="FAILURE"/>
  </ErrorsList>
</Command>