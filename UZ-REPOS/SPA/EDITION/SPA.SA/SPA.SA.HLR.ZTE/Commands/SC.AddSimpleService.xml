<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.AddSimpleService" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
  <Description>Активация услуг SS</Description>
  <Author Name="Khovanskiy Vladimir" Date="2015-11-16+15:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA[MOD SS:MSISDN=##msisdn##,##ServiceType##;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown"/>
    <Item Name="ServiceType" Type="LocalParameter"/>
  </Request>
  <ParametersList>
    <Parameter Name="ServiceType" IsChoice="true" AbsenceValue="">
      <Source Path="//RFS_HL_MCA" PostFormat="CFD=1,CFDNTC=1,CFDNTF=1" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="windows-1251" />
</Command>