<?xml version="1.0" encoding="UTF-8"?>
<Command Name="ActivateCFNRC" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
  <Description>Активания услуги 'Переадресация вызова (по недоступности)'</Description>
  <Author Name="Timur Khamzin" Date="2015-11-16+15:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA[Reg FN :MSISDN=##msisdn##,SS=CFNRC,Bsg=0,Register=1,Active=1,FwdNum=00##ForwardingToAddress##;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown"/>
    <Item Name="ForwardingToAddress" Type="LocalParameter"/>
  </Request>
  <ParametersList>
    <Parameter Name="ForwardingToAddress">
      <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList//CFNRC/ForwardingToAddress"/>
      <PossibleValues StringToKeyTable="false"/>
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="windows-1251" />
</Command>