<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.ODB_OC_Modify" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>Модификация баррингов исходящей связи</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA[MOD ODB:MSISDN=##msisdn##,BOC=##OutgoingCallsBarring##;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown"/>
    <Item Name="OutgoingCallsBarring" Type="LocalParameter"/>
  </Request>
  <ParametersList>
    <Parameter Name="OutgoingCallsBarring" IsChoice="true" AbsenceValue="">
      <Source Path="//AddList/RFS_HL_ODB_OC/RES_HL_ODB_OC/Value[text()='NoCalls']" PostFormat="1" />
      <Source Path="//ModifyList/RFS_HL_ODB_OC/RES_HL_ODB_OC/Value[text()='NoCalls']" PostFormat="1" />
      <Source Path="//RemoveList/RFS_HL_ODB_OC/RES_HL_ODB_OC/Value[text()='NoCalls']" PostFormat="3" />
      <Source Path="//RFS_HL_ODB_OC/RES_HL_ODB_OC/Value[text()='IntercityCalls']" PostFormat="2" />
      <Source Path="//RFS_HL_ODB_OC/RES_HL_ODB_OC/Value[text()='InternationalCalls']" PostFormat="0" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="windows-1251" />
</Command>