<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.GPRSAPN_Add" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
  <Description>Добавление услуги 'GPRS'APN</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA[SET TPLGPRS:MSISDN={0},GPRSTPL=51;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown"/>
  </Request>
  <ErrorsList CodePage="windows-1251" />
</Command>