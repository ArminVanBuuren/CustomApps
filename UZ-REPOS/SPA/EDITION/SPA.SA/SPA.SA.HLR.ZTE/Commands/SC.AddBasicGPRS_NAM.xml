<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.AddBasicGPRS_NAM" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
  <Description>Добавление услуги 'GPRS'NAM</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA[MOD NAM:MSISDN=##msisdn##,NAM=0,Keep=1,KEEPEPS=1;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown"/>
  </Request>
  <ErrorsList CodePage="windows-1251" />
</Command>