<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.PLSS_D_Modify" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>Изменение статуса USSD</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA[MOD SS: MSISDN=##msisdn##,PLSS_D=##USSD_STATUS##;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown"/>
    <Item Name="USSD_STATUS" Type="LocalParameter"/>
  </Request>
  <ParametersList>
    <Parameter Name="USSD_STATUS" IsChoice="true" AbsenceValue="">
      <Source Path="//AddList/RFS_HL_PLSS/RES_HL_PLSS_MODE/Value[text()='Provision']" PostFormat="0" />
      <Source Path="//ModifyList/RFS_HL_PLSS/RES_HL_PLSS_MODE/Value[text()='Provision']" PostFormat="0" />
      <Source Path="//RemoveList/RFS_HL_PLSS/RES_HL_PLSS_MODE/Value[text()='Suspend']" PostFormat="0" />
      <Source Path="//RemoveList/RFS_HL_PLSS/RES_HL_PLSS_MODE/Value[text()='Provision']" PostFormat="1" />
      <Source Path="//AddList/RFS_HL_PLSS/RES_HL_PLSS_MODE/Value[text()='Suspend']" PostFormat="1" />
      <Source Path="//ModifyList/RFS_HL_PLSS/RES_HL_PLSS_MODE/Value[text()='Suspend']" PostFormat="1" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="windows-1251" />
</Command>