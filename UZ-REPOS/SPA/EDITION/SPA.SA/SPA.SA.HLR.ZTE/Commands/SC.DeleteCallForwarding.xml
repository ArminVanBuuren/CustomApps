<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.DeleteCallForwarding" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
  <Description>Удаление услуги переадресация</Description>
  <Author Name="Khovanskiy Vladimir" Date="2015-11-16+15:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA[MOD SS:MSISDN=##msisdn##,##CallForwardName##;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown"/>
    <Item Name="CallForwardName" Type="LocalParameter"/>
  </Request>
  <ParametersList>
    <Parameter Name="CallForwardName" IsChoice="true" AbsenceValue="">
      <Source Path="//RFS_HL_CFB" PostFormat="CFB=0" />
      <Source Path="//RFS_HL_CFU" PostFormat="CFU=0" />
      <Source Path="//RFS_HL_CFNRC" PostFormat="CFNRC=0" />
      <Source Path="//RFS_HL_CFNRY" PostFormat="CFNRY=0" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="windows-1251" />
</Command>