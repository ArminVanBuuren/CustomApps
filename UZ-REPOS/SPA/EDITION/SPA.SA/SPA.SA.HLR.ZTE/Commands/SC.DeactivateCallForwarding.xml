<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.DeactivateCallForwarding" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
  <Description>Деактивация услуги переадресация вызова</Description>
  <Author Name="Khovanskiy Vladimir" Date="2015-11-16+15:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA[Reg FN :MSISDN=##msisdn##,SS=##CallForwardName##,Bsg=0,Register=0,Active=0;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown"/>
    <Item Name="CallForwardName" Type="LocalParameter"/>
  </Request>
  <ParametersList>
    <Parameter Name="CallForwardName" IsChoice="true" AbsenceValue="">
      <Source Path="//RFS_HL_CFB" PostFormat="CFB" />
      <Source Path="//RFS_HL_CFU" PostFormat="CFU" />
      <Source Path="//RFS_HL_CFNRC" PostFormat="CFNRC" />
      <Source Path="//RFS_HL_CFNRY" PostFormat="CFNRY" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="windows-1251" />
</Command>