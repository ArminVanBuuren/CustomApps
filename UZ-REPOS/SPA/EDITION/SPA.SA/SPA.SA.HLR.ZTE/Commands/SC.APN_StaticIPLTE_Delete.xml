<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.APN_StaticIPLTE_Delete" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>Удалить статический IP</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00"/>
  <Request>
    <RequestTemplate>
      <![CDATA[Mod APNPROF:MSISDN=##msisdn##,OAPNCFGID=1,EPCPDNAddr1=*;^M]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="msisdn" Type="Wellknown"/>
  </Request>
  <ErrorsList CodePage="windows-1251" >
    <Error Name="104080" Value="SUCCESS"/> <!--Can not find the corresponding PDP information-->
  </ErrorsList>
</Command>