<Command Name="SC.CreateSubscriber">
  <Description>Создание абонента</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[cmd=additem&imsi=##imsi##&msisdn=##msisdn##&timestamp=##datetime##&foreign=##foreign##]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="imsi" Type="Wellknown" />
    <Item Name="foreign" Type="LocalParameter" />
    <Item Name="datetime" Type="Wellknown" Format="{0:yyyyMMddHHmmss}" />
  </Request>
  <ParametersList>
    <Parameter Name="foreign">
      <Source Path="ExtendedRequestMessage/AdditionalData/Foreign" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="ascii" />
</Command>