<Command Name="SC.ChangeMSISDN">
  <Description>Изменение MSISDN абонента</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[cmd=changemsisdn&imsi=##imsi##&oldmsisdn=##msisdn##&newmsisdn=##NewMSISDN##&timestamp=##datetime##]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="imsi" Type="Wellknown" />
    <Item Name="NewMSISDN" Type="LocalParameter" />
    <Item Name="datetime" Type="Wellknown" Format="{0:yyyyMMddHHmmss}" />
  </Request>
  <ParametersList>
    <Parameter Name="NewMSISDN">
      <Source Path="//NewMSISDN/Value[text()!='']" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="ascii" />
</Command>