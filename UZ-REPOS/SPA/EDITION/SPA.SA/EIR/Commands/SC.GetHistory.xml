<Command Name="SC.GetHistory">
  <Description>Получение истории</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[cmd=gethistory&imsi=##imsi##&msisdn=##msisdn##&pastdays=##pastdays##]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="imsi" Type="Wellknown" />
    <Item Name="pastdays" Type="LocalParameter" />
  </Request>
  <ParametersList>
    <Parameter Name="pastdays">
      <Source Path="ExtendedRequestMessage/AdditionalData/Pastdays" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="ascii" />
</Command>