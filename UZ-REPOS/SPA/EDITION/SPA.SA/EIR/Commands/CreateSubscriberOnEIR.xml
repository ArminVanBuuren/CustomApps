<Command Name="CreateSubscriberOnEIR" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
   <Description>Добавление связки Номера и Имси</Description>
   <Author Name="Timur Khamzin" Date="2019-04-04+05:00" />
   <Request Template="cmd=additem&amp;imsi={1}&amp;msisdn={0}&amp;timestamp={3:yyyyMMddHHmmss}&amp;foreign={2}">
      <Item Name="msisdn" Type="Wellknown" />
      <Item Name="imsi" Type="Wellknown" />
      <Item Name="foreign" Type="Parameter" />
      <Item Name="datetime" Type="Wellknown" Format="{0:yyyyMMddHHmmss}" />
   </Request>
   <ErrorsList CodePage="ASCII" />
</Command>