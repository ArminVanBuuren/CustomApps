<Command Name="SC.ChangeIMSI">
  <Description>Изменение IMSI абонента</Description>
  <Author Name="Khovanskiy Vladimir" Date="2018-09-20+09:00" />
  <Request>
    <RequestTemplate>
      <![CDATA[cmd=changeimsi&msisdn=##msisdn##&oldimsi=##imsi##&newimsi=##NewIMSI##&timestamp=##datetime##]]>
    </RequestTemplate>
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="imsi" Type="Wellknown" />
    <Item Name="NewIMSI" Type="LocalParameter" />
    <Item Name="datetime" Type="Wellknown" Format="{0:yyyyMMddHHmmss}" />
  </Request>
  <ParametersList>
    <Parameter Name="NewIMSI">
      <Source Path="//NewIMSI/Value[text()!='']" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="ascii" />
</Command>