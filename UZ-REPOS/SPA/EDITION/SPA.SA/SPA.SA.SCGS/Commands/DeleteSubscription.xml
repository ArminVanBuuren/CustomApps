<?xml version="1.0" encoding="UTF-8"?>
<Command Name="AddSubscription" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
	<Description>Удаление подписки услуги на платформе SCG</Description>
	<Author Name="Vladimir Khovanskiy" Date="2009-04-26+11:09"/>
	<Request>
		<RequestTemplate>
			<![CDATA[<soapenv:Envelope 
xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" 
xmlns:bas="http://basicservices.ib.sdp.huawei.com" 
xmlns:req="http://request.basicservices.ib.sdp.huawei.com" 
xmlns:sch="http://schema.ib.sdp.huawei.com">
   <soapenv:Header/>
   <soapenv:Body>
      <bas:in1>
         <req:channelID>0</req:channelID>         
         <req:subInfo>            
            <sch:payType>0</sch:payType>
            <sch:productID>1000##InspectID##</sch:productID>
            <sch:subType>0</sch:subType>
         </req:subInfo>
         <req:userID>
            <sch:ID>##msisdn##</sch:ID>
            <sch:type>0</sch:type>
         </req:userID>
      </bas:in1>
   </soapenv:Body>
</soapenv:Envelope>]]>
		</RequestTemplate>
		<EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
		<Item Name="msisdn" Type="Wellknown"/>	
		<Item Name="InspectID" Type="Parameter"/>	
	</Request>
	<ErrorsList CodePage="windows-1251" />
</Command>