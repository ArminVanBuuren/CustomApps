<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.Modify_DataTransfer">
	<Description>Изменение мобильной передачи данных</Description>
	<Author Name="system" Date="2018-09-20+09:00"/>
	<Request>
		<RequestTemplate>
			<![CDATA[Mod BscEx:MSISDN=##msisdn##,CDA9600=##action##;^M]]>
		</RequestTemplate>
		<Item Name="msisdn" Type="Wellknown"/>
		<Item Name="action" Type="LocalParameter"/>
	</Request>
	<ParametersList>
		<Parameter Name="action">
			<Source Path="//AddList/RFS_HLR_DATATRANSFER" PresenceValue="1" AbsenceValue="0"/>
		</Parameter>
	</ParametersList>
</Command>