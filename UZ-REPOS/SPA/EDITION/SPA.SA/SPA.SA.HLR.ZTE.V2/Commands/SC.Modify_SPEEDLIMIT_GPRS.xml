<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.Modify_SPEEDLIMIT_GPRS">
	<Description>Изменение ограничения скорости GPRS до 128кбс</Description>
	<Author Name="system" Date="2018-09-20+09:00"/>
	<Request>
		<RequestTemplate>
			<![CDATA[MOD PDP:MSISDN=##msisdn##,OAPN=##NET_APN##,OPDPTYPE=0,QOSID=##SPEEDLIMIT##;^M]]>
		</RequestTemplate>
		<Item Name="msisdn" Type="Wellknown"/>
		<Item Name="NET_APN" Type="Parameter"/>
		<Item Name="SPEEDLIMIT" Type="Parameter"/>
	</Request>
</Command>