<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.GetSubscriber">
	<Description>Запрос состояния абонента на HLR</Description>
	<Author Name="system" Date="2018-09-20+09:00"/>
	<Request>
		<RequestTemplate>
			<![CDATA[Qry User:MSISDN=##msisdn##,Item=ALL;^M]]>
		</RequestTemplate>
		<Item Name="msisdn" Type="Wellknown"/>
	</Request>
	<ErrorsList CodePage="windows-1251">
		<Error Name="ERR3001" Value="NoSuchObjectInstance"/>
		<Error Name="" Value="SUCCESS"/>
	</ErrorsList>
</Command>