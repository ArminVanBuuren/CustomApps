<scenario name="SC.RBT.Modify.Subscriber">
  <objectList>
    <object name="BySubscribersCutter" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.RequestCutterBySubscribers" assembly="FORIS.SPA.MmlProcessing.dll" />
    <object name="Commander" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.Processors.RequestProcessor" assembly="FORIS.SPA.MmlProcessing.dll" />
    <object name="ResponseAssembler" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.ResponseAssembler" assembly="FORIS.SPA.MmlProcessing.dll" />
    <object name="RequestCutterByXmlNodes" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.RequestCutterByXmlNodes" assembly="FORIS.SPA.MmlProcessing.dll" />
    <object name="IfMultipleXmlConditionsResolver" class="FORIS.ScenarioProcessing.CommonUnits.IfMultipleXmlConditionsResolver" assembly="FORIS.SPA.SPF.dll" />
    <object name="WellknownsReplacer" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.WellknownsReplacer" assembly="FORIS.SPA.MmlProcessing.dll" />
  </objectList>
  <automat>
    <node name="BySubscribersCutter" object="BySubscribersCutter">
      <resultlist>
        <result value="0" node="RequestCutterByXmlNodes" />
        <result value="1" node="" />
      </resultlist>
    </node>
    <node name="RequestCutterByXmlNodes" object="RequestCutterByXmlNodes">
      <resultlist>
        <result value="0" node="CheckNewMSISDN" />
        <result value="1" node="ResponseAssembler" />
      </resultlist>
      <parameterslist>
        <param name="XmlPath:0" value="//RemoveList | //AddList" />
      </parameterslist>
    </node>
    <node name="CheckNewMSISDN" object="IfMultipleXmlConditionsResolver">
      <resultlist>
        <result value="0" node="WellknownsReplacerNewMSISDN" />
        <result value="" node="Modify_Subscriber" />
      </resultlist>
      <parameterslist>
        <param name="IfXmlPath:0" value="//NewMSISDN/Value[text()!='']" />
        <param name="IfXmlPath:1" value="//AddList" />
      </parameterslist>
    </node>
    <node name="WellknownsReplacerNewMSISDN" object="WellknownsReplacer">
      <resultlist>
        <result value="" node="Modify_Subscriber" />
      </resultlist>
      <parameterslist>
        <param name="msisdn" value="xmlsubstr(&quot;//NewMSISDN&quot;)" />
      </parameterslist>
    </node>
    <node name="Modify_Subscriber" object="Commander">
      <resultlist>
        <result value="0" node="RequestCutterByXmlNodes" />
        <result value="" node="ResponseAssembler" />
      </resultlist>
      <parameterslist>
        <param name="Command" value="SC.Modify_Subscriber" />
      </parameterslist>
    </node>
    <node name="ResponseAssembler" object="ResponseAssembler">
      <resultlist>
        <result value="" node="BySubscribersCutter" />
      </resultlist>
    </node>
  </automat>
</scenario>