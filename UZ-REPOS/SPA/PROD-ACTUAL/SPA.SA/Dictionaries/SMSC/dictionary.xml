<?xml version="1.0" encoding="utf-8"?>
<Dictionary xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <WellknownsList>
    <Wellknown Name="msisdn" Value="xmlsubstr(&quot;ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN&quot;)"/>
    <Wellknown Name="msisdn.cc" Value="xmlsubstr(&quot;ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN&quot;,0,3)"/>
    <Wellknown Name="msisdn.ndc" Value="xmlsubstr(&quot;ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN&quot;,3,2)"/>
    <Wellknown Name="msisdn.sn" Value="xmlsubstr(&quot;ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN&quot;,5)"/>
    <Wellknown Name="msisdn.fn" Value="xmlsubstr(&quot;ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN&quot;,3)"/>
    <Wellknown Name="imsi" Value="xmlsubstr(&quot;ExtendedRequestMessage/IdentifiersList/Identifier/IMSI&quot;)"/>
    <Wellknown Name="imsi.mcc" Value="xmlsubstr(&quot;ExtendedRequestMessage/IdentifiersList/Identifier/IMSI&quot;,0,3)"/>
    <Wellknown Name="imsi.mnc" Value="xmlsubstr(&quot;ExtendedRequestMessage/IdentifiersList/Identifier/IMSI&quot;,3,2)"/>
    <Wellknown Name="imsi.msin" Value="xmlsubstr(&quot;ExtendedRequestMessage/IdentifiersList/Identifier/IMSI&quot;,5)"/>
  </WellknownsList>
  <ErrorsList CodePage="ASCII">                                                 
    <Error Name="000"  Value="SUCCESS"               Description="Абонент успешно создан"/>
    <Error Name="102"  Value="SUCCESS"               Description="Абонент не существует (при удалении)"/>
    <Error Name="101"  Value="MsisdnAlreadyDefined"  Description="Абонент уже существует" />
    <Error Name="103"  Value="UnregisteredProfile"   Description="Абонент не существует (при внесении изменений)"/>
    <Error Name="104"  Value="InvalidProfileFormat"  Description="В списке профайлов нет указанной системы"/>
    <Error Name="105"  Value="UnregisteredError"     Description="Ошибка соединения с базой данных"/>
    <Error Name="106"  Value="SUCCESS"               Description="Ошибка выполнения запроса"/>
    <Error Name="001"  Value="InvalidRequest"        Description="Неизвестная операция"/>
    <Error Name="002"  Value="InvalidIdentifier"     Description="Недостаточно параметров (не заданы операция, msisdn, imsi..)"/>
    <Error Name="003"  Value="InvalidIdentifier"     Description="Не найден ключ поиска (одновременно указываются nmsisdn и nimsi)"/>
    <Error Name="004"  Value="InvalidIdentifier"     Description="Не найдены параметры для обновления (в запросе нет nmsisdn или nimsi или nid)"/>
  </ErrorsList>
  <RegexpsList>
    <Regexp Name="Error" Value="^(?&lt;ERROR&gt;\d{1,3})$"/>
    <!--Regexp Name="Error" Value="(Result&quot;\s{0,}value=&quot;(?&lt;ERROR&gt;^\d{1,3}$)&quot;)"/-->
  </RegexpsList>
  <ConstantsList>
    <Constant Name="_MakroRegion" Value="2"/>
  </ConstantsList>
  <ParametersList>
    <Parameter Name="NewIMSI">
      <Source Path="ExtendedRequestMessage/AdditionalData//NewIMSI"/>
      <PossibleValues StringToKeyTable="true"/>
    </Parameter>
    <Parameter Name="NewMsisdn">
      <Source Path="ExtendedRequestMessage/AdditionalData//NewMSISDN"/>
      <PossibleValues StringToKeyTable="true"/>
    </Parameter>
    <Parameter Name="SCP_Profile">
      <Source Path="//AdditionalData/BalanceStorage/HostCode"/>
      <PossibleValues StringToKeyTable="false">
        <Item Key="MS" StringValue="SCP1"/>
        <Item Key="05" StringValue="AM"/>
      </PossibleValues>
    </Parameter>
    <Parameter Name="NewSCP_Profile">
      <Source Path="//AdditionalData/ProvisionList/ChangeTP/NewPlatform/HostCode"/>
      <PossibleValues StringToKeyTable="false">
        <Item Key="MS" StringValue="SCP1"/>
        <Item Key="05" StringValue="AM"/>
      </PossibleValues>
    </Parameter>
  </ParametersList>
  <DynamicParametersList/>
  <CommandsList Root="C:\FORIS\SPA\Configuration\SPA.SA\Dictionaries\SMSC\Commands">
    <Command Path="AddSubscriberOnSMSC.xml"/>
    <Command Path="DeleteSubscriberOnSMSC.xml"/>
    <Command Path="ChangeIMSI.xml"/>
    <Command Path="ChangeSMSCProfile.xml"/>
    <Command Path="ChangeMSISDN.xml"/>
    <Command Path="GetSubscriberByIMSI.xml"/>
    <Command Path="GetSubscriberByMSISDN.xml"/>
    <Command Path="DeleteSubscriberOnSMSC_MSISDN.xml"/>
    <Command Path="DeleteSubscriberOnSMSC_IMSI.xml"/>
    <Command Path="DeleteSubscriberOnSMSC_NEW_MSISDN.xml"/>
    <Command Path="DeleteSubscriberOnSMSC_NEW_IMSI.xml"/>
  </CommandsList>
</Dictionary>
