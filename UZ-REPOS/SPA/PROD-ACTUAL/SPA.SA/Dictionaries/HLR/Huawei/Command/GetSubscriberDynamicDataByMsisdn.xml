	<Description>Запрос динамических параметров абонента</Description>
	<Author Name="Vladimir Khovanskiy" Date="2010-03-16+15:00"/>
	<!--Request Template='LST SUB: ISDN="{0}", DETAIL=FALSE;'-->
	<Request Template='LST ODBDAT: ISDN="{0}";'>
