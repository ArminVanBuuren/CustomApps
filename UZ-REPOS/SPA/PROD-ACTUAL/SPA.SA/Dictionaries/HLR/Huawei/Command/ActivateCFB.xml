<?xml version="1.0" encoding="UTF-8"?>
<Command Name="ActivateCFB" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
	<Description>Активания услуги 'Переадресация вызова (по занятости)'</Description>
	<Author Name="Vladimir Khovanskiy" Date="2010-03-16+15:00"/>
	<Request Template='REG CFB: ISDN="{0}",FTN="{1}",PROPERTY=INTERNATIONAL;^M'>
		<Item Name="msisdn" Type="Wellknown"/>
		<Item Name="ForwardingToAddress" Type="LocalParameter"/>
	</Request>
	<ParametersList>
		<Parameter Name="ForwardingToAddress">
			<Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList//CFB/ForwardingToAddress"/>
			<PossibleValues StringToKeyTable="false"/>
		</Parameter>
	</ParametersList>
	<ErrorsList CodePage="windows-1251" />
</Command>