<?xml version="1.0" encoding="UTF-8"?>
<Command Name="AddFRIP_DEALER" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
	<Description>AddFRIP_DEALER</Description>
	<Author Name="Timur Khamzin" Date="2015-03-16+15:00"/>
	<Request Template='MOD GPRS: ISDN="{0}", PROV=TRUE, CNTXID=5, PDPTYPE=IPV4, PDPADD="{1}", ADDIND=STATIC;^M'>
                           
		<Item Name="msisdn" Type="Wellknown"/>
                <Item Name="IP" Type="Parameter"/>
	</Request>

	<ErrorsList CodePage="windows-1251" >
          <Error Name="ERR3045" Value="FAILURE"/>
        </ErrorsList>
</Command>
