<Decoder Name="CallForwardingInfo" Type="FORIS.ServiceProvisioning.MmlProcessing.Dictionary.NestedIdentifiersDecoder, FORIS.SPA.MmlProcessing">
	<Attribute Key="BasicService" RegexpPattern="(?&lt;parameter&gt;(CFB|CFU|CFNRY|CFNRC))" RegexpOptions="Singleline, ExplicitCapture">
		<PossibleValues StringToKeyTable="false">
			<Item Key="CFB" StringValue="TeleService"/>
			<Item Key="CFU" StringValue="TeleService"/>
			<Item Key="CFNRC" StringValue="TeleService"/>
			<Item Key="CFNRY" StringValue="TeleService"/>
		</PossibleValues>
	</Attribute>
	<Attribute Key="AdditionalInfo/Number" RegexpPattern="(CFB|CFU|CFNRY|CFNRC)\s*(NOT ACTIVE|ACTIVE-OP)\s*(?&lt;parameter&gt;\d+)" RegexpOptions="Singleline, ExplicitCapture"/>
	<Attribute Key="GeneralInfo/NoReplyTimer" RegexpPattern="(CFB|CFU|CFNRY|CFNRC)\s*(NOT ACTIVE|ACTIVE-OP)\s*\d+\s+(?&lt;parameter&gt;\d+)" RegexpOptions="Singleline, ExplicitCapture"/>
	<Attribute Key="ActivationStatus" RegexpPattern="(?&lt;parameter&gt;(NOT ACTIVE|ACTIVE-OP))" RegexpOptions="Singleline, ExplicitCapture">
		<PossibleValues StringToKeyTable="false">
			<Item Key="NOT ACTIVE" StringValue="Provisioned"/>
			<Item Key="ACTIVE-OP" StringValue="Activated"/>
		</PossibleValues>
	</Attribute>
</Decoder>
