<?xml version="1.0" encoding="UTF-8"?>
<Command Name="ChangeMSISDNOnEIR" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
	<Description>Замена связки Номера и Имси</Description>
	<Author Name="Timur Khamzin" Date="2019-04-04+05:00"/>
	<Request Template="cmd=changemsisdn&amp;imsi={1}&amp;oldmsisdn={0}&amp;newmsisdn={2}&amp;timestamp={3:yyyyMMddHHmmss}">
		<Item Name="msisdn" Type="Wellknown"/>
		<Item Name="imsi" Type="Wellknown"/>
                <Item Name="NewMSISDN" Type="Parameter"/>
        	<Item Name="datetime" Type="Wellknown" Format="{0:yyyyMMddHHmmss}"/>
	</Request>                                                                                            
	<ErrorsList CodePage="ASCII"/>
</Command>
