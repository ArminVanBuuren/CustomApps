<Command Name="ActivateCFU" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
   <Description>Активания услуги 'Переадресация вызова (безусловная)'</Description>
   <Author Name="Timur Khamzin" Date="2015-11-16+15:00" />
   <Request Template="Reg FN :MSISDN={0},SS=CFU,Bsg=0,Register=1,Active=1,FwdNum=00{1};^M">
      <Item Name="msisdn" Type="Wellknown" />
      <Item Name="ForwardingToAddress" Type="LocalParameter" />
   </Request>
   <ParametersList>
      <Parameter Name="ForwardingToAddress">
         <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList//CFU/ForwardingToAddress" />
         <PossibleValues StringToKeyTable="false" />
      </Parameter>
   </ParametersList>
   <ErrorsList CodePage="windows-1251" />
</Command>