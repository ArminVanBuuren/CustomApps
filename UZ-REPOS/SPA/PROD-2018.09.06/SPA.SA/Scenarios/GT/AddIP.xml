<?xml version="1.0" encoding="UTF-8"?>
<scenario name="AddIP">
	<objectList>
		<object name="BySubscirbersSplitter" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.RequestSplitterBySubscribers" assembly="FORIS.SPA.MmlProcessing.dll"/>
		<object name="Dequeuer" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.RequestDequeuer" assembly="FORIS.SPA.MmlProcessing.dll"/>
		<object name="Commander" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.Processors.RequestProcessor" assembly="FORIS.SPA.MmlProcessing.dll"/>
		<object name="ResponseAssembler" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.ResponseAssembler" assembly="FORIS.SPA.MmlProcessing.dll"/>
                <object name="DummyResponder" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.DummyResponder" assembly="FORIS.SPA.MmlProcessing.dll"/>
                <object name="DataBaseProcessor" class="FORIS.ScenarioProcessing.CommonUnits.DataBaseProcessor" assembly="FORIS.SPA.SPF.dll"/>
		<object name="XmlNodesCopier" class="FORIS.ScenarioProcessing.CommonUnits.XmlNodesCopier" assembly="FORIS.SPA.SPF.dll"/>
	</objectList>
	<automat>
		<node name="BySubscirbersSplitter" object="BySubscirbersSplitter">
			<resultlist>
				<result value="0" node="SingleSubscirverRequestDequeuer"/>
			</resultlist>
		</node>
		<node name="SingleSubscirverRequestDequeuer" object="Dequeuer">
			<resultlist>
				<result value="0" node="DataBaseProcessorSELECTIP"/>
				<result value="1" node=""/>
			</resultlist>
			<parameterslist>
				<param name="queueName" value="requestsSplittedBySubscriberQueue"/>
			</parameterslist>
		</node>
                <node name="DataBaseProcessorSELECTIP" object="DataBaseProcessor">
			<resultlist>
				<result value="0" node="ExtractIP"/>
				<result value="" node="ResponseAssembler"/>
			</resultlist>
			<parameterslist>
				<param name="DataBaseName" value="FORIS.SPA.ARCHIVE"/>
				<param name="FunctionName" value="SELECT ip FROM static_ip WHERE MSISDN = '{0}'"/>
				<param name="FunctionType" value="customQuery"/>
				<param name="OutXml" value="db_unit_result"/>
				<param name="DbParameterName:0" value="p_msisdn"/>
				<param name="DbParameterDirection:0" value="Input"/>
				<param name="DbParameterType:0" value="Varchar2"/>
				<param name="DbParameterXpathValue:0" value="ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN" />
			</parameterslist>
		</node>
                <node name="ExtractIP" object="XmlNodesCopier">
			<resultlist>
				<result value="" node="AddIP"/>
			</resultlist>
			<parameterslist>
				<param name="SrcXml" value="db_unit_result"/>
				<param name="DstMessage" value="xmlrequest"/>
				<param name="CopyContentType" value="InnerText"/>
				<param name="SrcXmlPath:0" value="root/return/record/column[@name='IP']"/>
				<param name="DstXmlPath:0" value="ExtendedRequestMessage/AdditionalData/IP"/>
                	</parameterslist>
		</node>
                <node name="AddIP" object="Commander">
			<resultlist>
				<result value="" node="dummySUCCESS"/>
			</resultlist>
			<parameterslist>
				<param name="CommandSource" value="Dictionary"/>
				<param name="Command" value="AddIP"/>
				<param name="Script" value="Command"/>
				<param name="AppendSubscribersData" value="false"/>
				<param name="ErrorDecodingType" value="ByRegexp"/>
				<param name="DynamicObject" value="(empty)"/>
				<param name="OverwriteDetailedResult" value="true"/>
			</parameterslist>
		</node>
		<node name="dummySUCCESS" object="DummyResponder">
			<resultlist>
				<result value="" node="ResponseAssembler"/>
			</resultlist>
			<parameterslist>
				<param name="OverallResult" value="SUCCESS"/>
				<param name="DetailedResult" value="SUCCESS"/>
				<param name="Delay" value="0"/>
			</parameterslist>
		</node>
		<node name="ResponseAssembler" object="ResponseAssembler">
			<resultlist>
				<result value="" node="SingleSubscirverRequestDequeuer"/>
			</resultlist>
		</node>
	</automat>
</scenario>