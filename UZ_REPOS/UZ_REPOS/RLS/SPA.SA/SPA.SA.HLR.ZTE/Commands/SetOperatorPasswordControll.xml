<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SetOperatorPasswordControll" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
	<Description>'Установка контроля пароля оператором (абонентская)'</Description>
	<Author Name="Timur Khamzin" Date="2015-11-16+15:00"/>
	<Request Template='Mod SS:MSISDN={0},CBCtrl=0,CB_PWD=0000;^M'>
		<Item Name="msisdn" Type="Wellknown"/>
	</Request>
	<ErrorsList CodePage="windows-1251">
		<Error Name="110109" Value="SUCCESS"/> <!--Subscriber does not exist-->
	</ErrorsList>
</Command>
