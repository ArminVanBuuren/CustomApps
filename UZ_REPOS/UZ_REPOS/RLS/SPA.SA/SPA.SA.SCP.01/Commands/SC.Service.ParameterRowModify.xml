      <Source Path="//AdditionalData/ModifyList/RFS_FP_SERVICE_GENERAL/Parameters/*[1]" SelectMode="NodeName" AbsenceValue="" />
      <Source Path="//AdditionalData/AddList/RFS_FP_SERVICE_GENERAL/Parameters/*[1]" SelectMode="NodeName" AbsenceValue="" />
      <Source Path="//AdditionalData/RemoveList/RFS_FP_SERVICE_GENERAL/Parameters/*[1]" SelectMode="NodeName" AbsenceValue="" />
    </Parameter>
    <Parameter Name="ParamValue">
      <Source Path="//AdditionalData/ModifyList/RFS_FP_SERVICE_GENERAL/Parameters/*[1]/@oldValue[string-length(.) > 0]" AbsenceValue="" />
      <Source Path="//AdditionalData/AddList/RFS_FP_SERVICE_GENERAL/Parameters/*[1]/@oldValue[string-length(.) > 0]" AbsenceValue="" />
      <Source Path="//AdditionalData/RemoveList/RFS_FP_SERVICE_GENERAL/Parameters/*[1]/@oldValue[string-length(.) > 0]" AbsenceValue="" />
    </Parameter>
    <Parameter Name="NewParamValue">
      <Source Path="//AdditionalData/ModifyList/RFS_FP_SERVICE_GENERAL/Parameters/*[1]/@value[string-length(.) > 0]" AbsenceValue="" PostFormat="NEW_PARAM_VALUE=&quot;{0}&quot;" />
      <Source Path="//AdditionalData/AddList/RFS_FP_SERVICE_GENERAL/Parameters/*[1]/@value[string-length(.) > 0]" AbsenceValue="" PostFormat="NEW_PARAM_VALUE=&quot;{0}&quot;" />
      <Source Path="//AdditionalData/RemoveList/RFS_FP_SERVICE_GENERAL/Parameters/*[1]/@value[string-length(.) > 0]" AbsenceValue="" PostFormat="NEW_PARAM_VALUE=&quot;{0}&quot;" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="ascii"/>
</Command>
