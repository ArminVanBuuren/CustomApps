<Configuration>
  <Settings>
    <Add Key="ReportTimeout" Value="6000" />
    <Add Key="Requests.MaxQueuedPerHost" Value="500" />
    <Add Key="Requests.PollPeriod" Value="5" />
    <Add Key="Requests.PollIdlePeriod" Value="2" />
    <Add Key="ProfilerConfiguration" Value="Profiler.xml" />
    <Add Key="FORIS.RI" Value="BEB" />
  </Settings>
  <serviceInstances>
    <serviceInstance hardwareID="HL:16">
      <dictionary>..\SPA.SA\HLR\Dictionary.xml</dictionary>
      <scenarios>..\SPA.SA\HLR\Scenarios</scenarios>
      <repetitionTable>..\SPA.SA\HLR\Misc\RepetitionTable.xml</repetitionTable>
      <socketList poolSize="20" maxConnections="20" minConnections="1" averageTime="1000" deltaAverageTime="4000" maxCommandsPerPeriod="500" periodDuration="20" autoconnect="1" reconfigurationTimeout="0">
        <socket type="FORIS.ServiceProvisioning.Communication.Script.ManagedScriptSocket, FORIS.SPA.Communication">
          <config workingmode="wmGeneral">
            <socket type="FORIS.ServiceProvisioning.Communication.TelnetSocket, FORIS.SPA.Communication">
              <config host="172.24.67.145" port="7776" />
            </socket>
            <Emulation Success="SUCCESS0001" Failure="ERR1017" RandomizePrintout="false" OverwriteCommandPrintout="true" Lifetime="00:00:00.00" />
            <Timers IdleTimeout="27" />
            <PredefinedScripts Login="Login" Command="Command" Idle="NoSleep" Logoff="Logoff" />
            <ScriptsList>
              <Script Name="Login" Path="..\SPA.SA\HLR\Scripts\Login.xml" />
              <Script Name="Logoff" Path="..\SPA.SA\HLR\Scripts\Logoff.xml" />
              <Script Name="Command" Path="..\SPA.SA\HLR\Scripts\Command.xml" />
              <Script Name="NoSleep" Path="..\SPA.SA\HLR\Scripts\nosleep.xml" />
            </ScriptsList>
          </config>
        </socket>
      </socketList>
    </serviceInstance>
  </serviceInstances>
</Configuration>