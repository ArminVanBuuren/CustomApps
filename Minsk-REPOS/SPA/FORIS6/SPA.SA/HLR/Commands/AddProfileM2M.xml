<Command Name="AddProfileM2M" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
  <Description>&gt;Добавление GPRS профиль M2M</Description>
  <Author Name="YAI" Date="2012-07-19+12:18" />
  <Request Template="MOD OPTGPRS: ISDN=&quot;{0}&quot;, PROV=ADDPDPCNTX, APN_TYPE=PS_APN, APNTPLID=16, QOSTPLID=1, PDPTYPE=IPV4, ADDIND=DYNAMIC, VPLMN=FALSE, CHARGE=NORMAL;">
    <Item Name="msisdn" Type="Wellknown" />
  </Request>
  <ErrorsList CodePage="ASCII">
    <Error Name="ERR3197" Value="SUCCESS" />
  </ErrorsList>
</Command>