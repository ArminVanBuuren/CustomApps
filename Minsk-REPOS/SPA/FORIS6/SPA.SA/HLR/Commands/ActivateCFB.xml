  <Author Name="YAI" Date="2011-10-04+12:18" />
  <Request Template="REG CFB: ISDN=&quot;{0}&quot;, FTN=&quot;{1}&quot;, PROPERTY=INTERNATIONAL, BSG=TS1X;">
