<Dictionary>
  <WellknownsList>
    <Wellknown Name="msisdn" Value="xmlsubstr(&quot;ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN&quot;)" />
  </WellknownsList>
  <ErrorsList CodePage="ASCII">
    <Error Name="[STATUS] =&gt; 0" Value="SUCCESS" />
    <Error Name="[STATUS] =&gt; 1" Value="FAILURE" />
    <Error Name="[STATUS] =&gt; 2" Value="FAILURE" />
    <Error Name="[STATUS] =&gt; 3" Value="NoSuchObjectInstance" />
    <Error Name="[STATUS] =&gt; -1" Value="FAILURE" />
  </ErrorsList>
  <RegexpsList />
  <ParametersList>
    <Parameter Name="NewMSISDN">
      <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList//NewMSISDN" />
      <PossibleValues StringToKeyTable="true" />
    </Parameter>
    <Parameter Name="NewIMSI">
      <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList//NewIMSI" />
      <PossibleValues StringToKeyTable="true" />
    </Parameter>
    <Parameter Name="MACADDR2">
      <Source Path="ExtendedRequestMessage/AdditionalData/MACADDR" />
      <PossibleValues StringToKeyTable="true" />
    </Parameter>
  </ParametersList>
  <DynamicParametersList>
    <DynamicParameter Name="MACADDR">
      <Source Path="root/return/record/column[@name='MACADDRFTTX']" />
      <DynamicObject Name="db_get_macaddr" />
    </DynamicParameter>
  </DynamicParametersList>
  <CommandsList Root="C:\FORIS\SPA\SPA.SA\ITV\Commands" Mask="*.xml">
    <!-- All new commands are included by Mask=*.xml attribute -->
  </CommandsList>
</Dictionary>