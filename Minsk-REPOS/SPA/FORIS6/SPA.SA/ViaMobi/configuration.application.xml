<Configuration>
  <Settings>
    <Add Key="ReportTimeout" Value="180" />
    <Add Key="ProfilerConfiguration" Value="Profiler.xml" />
    <Add Key="Requests.MaxQueuedPerHost" Value="500" />
    <Add Key="Requests.PollPeriod" Value="5" />
    <Add Key="Requests.PollIdlePeriod" Value="2" />
    <Add Key="FORIS.RI" Value="BEB" />
  </Settings>
  <serviceInstances>
    <serviceInstance hardwareID="VI:1838">
      <dictionary>..\SPA.SA\ViaMobi\Dictionary.xml</dictionary>
      <scenarios>..\SPA.SA\ViaMobi\Scenarios</scenarios>
      <repetitionTable>..\SPA.SA\ViaMobi\Misc\RepetitionTable.xml</repetitionTable>
      <socketList poolSize="10" maxConnections="10" minConnections="1" averageTime="1000" deltaAverageTime="4000" maxCommandsPerPeriod="500" periodDuration="20" autoconnect="1" reconfigurationTimeout="0">
        <socket type="FORIS.ServiceProvisioning.Communication.Script.ManagedScriptSocket, FORIS.SPA.Communication">
          <config workingmode="wmPermanentEmulation">
            <socket type="FORIS.ServiceProvisioning.Communication.HttpSocket, FORIS.SPA.Communication">
              <config>
                <ParametersList>
                  <!--Parameter Key="Host" Value="10.128.213.41"/-->
                  <Parameter Key="Host" Value="127.0.0.1" />
                  <Parameter Key="Port" Value="80" />
                  <Parameter Key="Url" Value="viamobi_mts/" />
                  <Parameter Key="MethodName" Value="GET" />
                  <Parameter Key="Method" Value="GET" />
                  <Parameter Key="UserName" Value="" />
                  <Parameter Key="UserPwd" Value="" />
                  <Parameter Key="ContentType" Value="text/plain; charset=utf-8" />
                  <Parameter Key="Authorization" Value="Basic" />
                  <Parameter Key="ParameterName" Value="" />
                </ParametersList>
              </config>
            </socket>
            <Emulation Success="&quot;status&quot;:&quot;0&quot;" Failure="500" RandomizePrintout="false" OverwriteCommandPrintout="true" Lifetime="00:00:00.00" />
            <Timers IdleTimeout="6000" />
            <PredefinedScripts Login="Login" Command="Command" Idle="" Logoff="" />
            <ScriptsList>
              <Script Name="Login" Path="..\SPA.SA\ViaMobi\Scripts\Login.xml" />
              <Script Name="Logoff" Path="..\SPA.SA\ViaMobi\Scripts\logoff.xml" />
              <Script Name="Command" Path="..\SPA.SA\ViaMobi\Scripts\Command.xml" />
              <Script Name="NoSleep" Path="..\SPA.SA\ViaMobi\Scripts\nosleep.xml" />
            </ScriptsList>
          </config>
        </socket>
      </socketList>
    </serviceInstance>
  </serviceInstances>
</Configuration>