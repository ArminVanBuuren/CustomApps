<Configuration>
  <Settings>
    <Add Key="ReportTimeout" Value="180" />
    <Add Key="Requests.PollPeriod" Value="5" />
    <Add Key="Requests.PollIdlePeriod" Value="2" />
    <Add Key="ProfilerConfiguration" Value="Profiler.xml" />
    <Add Key="FORIS.RI" Value="BEB" />
  </Settings>
  <serviceInstances>
    <serviceInstance hardwareID="RB:1616">
      <dictionary>..\SPA.SA\RBT\Dictionary.xml</dictionary>
      <scenarios>..\SPA.SA\RBT\Scenarios</scenarios>
      <repetitionTable>..\SPA.SA\RBT\Misc\RepetitionTable.xml</repetitionTable>
      <socketList poolSize="4" maxConnections="2" minConnections="1" averageTime="1000" deltaAverageTime="4000" maxCommandsPerPeriod="5" periodDuration="5" autoconnect="1" reconfigurationTimeout="0">
        <socket type="FORIS.ServiceProvisioning.Communication.Script.ManagedScriptSocket, FORIS.SPA.Communication">
          <config workingmode="wmPermanentEmulation">
            <socket type="FORIS.ServiceProvisioning.Communication.HttpSocket, FORIS.SPA.Communication">
              <config>
                <ParametersList>
                  <!--Parameter Key="Host" Value="172.17.134.1"/-->
                  <Parameter Key="Host" Value="127.0.0.1" />
                  <Parameter Key="Port" Value="10085" />
                  <Parameter Key="Url" Value="?OPERATION=" />
                  <Parameter Key="MethodName" Value="GET" />
                  <Parameter Key="Method" Value="GET" />
                  <Parameter Key="UserName" Value="" />
                  <Parameter Key="UserPwd" Value="" />
                  <Parameter Key="ContentType" Value="text/html" />
                  <Parameter Key="ParameterName" Value="" />
                </ParametersList>
              </config>
            </socket>
            <Emulation Success="RETURN-CODE&gt;000" Failure="RESULT:-1" RandomizePrintout="false" OverwriteCommandPrintout="true" Lifetime="00:00:00.00" />
            <Timers IdleTimeout="6000" />
            <PredefinedScripts Login="Login" Command="Command" Idle="" Logoff="" />
            <ScriptsList>
              <Script Name="Login" Path="..\SPA.SA\RBT\Scripts\Login.xml" />
              <Script Name="Logoff" Path="..\SPA.SA\RBT\Scripts\logoff.xml" />
              <Script Name="Command" Path="..\SPA.SA\RBT\Scripts\Command.xml" />
              <Script Name="NoSleep" Path="..\SPA.SA\RBT\Scripts\nosleep.xml" />
            </ScriptsList>
          </config>
        </socket>
      </socketList>
    </serviceInstance>
  </serviceInstances>
</Configuration>