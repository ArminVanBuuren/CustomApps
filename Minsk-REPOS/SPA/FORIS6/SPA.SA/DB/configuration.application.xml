<Configuration>
  <Settings>
    <Add Key="Requests.MaxQueuedPerHost" Value="500" />
    <Add Key="Requests.PollPeriod" Value="5" />
    <Add Key="Requests.PollIdlePeriod" Value="2" />
    <Add Key="ProfilerConfiguration" Value="Profiler.xml" />
    <Add Key="FORIS.RI" Value="BEB" />
  </Settings>
  <serviceInstances>
    <serviceInstance hardwareID="DB:2525">
      <dictionary>..\SPA.SA\DB\Dictionary.xml</dictionary>
      <scenarios>..\SPA.SA\DB\Scenarios</scenarios>
      <repetitionTable>..\SPA.SA\DB\Misc\RepetitionTable.xml</repetitionTable>
      <socketList poolSize="10" maxConnections="10" minConnections="1" averageTime="1000" deltaAverageTime="4000" maxCommandsPerPeriod="500" periodDuration="20" autoconnect="1" reconfigurationTimeout="0">
        <socket type="FORIS.ServiceProvisioning.Communication.Script.ManagedScriptSocket, FORIS.SPA.Communication">
          <config workingmode="wmPermanentEmulation">
            <socket type="FORIS.ServiceProvisioning.Communication.DatabaseSocket, FORIS.SPA.Communication">
              <config connectionData="FORIS.SPA.BILLING" responseTemplate="" />
            </socket>
            <Emulation Success="&lt;returned value=&quot;0&quot; /&gt;" Failure="no data found" RandomizePrintout="false" OverwriteCommandPrintout="true" Lifetime="00:00:00.00" />
            <Timers IdleTimeout="6000" />
            <PredefinedScripts Login="Login" Command="Command" Idle="" Logoff="" />
            <ScriptsList>
              <Script Name="Login" Path="..\SPA.SA\DB\Scripts\Login.xml" />
              <Script Name="Command" Path="..\SPA.SA\DB\Scripts\Command.xml" />
            </ScriptsList>
          </config>
        </socket>
      </socketList>
    </serviceInstance>
  </serviceInstances>
</Configuration>