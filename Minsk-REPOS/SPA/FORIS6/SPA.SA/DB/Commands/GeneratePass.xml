<Command Name="GeneratePass" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
  <Description>Проверка  и генерация пароля</Description>
  <Author Name="YAI" Date="2016-04-14+15:50" />
  <Properties GenerationType="Database" />
  <Request Template="function TTT_GenerateNewPassSubscriber({0}) return number">
    <Item Name="MSISDN" Type="LocalDatabaseParameter" />
  </Request>
  <DatabaseParametersList>
    <DatabaseParameter Name="MSISDN">
      <Properties DatabaseArgumentName="i_customer" DatabaseArgumentType="varchar2" />
      <Source Path="ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN" />
      <PossibleValues StringToKeyTable="true" />
    </DatabaseParameter>
  </DatabaseParametersList>
  <ErrorsList CodePage="ASCII">
    <Error Name="&lt;returned value=&quot;1&quot; /&gt;" Value="SUCCESS" />
    <Error Name="&lt;returned value=&quot;0&quot; /&gt;" Value="SUCCESS" />
    <Error Name="&lt;returned value=&quot;-1&quot; /&gt;" Value="FAILURE" />
  </ErrorsList>
</Command>