<Command Name="AddToningNEWDB" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
  <Description>ТОНИНГ + Мелодии дня - технологическая</Description>
  <Author Name="YAI" Date="2016-07-21+15:50" />
  <Properties GenerationType="Database" />
  <Request Template="function spa_func.f_spa_addservice({0},{1},{2}) return number">
    <Item Name="MSISDN" Type="LocalDatabaseParameter" />
    <Item Name="ServiceCode" Type="LocalDatabaseParameter" />
    <Item Name="SRC" Type="LocalDatabaseParameter" />
  </Request>
  <DatabaseParametersList>
    <DatabaseParameter Name="MSISDN">
      <Properties DatabaseArgumentName="i_msisdn" DatabaseArgumentType="varchar2" />
      <Source Path="ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN" />
      <PossibleValues StringToKeyTable="true" />
    </DatabaseParameter>
    <DatabaseParameter Name="ServiceCode">
      <Properties DatabaseArgumentName="i_servicecode" DatabaseArgumentType="varchar2" />
      <Source Path="dummy" AbsenceValue="FR12173" />
      <PossibleValues StringToKeyTable="true" />
    </DatabaseParameter>
    <DatabaseParameter Name="SRC">
      <Properties DatabaseArgumentName="src" DatabaseArgumentType="varchar2" />
      <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList//SourceTypeCode" />
      <PossibleValues StringToKeyTable="true" />
    </DatabaseParameter>
  </DatabaseParametersList>
  <ErrorsList CodePage="ASCII">
    <Error Name="&lt;returned value=&quot;0&quot; /&gt;" Value="SUCCESS" />
    <Error Name="&lt;returned value=&quot;320204&quot; /&gt;" Value="FAILURE" />
    <Error Name="&lt;returned value=&quot;320205&quot; /&gt;" Value="SUCCESS" />
    <Error Name="&lt;returned value=&quot;320206&quot; /&gt;" Value="FAILURE" />
  </ErrorsList>
</Command>