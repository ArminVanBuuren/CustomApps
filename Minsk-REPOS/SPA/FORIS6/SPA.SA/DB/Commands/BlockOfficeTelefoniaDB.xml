<Command Name="BlockOfficeTelefoniaDB" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
  <Description>Добавление блокировки </Description>
  <Author Name="YAI" Date="2012-06-22+15:50" />
  <Properties GenerationType="Database" />
  <Request Template="function spa_func.officetelefoniablockmss({0},{1}) return number">
    <Item Name="MSISDN" Type="LocalDatabaseParameter" />
    <Item Name="Operation" Type="LocalDatabaseParameter" />
  </Request>
  <DatabaseParametersList>
    <DatabaseParameter Name="MSISDN">
      <Properties DatabaseArgumentName="i_msisdn" DatabaseArgumentType="varchar2" />
      <Source Path="ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN" />
      <PossibleValues StringToKeyTable="true" />
    </DatabaseParameter>
    <DatabaseParameter Name="Operation">
      <Properties DatabaseArgumentName="i_operation" DatabaseArgumentType="varchar2" />
      <Source Path="dummy" AbsenceValue="Block" />
      <PossibleValues StringToKeyTable="true" />
    </DatabaseParameter>
  </DatabaseParametersList>
  <ErrorsList CodePage="ASCII">
    <Error Name="&lt;returned value=&quot;0&quot; /&gt;" Value="SUCCESS" />
    <Error Name="&lt;returned value=&quot;-1&quot; /&gt;" Value="FAILURE" />
    <Error Name="&lt;returned value=&quot;-2&quot; /&gt;" Value="FAILURE" />
    <Error Name="&lt;returned value=&quot;-10&quot; /&gt;" Value="FAILURE" />
  </ErrorsList>
</Command>