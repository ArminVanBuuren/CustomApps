      <Source Path="//AdditionalData/EndList//BonusCampaignId/Value[string-length(.)&gt;0]" />
    </Parameter>
    <Parameter Name="BasePenaltyPrice" IsChoice="true">
      <Source Path="ExtendedRequestMessage/AdditionalData/RegisteredList//AnnualContractBasePenaltyPrice" />
      <Source Path="//AdditionalData/EndList//AnnualContractBasePenaltyPrice/Value[string-length(.)&gt;0]" />
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="ascii" />
</Command>