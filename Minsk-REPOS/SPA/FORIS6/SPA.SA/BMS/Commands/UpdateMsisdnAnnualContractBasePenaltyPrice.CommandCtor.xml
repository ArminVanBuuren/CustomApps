<Command Name="UpdateMsisdnAnnualContractBasePenaltyPrice.CommandCtor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>Table constructor for UpdateMsisdnAnnualContractBasePenaltyPrice command</Description>
  <Author Name="Serg Kovalenko" Date="2012-05-25+14:08" />
  <ModifiedBy Name="Victor Kosonogov" Date="2017-07-07+07:07" />
  <Request>
    <RequestTemplate>
      <![CDATA[<Row CampaignId="##CampaignId##" BasePenaltyPrice="##BasePenaltyPrice##"/>]]>
