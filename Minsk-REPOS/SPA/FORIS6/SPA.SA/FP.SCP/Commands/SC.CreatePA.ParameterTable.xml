<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.CreatePA.ParameterTable">
	<Description>CreatePA (parameters table)</Description>
	<Author Name="Daniil Lukinov" Date="2019-09-09+01:01"/>
	<Properties GenerationType="MmlXml" />
	<Request>
    <RequestTemplatePrefix>
      <![CDATA[ 
      <DataTable Name="ServiceParametersDataTable">
        <Columns>
          <Column Name="SERVICE_ROW_ID" Type="System.Int32"/>
          <Column Name="PARAM_CODE" Type="System.String"/>
          <Column Name="PARAM_VALUE" Type="System.String"/>
        </Columns>
        <Rows>
      ]]>
    </RequestTemplatePrefix>
		<RequestTemplate>
			<![CDATA[<Row SERVICE_ROW_ID="##ServiceIdentifier##" PARAM_CODE="xmldecodename(##ParamCode##)" PARAM_VALUE="##ParamValue##" />]]>
		</RequestTemplate>
    <RequestTemplatePostfix>
      <![CDATA[ 
        </Rows>
      </DataTable>
      ]]>
    </RequestTemplatePostfix>
		<EmulationPrintoutTemplate>
			<![CDATA[]]>
		</EmulationPrintoutTemplate>
		<Item Name="ServiceIdentifier" Type="Wellknown"/>
		<Item Name="ParamCode" Type="LocalParameter"/>
		<Item Name="ParamValue" Type="LocalParameter"/>
	</Request>
	<ParametersList>
		<Parameter Name="ParamCode">
			<Source Path="//AdditionalData/EndList/RFS_FP_SERVICE_GENERAL/Parameters/*[1]" SelectMode="NodeName" />
		</Parameter>
		<Parameter Name="ParamValue">
			<Source Path="//AdditionalData/EndList/RFS_FP_SERVICE_GENERAL/Parameters/*[1]/@value[string-length(.) > 0]" />
		</Parameter>
	</ParametersList>
</Command>