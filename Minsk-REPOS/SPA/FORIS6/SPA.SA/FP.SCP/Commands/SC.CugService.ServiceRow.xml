<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.CugService.ServiceRow">
  <Description></Description>
  <Author Name="Daniil Lukinov" Date="2019-02-02+12:00"/>
  <Properties GenerationType="MmlXml" />
  <Request>
    <RequestTemplate>
      <![CDATA[<Row
SERVICE_ROW_ID="##ServiceIdentifier##"
REQUEST_ID="##ASYNC_REQUEST_ID##"
TERMINAL_DEVICE_CONTRACT_ID="##TerminalDeviceID##" 
PERSONAL_ACCOUNT_ID="##PersonalAccount##"
SERVICE_CODE="xmldecodename(##SERVICE_CODE##)" 
##DateFrom##
SERVICE_ACTION="##ServiceAction##"/>]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="TerminalDeviceID" Type="Wellknown"/>
    <Item Name="PersonalAccount" Type="Wellknown" />
    <Item Name="SERVICE_CODE" Type="LocalParameter"/>
    <Item Name="DateFrom" Type="LocalParameter"/>
    <Item Name="ServiceIdentifier" Type="Wellknown"/>
    <Item Name="ServiceAction" Type="LocalParameter"/>
  </Request>
  <ParametersList>
    <Parameter Name="SERVICE_CODE">
      <Source Path="//AdditionalData/*/RFS_FP_SERVICE_CUG/ServiceCode" AbsenceValue=""/>
      <Source Path="//AdditionalData/*/RFS_FP_SERVICE_VUG/ServiceCode" AbsenceValue=""/>
    </Parameter>
    <Parameter Name="DateFrom" IsChoice="true" AbsenceValue="">
      <Source Path="//AdditionalData/*/RFS_FP_SERVICE_CUG/DateFrom/Value[string-length(.) > 0]" PostFormat="DATE_FROM=&quot;{0}&quot;" />
      <Source Path="//AdditionalData/*/RFS_FP_SERVICE_VUG/DateFrom/Value[string-length(.) > 0]" PostFormat="DATE_FROM=&quot;{0}&quot;" />
    </Parameter>
    <Parameter Name="ServiceAction" IsChoice="true">
      <Source Path="//AdditionalData/AddList/RFS_FP_SERVICE_CUG" PostFormat="A"/>
      <Source Path="//AdditionalData/RemoveList/RFS_FP_SERVICE_CUG" PostFormat="D"/>
      <Source Path="//AdditionalData/ModifyList/RFS_FP_SERVICE_CUG" PostFormat="M"/>
      <Source Path="//AdditionalData/EndList/RFS_FP_SERVICE_CUG" PostFormat="A"/>
      <Source Path="//AdditionalData/AddList/RFS_FP_SERVICE_VUG" PostFormat="A"/>
      <Source Path="//AdditionalData/RemoveList/RFS_FP_SERVICE_VUG" PostFormat="D"/>
      <Source Path="//AdditionalData/ModifyList/RFS_FP_SERVICE_VUG" PostFormat="M"/>
      <Source Path="//AdditionalData/EndList/RFS_FP_SERVICE_VUG" PostFormat="A"/>
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="ascii"/>
</Command>
