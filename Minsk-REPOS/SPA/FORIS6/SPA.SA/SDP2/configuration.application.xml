<Configuration>
  <Settings>
    <Add Key="ReportTimeout" Value="6000" />
    <Add Key="Requests.MaxQueuedPerHost" Value="100" />
    <Add Key="Requests.PollPeriod" Value="5" />
    <Add Key="Requests.PollIdlePeriod" Value="2" />
    <Add Key="ProfilerConfiguration" Value="Profiler.xml" />
    <Add Key="FORIS.RI" Value="BEB" />
  </Settings>
  <serviceInstances>
    <serviceInstance hardwareID="SS:1819">
      <dictionary>..\SPA.SA\SDP2\Dictionary.xml</dictionary>
      <scenarios>..\SPA.SA\SDP2\Scenarios</scenarios>
      <repetitionTable>..\SPA.SA\SDP2\Misc\RepetitionTable.xml</repetitionTable>
      <socketList poolSize="5" maxConnections="5" minConnections="1" averageTime="1000" deltaAverageTime="4000" maxCommandsPerPeriod="100" periodDuration="10" autoconnect="1" reconfigurationTimeout="0">
        <socket type="FORIS.ServiceProvisioning.Communication.Script.ManagedScriptSocket, FORIS.SPA.Communication">
          <config workingmode="wmGeneral">
            <socket type="FORIS.ServiceProvisioning.Communication.TelnetSocket, FORIS.SPA.Communication">
              <config host="172.24.189.193" port="23" />
            </socket>
            <Emulation Success="Released" Failure="Command failed" RandomizePrintout="false" OverwriteCommandPrintout="true" Lifetime="00:00:00.00" />
            <Timers IdleTimeout="5000" />
            <PredefinedScripts Login="Login" Command="Command" Idle="NoSleep" Logoff="Logoff" />
            <ScriptsList>
              <Script Name="Login" Path="..\SPA.SA\SDP2\Scripts\Login.xml" />
              <Script Name="NoSleep" Path="..\SPA.SA\SDP2\Scripts\nosleep.xml" />
              <Script Name="Logoff" Path="..\SPA.SA\SDP2\Scripts\logoff.xml" />
              <Script Name="Command" Path="..\SPA.SA\SDP2\Scripts\Command.xml" />
            </ScriptsList>
          </config>
        </socket>
      </socketList>
    </serviceInstance>
  </serviceInstances>
</Configuration>