<Configuration>
  <Settings>
    <Add Key="ReportTimeout" Value="6000" />
    <Add Key="Requests.MaxQueuedPerHost" Value="300" />
    <Add Key="Requests.PollPeriod" Value="5" />
    <Add Key="Requests.PollIdlePeriod" Value="2" />
    <Add Key="ProfilerConfiguration" Value="Profiler.xml" />
    <Add Key="FORIS.RI" Value="BEB" />
  </Settings>
  <serviceInstances>
    <serviceInstance hardwareID="HL:100">
      <dictionary>..\SPA.SA\WLL\Dictionary.xml</dictionary>
      <scenarios>..\SPA.SA\WLL\Scenarios</scenarios>
      <repetitionTable>..\SPA.SA\WLL\Misc\RepetitionTable.xml</repetitionTable>
      <socketList poolSize="10" maxConnections="10" minConnections="1" averageTime="1000" deltaAverageTime="4000" maxCommandsPerPeriod="300" periodDuration="10" autoconnect="0" reconfigurationTimeout="0">
        <socket type="FORIS.ServiceProvisioning.Communication.Script.ManagedScriptSocket, FORIS.SPA.Communication">
          <config workingmode="wmPermanentEmulation">
            <socket type="FORIS.ServiceProvisioning.Communication.TelnetSocket, FORIS.SPA.Communication">
              <!--config host="172.24.67.145" port="7776"/-->
              <config host="127.0.0.1" port="7776" />
            </socket>
            <Emulation Success="SUCCESS0001" Failure="ERR1017" RandomizePrintout="false" OverwriteCommandPrintout="true" Lifetime="00:00:00.00" />
            <Timers IdleTimeout="20" />
            <PredefinedScripts Login="Login" Command="Command" Idle="NoSleep" Logoff="Logoff" />
            <ScriptsList>
              <Script Name="Login" Path="..\SPA.SA\WLL\Scripts\Login.xml" />
              <Script Name="Logoff" Path="..\SPA.SA\WLL\Scripts\Logoff.xml" />
              <Script Name="Command" Path="..\SPA.SA\WLL\Scripts\Command.xml" />
              <Script Name="NoSleep" Path="..\SPA.SA\WLL\Scripts\nosleep.xml" />
            </ScriptsList>
          </config>
        </socket>
      </socketList>
    </serviceInstance>
  </serviceInstances>
</Configuration>