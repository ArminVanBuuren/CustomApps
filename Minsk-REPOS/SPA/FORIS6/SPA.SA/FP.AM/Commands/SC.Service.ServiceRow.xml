<?xml version="1.0" encoding="UTF-8"?>
<Command Name="SC.Service.ServiceRow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>AssignService (services table)</Description>
  <Author Name="Victor Kosonogov" Date="2017-05-01+01:01"/>
  <Properties GenerationType="MmlXml" />
  <Request>
    <RequestTemplate>
      <![CDATA[<Row
SERVICE_ROW_ID="##ServiceIdentifier##"
REQUEST_ID="##ASYNC_REQUEST_ID##"
TERMINAL_DEVICE_CONTRACT_ID="##TerminalDeviceID##" 
SERVICE_CODE="xmldecodename(##Service##)" 
##DateFrom##
SERVICE_ACTION="##ServiceAction##"/>]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="TerminalDeviceID" Type="Wellknown"/>
    <Item Name="Service" Type="LocalParameter"/>
    <Item Name="DateFrom" Type="LocalParameter"/>
    <Item Name="ServiceIdentifier" Type="Wellknown"/>
    <Item Name="ServiceAction" Type="LocalParameter"/>
  </Request>
  <ParametersList>
    <Parameter Name="Service" IsChoice="true">
      <Source Path="//AdditionalData//RFS_FP_SERVICE_GENERAL/ServiceCode/Value" />
      <Source Path="//AdditionalData//RFS_FP_SERVICE_FAVORITE_NUMBER/ServiceCode/Value" />
    </Parameter>
    <Parameter Name="DateFrom" IsChoice="true" AbsenceValue="">
      <Source Path="//AdditionalData//RFS_FP_SERVICE_GENERAL/DateFrom/Value[string-length(.) > 0]" PostFormat="DATE_FROM=&quot;{0}&quot;"/>
      <Source Path="//AdditionalData//RFS_FP_SERVICE_FAVORITE_NUMBER/DateFrom/Value[string-length(.) > 0]" PostFormat="DATE_FROM=&quot;{0}&quot;"/>
    </Parameter>
    <Parameter Name="ServiceAction" IsChoice="true">
      <Source Path="//AdditionalData/AddList/RFS_FP_SERVICE_GENERAL" PostFormat="A"/>
      <Source Path="//AdditionalData/RemoveList/RFS_FP_SERVICE_GENERAL" PostFormat="D"/>
      <Source Path="//AdditionalData/ModifyList/RFS_FP_SERVICE_GENERAL" PostFormat="M"/>
      <Source Path="//AdditionalData/EndList/RFS_FP_SERVICE_GENERAL" PostFormat="M"/>
      <Source Path="//AdditionalData/AddList/RFS_FP_SERVICE_FAVORITE_NUMBER" PostFormat="A"/>
      <Source Path="//AdditionalData/RemoveList/RFS_FP_SERVICE_FAVORITE_NUMBER" PostFormat="D"/>
      <Source Path="//AdditionalData/ModifyList/RFS_FP_SERVICE_FAVORITE_NUMBER" PostFormat="M"/>
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="ascii"/>
</Command>
