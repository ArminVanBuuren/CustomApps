<Command Name="AddExtra" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
  <Description>Добавление SMSExtra </Description>
  <Author Name="YAI" Date="2016-01-29+12:50" />
  <Properties GenerationType="Database" />
  <Request Template="function spa_func.f_manage_service_smsextra({0},{1},{2}) return number">
    <Item Name="MSISDN" Type="DatabaseParameter" />
    <Item Name="i_service" Type="LocalDatabaseParameter" />
    <Item Name="i_action" Type="LocalDatabaseParameter" />
  </Request>
  <DatabaseParametersList>
    <DatabaseParameter Name="i_service">
      <Properties DatabaseArgumentName="i_service" DatabaseArgumentType="varchar2" />
      <Source Path="dummy" AbsenceValue="BSE" />
      <PossibleValues StringToKeyTable="true" />
    </DatabaseParameter>
    <DatabaseParameter Name="i_action">
      <Properties DatabaseArgumentName="i_action" DatabaseArgumentType="number" />
      <Source Path="dummy" AbsenceValue="1" />
      <PossibleValues StringToKeyTable="true" />
    </DatabaseParameter>
  </DatabaseParametersList>
  <ErrorsList CodePage="ASCII">
    <Error Name="&lt;returned value=&quot;20006&quot; /&gt;" Value="SUCCESS" />
  </ErrorsList>
</Command>