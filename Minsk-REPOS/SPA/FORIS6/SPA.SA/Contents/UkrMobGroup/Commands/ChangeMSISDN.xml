<Command Name="ChangeMSISDN" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
  <Description>Замена номера</Description>
  <Author Name="YAI" Date="2017-07-21+10:21" />
  <Request Template="server_obr_mts/api.php?mobile={0}&amp;newmobile={1}&amp;action=2">
    <Item Name="msisdn" Type="Wellknown" />
    <Item Name="NewMSISDN" Type="Parameter" />
  </Request>
  <ErrorsList CodePage="ASCII">
