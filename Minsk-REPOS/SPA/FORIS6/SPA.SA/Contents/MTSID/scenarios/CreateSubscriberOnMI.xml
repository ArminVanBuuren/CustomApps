    <object name="Commander" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.Processors.RequestProcessor" assembly="FORIS.SPA.MmlProcessing.dll" />
    <object name="ResponseAssembler" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.ResponseAssembler" assembly="FORIS.SPA.MmlProcessing.dll" />
    <object name="IfCommander" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.Processors.RequestProcessorConditional" assembly="FORIS.SPA.MmlProcessing.dll" />
    <object name="IfConditionXmlResolver" class="FORIS.ScenarioProcessing.CommonUnits.IfConditionXmlResolver" assembly="FORIS.SPA.SPF.dll" />
    <object name="IfMultipleXmlConditionsResolver" class="FORIS.ScenarioProcessing.CommonUnits.IfMultipleXmlConditionsResolver" assembly="FORIS.SPA.SPF.dll" />
    <object name="Switch" class="FORIS.ScenarioProcessing.CommonUnits.ParametersSwitch" assembly="FORIS.SPA.SPF.dll" />
    <object name="DummyResponder" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.DummyResponder" assembly="FORIS.SPA.MmlProcessing.dll" />
    <object name="DataBaseProcessor" class="FORIS.ScenarioProcessing.CommonUnits.DataBaseProcessor" assembly="FORIS.SPA.SPF.dll" />
    <object name="XmlNodesCopier" class="FORIS.ScenarioProcessing.CommonUnits.XmlNodesCopier" assembly="FORIS.SPA.SPF.dll" />
  </objectList>
  <automat>
    <node name="BySubscribersSplitter" object="BySubscribersSplitter">
      <resultlist>
        <result value="0" node="BySubscriberRequestDequeuer" />
      </resultlist>
      <parameterslist>
        <param name="queueName" value="requestsSplittedBySubscriberQueue" />
      </parameterslist>
    </node>
    <node name="BySubscriberRequestDequeuer" object="RequestDequeuer">
      <resultlist>
        <result value="0" node="GetTDI_Node" />
        <result value="1" node="" />
      </resultlist>
      <parameterslist>
        <param name="queueName" value="requestsSplittedBySubscriberQueue" />
      </parameterslist>
    </node>
    <node name="GetTDI_Node" object="DataBaseProcessor">
      <resultlist>
        <result value="" node="ExtractTDI" />
      </resultlist>
      <parameterslist>
        <param name="DataBaseName" value="FORIS.SPA.BILLING" />
        <param name="FunctionName" value="select  nvl(max(t.terminal_device_id),0) as TDI from cust.terminal_device t
													where t.msisdn = {0}
														and t.date_to is null" />
        <param name="FunctionType" value="customQuery" />
        <param name="OutXml" value="db_get_tdi" />
        <param name="DbParameterName:0" value="i_customer" />
        <param name="DbParameterDirection:0" value="Input" />
        <param name="DbParameterType:0" value="NVarchar2" />
        <param name="DbParameterXpathValue:0" value="ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN" />
      </parameterslist>
    </node>
    <node name="ExtractTDI" object="XmlNodesCopier">
      <resultlist>
        <result value="" node="IFTDINode" />
      </resultlist>
      <parameterslist>
        <param name="SrcXml" value="db_get_tdi" />
        <param name="DstMessage" value="xmlrequest" />
        <param name="CopyContentType" value="InnerText" />
        <param name="SrcXmlPath:0" value="root/return/record/column[@name='TDI']" />
        <param name="DstXmlPath:0" value="ExtendedRequestMessage/AdditionalData/TdId" />
      </parameterslist>
    </node>
    <node name="IFTDINode" object="Switch">
      <resultlist>
        <result value="1" node="GetUNP_Node" />
        <result value="2" node="DummyResponderErrNode" />
      </resultlist>
      <parameterslist>
        <param name="XmlPath" value="ExtendedRequestMessage/AdditionalData/TdId" />
        <param name="Value:2" value="0" />
      </parameterslist>
    </node>
    <node name="GetUNP_Node" object="DataBaseProcessor">
      <resultlist>
        <result value="" node="ExtractUNP" />
      </resultlist>
      <parameterslist>
        <param name="DataBaseName" value="FORIS.SPA.BILLING" />
        <param name="FunctionName" value="select /*+ ordered use_nl(td,ptd,cpa,cc,c) */
 nvl(max(c.inn),0) as UNP
  from cust.terminal_device td,
  cust.personal_account_td ptd,
  cust.contract_personal_account cpa,
  cust.customer_contract cc,
  cust.customer c
   where td.msisdn= {0}
       and ptd.terminal_device_id=td.terminal_device_id
       and ptd.personal_account_id=cpa.personal_account_id
       and cpa.contract_id=cc.contract_id
       and cc.customer_id=c.customer_id
       and c.customer_type_id = 2
       and sysdate between td.date_from and nvl(td.date_to,sysdate)
       and sysdate between ptd.date_from and nvl(ptd.date_to,sysdate)
       and sysdate between cpa.date_from and nvl(cpa.date_to,sysdate)
       and sysdate between cc.date_from and nvl(cc.date_to,sysdate)
       and sysdate between c.date_from and nvl(c.date_to,sysdate)
       " />
        <param name="FunctionType" value="customQuery" />
        <param name="OutXml" value="db_get_unp" />
        <param name="DbParameterName:0" value="i_customer" />
        <param name="DbParameterDirection:0" value="Input" />
        <param name="DbParameterType:0" value="NVarchar2" />
        <param name="DbParameterXpathValue:0" value="ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN" />
      </parameterslist>
    </node>
    <node name="ExtractUNP" object="XmlNodesCopier">
      <resultlist>
        <result value="" node="AddFR15695Node" />
      </resultlist>
      <parameterslist>
        <param name="SrcXml" value="db_get_unp" />
        <param name="DstMessage" value="xmlrequest" />
        <param name="CopyContentType" value="InnerText" />
        <param name="SrcXmlPath:0" value="root/return/record/column[@name='UNP']" />
        <param name="DstXmlPath:0" value="ExtendedRequestMessage/AdditionalData/UNP_Id" />
      </parameterslist>
    </node>
    <node name="IFUNPNode" object="Switch">
      <resultlist>
        <result value="1" node="AddFR15695Node" />
        <result value="2" node="DummyResponderErrNode" />
      </resultlist>
      <parameterslist>
        <param name="XmlPath" value="ExtendedRequestMessage/AdditionalData/UNP_Id" />
        <param name="Value:2" value="0" />
      </parameterslist>
    </node>
    <node name="AddFR15695Node" object="IfCommander">
      <resultlist>
        <result value="0" node="AddFR15696Node" />
        <result value="1538" node="AddFR15696Node" />
        <result value="" node="ResponseAssembler" />
      </resultlist>
      <parameterslist>
        <param name="IfXmlPath" value="ExtendedRequestMessage/AdditionalData/RegisteredList/FR15695" />
        <param name="CommandSource" value="Dictionary" />
        <param name="AppendSubscribersData" value="false" />
        <param name="Command" value="AddServiceGetDoc" />
        <param name="Script" value="Command" />
        <param name="ErrorDecodingType" value="ByString" />
      </parameterslist>
    </node>
    <node name="AddFR15696Node" object="IfCommander">
      <resultlist>
        <result value="0" node="AddFR15697Node" />
        <result value="1538" node="AddFR15697Node" />
        <result value="" node="ResponseAssembler" />
      </resultlist>
      <parameterslist>
        <param name="IfXmlPath" value="ExtendedRequestMessage/AdditionalData/RegisteredList/FR15696" />
        <param name="CommandSource" value="Dictionary" />
        <param name="AppendSubscribersData" value="false" />
        <param name="Command" value="AddServiceGetDocUSBkey" />
        <param name="Script" value="Command" />
        <param name="ErrorDecodingType" value="ByString" />
      </parameterslist>
    </node>
    <node name="AddFR15697Node" object="IfCommander">
      <resultlist>
        <result value="" node="ResponseAssembler" />
      </resultlist>
      <parameterslist>
        <param name="IfXmlPath" value="ExtendedRequestMessage/AdditionalData/RegisteredList/FR15697" />
        <param name="CommandSource" value="Dictionary" />
        <param name="AppendSubscribersData" value="false" />
        <param name="Command" value="AddServiceSendDoc" />
        <param name="Script" value="Command" />
        <param name="ErrorDecodingType" value="ByString" />
      </parameterslist>
    </node>
    <node name="DummyResponderNode" object="DummyResponder">
      <resultlist>
        <result value="" node="ResponseAssembler" />
      </resultlist>
      <parameterslist>
        <param name="OverallResult" value="SUCCESS" />
        <param name="DetailedResult" value="SUCCESS" />
        <param name="Delay" value="0" />
      </parameterslist>
    </node>
    <node name="DummyResponderErrNode" object="DummyResponder">
      <resultlist>
        <result value="" node="ResponseAssembler" />
      </resultlist>
      <parameterslist>
        <param name="OverallResult" value="FAILURE" />
        <param name="DetailedResult" value="InvalidParameterValue" />
      </parameterslist>
    </node>
    <node name="ResponseAssembler" object="ResponseAssembler">
      <resultlist>
        <result value="" node="BySubscriberRequestDequeuer" />
      </resultlist>
    </node>
  </automat>
</scenario>