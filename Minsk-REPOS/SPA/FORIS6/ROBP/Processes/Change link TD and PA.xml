<BusinessProcessData name="Change link TD and PA">
  <businessprocess>
    <scenario>
      <objectlist>
        <object name="ImplicitHostRedirectingProcessor" class="FORIS.ServiceProvisioning.BPM.ImplicitHostRedirectingProcessor" assembly="FORIS.SPA.BPMProcessingUnit.dll" />
        <object name="BPMProcessingUnit" class="FORIS.ServiceProvisioning.BPM.BPMProcessingUnit" assembly="FORIS.SPA.BPMProcessingUnit.dll" />
        <object name="ChangeTPOnESPPPlatform" class="FORIS.ServiceProvisioning.BPM.BPMProcessingUnit" assembly="FORIS.SPA.BPMProcessingUnit.dll" />
        <object name="BPMProcessingUnitConditional" class="FORIS.ServiceProvisioning.BPM.BPMProcessingUnitConditional" assembly="FORIS.SPA.BPMProcessingUnit.dll" />
        <object name="SCProcessingUnit" class="FORIS.ServiceProvisioning.BPM.SCProcessingUnit" assembly="FORIS.SPA.BPMProcessingUnit.dll" />
      </objectlist>
      <automat>
        <node name="SCProcessingUnit" object="SCProcessingUnit">
          <parameterslist />
          <resultlist>
            <result value="" node="DeleteTDPAonFP" />
          </resultlist>
        </node>
        <node name="DeleteTDPAonFP" object="BPMProcessingUnitConditional">
          <parameterslist>
            <param name="operation" value="DeleteTDPAonFP_Migration" />
            <param name="SrcXml" value="$XmlRequest" />
            <param name="MultiLevelCondition" value="(//NewPlatform/HostCode) AND (//NewPlatform/HostCode != //PreviousPlatform/HostCode)" />
          </parameterslist>
          <resultlist>
            <result value="" node="ChangeTDPAOnFP" />
          </resultlist>
        </node>
        <node name="ChangeTDPAOnFP" object="ImplicitHostRedirectingProcessor">
          <parameterslist>
            <param name="operation" value="ChangeTDPAOnFP" />
            <param name="SrcXml" value="$XmlRequest" />
            <param name="CodeTemplate" value="FP:{//NewPlatform/HostCode}" />
            <param name="MultiLevelCondition" value="(//NewPlatform/HostCode) AND (//NewPlatform/HostCode != //PreviousPlatform/HostCode)" />
          </parameterslist>
          <resultlist>
            <result value="" node="ChangeTDPAOnFP2" />
          </resultlist>
        </node>
        <node name="ChangeTDPAOnFP2" object="BPMProcessingUnitConditional">
          <parameterslist>
            <param name="operation" value="ChangeTDPAOnFP" />
            <param name="SrcXml" value="$XmlRequest" />
            <param name="MultiLevelCondition" value="(NOT //NewPlatform/HostCode) OR (//NewPlatform/HostCode == //PreviousPlatform/HostCode)" />
          </parameterslist>
          <resultlist>
            <result value="" node="ChangeTDPAOnBMS" />
          </resultlist>
        </node>
        <!--node name="RestoreSessionHL" object="BPMProcessingUnit">
          <parameterslist>
            <param name="operation" value="RestoreSessionHL" />
          </parameterslist>
          <resultlist>
            <result value="" node="ChangeTDPAOnBMS" />
          </resultlist>
        </node-->
        <node name="ChangeTDPAOnBMS" object="BPMProcessingUnit">
          <parameterslist>
            <param name="operation" value="ChangeTDPAOnBMS" />
          </parameterslist>
          <resultlist>
            <result value="" node="" />
          </resultlist>
        </node>
      </automat>
    </scenario>
  </businessprocess>
</BusinessProcessData>