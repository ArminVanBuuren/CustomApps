				<result value="2" node="CreateContractSubscriberWithoutProfileNode"/>
			</resultlist>
			<parameterslist>
				<param name="XmlPath" value="ExtendedRequestMessage/AdditionalData/Profile"/>
				<param name="Value:2" value="All"/>
				<param name="Value:3" value="Internet"/>
				<param name="Value:4" value="WAP"/>
				<param name="Value:5" value="MMS"/>
			</parameterslist>
		</node>			
		<node name="InternetNode" object="Commander">
			<resultlist>
				<result value="" node="ResponseAssembler"/>
			</resultlist>
			<parameterslist>
				<param name="Command" value="SendSettingsInternet"/>
				<param name="Script" value="Command"/>
				<param name="ErrorDecodingType" value="ByString"/>
				<param name="CommandSource" value="Dictionary"/>
				<param name="DynamicObject" value="(empty)"/>
			</parameterslist>
		</node>
		<node name="ResponseAssembler" object="ResponseAssembler">
			<resultlist>
				<result value="" node="BySubscriberRequestDequeuer"/>
			</resultlist>
		</node>
	</automat>
</scenario>
