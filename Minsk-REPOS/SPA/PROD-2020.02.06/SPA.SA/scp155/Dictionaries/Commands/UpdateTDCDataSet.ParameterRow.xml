<?xml version="1.0" encoding="UTF-8"?>
<Command Name="UpdateTDCDataSet.ParameterRow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>Update list of services and blocks (services table)</Description>
  <Author Name="Oleg Safonov" Date="2011-02-08+12:35"/>
  <Properties GenerationType="MmlXml" />
  <Request>
    <RequestTemplate>
      <![CDATA[<Row 
SERVICE_ROW_ID="{0}" 
PARAM_CODE="xmldecodename({1})" 
PARAM_VALUE="{2}" 
NEW_PARAM_VALUE="{3}"  />]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="ServiceIdentifier" Type="Wellknown"/>
    <Item Name="ParamCode" Type="LocalParameter"/>
    <Item Name="ParamValue" Type="LocalParameter"/>
	<Item Name="NewParamValue" Type="LocalParameter"/>
  </Request>
  <ParametersList>
    <Parameter Name="ParamCode">
      <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList/ServiceList/*[1]/*[1]" SelectMode="NodeName"/>
    </Parameter>
    <Parameter Name="ParamValue">
      <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList/ServiceList/*[1]/*[1]/Value[string-length(.) > 0]"
              AbsenceValue="" PostFormat="{0}"/>
    </Parameter>
    <Parameter Name="NewParamValue">
      <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList/ServiceList/*[1]/*[1]/NewValue[string-length(.) > 0]"
              AbsenceValue="" PostFormat="{0}"/>
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="ascii"/>
</Command>
