<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Application Path=".\SCP\configuration.application.xml"/>
	<Settings>
		<Add Key="ReportTimeout" Value="0"/>
		<Add Key="Requests.MaxQueuedPerHost" Value="300"/>
		<Add Key="Requests.PollPeriod" Value="5"/>
		<Add Key="Requests.PollIdlePeriod" Value="2"/>
		<Add Key="Responses.WritersCount" Value="5"/>
		<Add Key="ProfilerConfiguration" Value="Profiler_Full.xml" />
