		<Add Key="AsyncScenario" Value="true" />
		<Add Key="CompileAsyncResponse" Value="true" />
		<Add Key="AsyncResponseTimeout" Value="1000" />
		<Add Key="ComplieResponse.PollPeriod" Value="10"/>
		<Add Key="ComplieResponse.PollIdlePeriod" Value="10"/>
