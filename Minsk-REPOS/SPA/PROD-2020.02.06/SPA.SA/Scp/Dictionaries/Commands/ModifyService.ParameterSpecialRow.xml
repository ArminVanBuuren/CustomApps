    <Parameter Name="ParamValue">
      <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList//*[1]/*[1]/NewValue[string-length(.) > 0]"
              AbsenceValue="" PostFormat="{0}"/>
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="ascii"/>
