<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<serviceInstances>
		<serviceInstance hardwareID="HL:100">
		<scenarioSource type="FORIS.ServiceProvisioning.ServiceActivator.ScenarioSource.FileScenarioSource, FORIS.SPA.ServiceActivator.Core">
				<fileScenarioSource dir=".\WLL\Scenarios"/>
			</scenarioSource>
			<socketList poolSize="10" maxConnections="10" minConnections="1" averageTime="1000" deltaAverageTime="4000" maxCommandsPerPeriod="300" periodDuration="10" autoconnect="0" reconfigurationTimeout="0">
				<socket type="FORIS.ServiceProvisioning.Communication.Script.ManagedScriptSocket, FORIS.SPA.Communication">
					<config workingmode="wmGeneral">
						<socket type="FORIS.ServiceProvisioning.Communication.TelnetSocket, FORIS.SPA.Communication">
							<config host="172.24.67.145" port="7776"/>
						</socket>
						<Emulation Success="SUCCESS0001" Failure="ERR1017" RandomizePrintout="false" OverwriteCommandPrintout="true"  Lifetime="00:00:00.00"/>
						<Timers IdleTimeout="20"/>
						<PredefinedScripts Login="Login" Command="Command" Idle="NoSleep" Logoff="Logoff"/>
						<ScriptsList>
							<Script Name="Login" Path=".\WLL\Dictionaries\Scripts\Login.xml"/>
							<Script Name="Logoff" Path=".\WLL\Dictionaries\Scripts\Logoff.xml"/>
							<Script Name="Command" Path=".\WLL\Dictionaries\Scripts\Command.xml"/>
							<Script Name="NoSleep" Path=".\WLL\Dictionaries\Scripts\nosleep.xml"/>
						</ScriptsList> 
					</config> 
				</socket> 
			</socketList>
			<context>
				<object name="dictionary" type="FORIS.ServiceProvisioning.Dictionary.ServiceActivatorDictionary, FORIS.SPA.Dictionary">
					<config path=".\WLL\Dictionaries" xml="Dictionary.xml"/>
				</object>
				<object name="repetitionTable" type="FORIS.ServiceProvisioning.MmlProcessing.SPF.RepetitionTable, FORIS.SPA.MmlProcessing">
					<fileTableSource path=".\WLL\Misc" xml="RepetitionTable.xml" /> 
				</object>
			</context>
		</serviceInstance>
	</serviceInstances>
</Configuration>
