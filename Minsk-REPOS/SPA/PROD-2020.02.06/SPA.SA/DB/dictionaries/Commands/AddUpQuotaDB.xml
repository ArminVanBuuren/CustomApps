<?xml version="1.0" encoding="UTF-8"?>
<Command Name="AddUpQuotaDB" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
	<Description>Пополнение квоты </Description>
	<Author Name="YAI" Date="2014-02-03+15:10"/>
	<Properties GenerationType="Database" />
	<Request Template="function spa_func.f_add_request({0},{1},{2},{3}) return number">
		<Item Name="MSISDN" Type="DatabaseParameter"/>
		<Item Name="ServiceID" Type="LocalDatabaseParameter"/>		
		<Item Name="Action" Type="LocalDatabaseParameter"/>		
		<Item Name="add_param" Type="DatabaseParameter"/>
	</Request>
	<DatabaseParametersList>
		<DatabaseParameter Name="ServiceID">
			<Properties DatabaseArgumentName="service_id" DatabaseArgumentType="number" />
				<Source Path="dummy" AbsenceValue="27880" />
			<PossibleValues StringToKeyTable="true"/>
		</DatabaseParameter>
		<DatabaseParameter Name="Action">
			<Properties DatabaseArgumentName="event_id" DatabaseArgumentType="number" />
				<Source Path="dummy" AbsenceValue="1" />
			<PossibleValues StringToKeyTable="true"/>
		</DatabaseParameter>		
	</DatabaseParametersList>	
	<ErrorsList CodePage="ASCII">
		<Error Name="&lt;returned value=&quot;1&quot; /&gt;" Value="SUCCESS"/>
		<Error Name="&lt;returned value=&quot;0&quot; /&gt;" Value="BalanceLessThanMin"/> 
		<Error Name="&lt;returned value=&quot;-1&quot; /&gt;" Value="ResourceLimitation"/> 		
		<Error Name="&lt;returned value=&quot;-2&quot; /&gt;" Value="FAILURE"/> 	
		<Error Name="&lt;returned value=&quot;-3&quot; /&gt;" Value="FAILURE"/> 			
	</ErrorsList>
</Command>
