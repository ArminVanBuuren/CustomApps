	<automat>
		<node name="BySubscribersSplitter" object="BySubscribersSplitter">
			<resultlist>
				<result value="0" node="BySubscriberRequestDequeuer"/>
			</resultlist>
			<parameterslist>
				<param name="queueName" value="requestsSplittedBySubscriberQueue"/>
			</parameterslist>
		</node>
		<node name="BySubscriberRequestDequeuer" object="RequestDequeuer">
			<resultlist>
				<result value="0" node="GetServiceSNode"/>
				<result value="1" node=""/>
			</resultlist>
			<parameterslist>
				<param name="queueName" value="requestsSplittedBySubscriberQueue"/>
			</parameterslist>
		</node>
		<node name="GetServiceSNode" object="DataBaseProcessor">
			<resultlist>
				<result value="" node="IfRoamingServ_DB_Node" /> 
			</resultlist>
			<parameterslist>
				<param name="DataBaseName" value="FORIS.SPA.BILLING" /> 
				<param name="FunctionName" value="select /*+ ordered use_nl(t,an,ar,rb) */ 
 an.val as SERVICES,rb.code as OPER 
 from CRM.REQ_EXT_LNK t, crm.att_num an,  crm.att_ref ar, crm.att_ref_book rb
  where t.ext_req_id='163.1625030287' 
  and t.req_id=an.req_id and an.att_type_id=87
  and t.req_id=ar.req_id and ar.att_type_id=170
  and ar.att_ref_book_id=rb.att_ref_book_id" /> 
				<param name="FunctionType" value="customQuery" /> 
				<param name="OutXml" value="db_get_reqservices" /> 
				<param name="DbParameterName:0" value="i_customer" /> 
				<param name="DbParameterDirection:0" value="Input" /> 
				<param name="DbParameterType:0" value="Varchar2" /> 
				<param name="DbParameterXpathValue:0" value="ExtendedRequestMessage/OriginalID" /> 
			</parameterslist>
		</node>	

		<node name="IfRoamingServ_DB_Node" object="IfMultipleXmlConditionsResolver">
			<resultlist>
				<result value="0" node="ExitDummyResponderNode" /> 
				<result value="1" node="IfRoamingServ_DB2_Node" /> 
			</resultlist>
			<parameterslist>
				<param name="ObjectNameInContext" value="db_get_reqservices" /> 
				<param name="OrXmlPath:0" value="/root/return/record[column/@name='SERVICES' and column/text()='6105' and column/@name='OPER' and column/text()='1']" /> 
				<param name="OrXmlPath:1" value="/root/return/record[column/@name='SERVICES' and column/text()='6514' and column/@name='OPER' and column/text()='1']" /> 
				<param name="IfXmlPath:0" value="/root/return/record[column/@name='SERVICES' and column/text()='8717' and column/@name='OPER' and column/text()='0']" /> 
			</parameterslist>
		</node>	
		<node name="IfRoamingServ_DB2_Node" object="IfMultipleXmlConditionsResolver">
			<resultlist>
				<result value="0" node="ExitDummyResponderNode" /> 
				<result value="1" node="ExitDummyResponderERRNode" /> 
			</resultlist>
			<parameterslist>
				<param name="ObjectNameInContext" value="db_get_reqservices" /> 
				<param name="OrXmlPath:0" value="/root/return/record[column/@name='SERVICES' and column/text()='6105' and column/@name='OPER' and column/text()='1']" /> 
				<param name="OrXmlPath:1" value="/root/return/record[column/@name='SERVICES' and column/text()='6514' and column/@name='OPER' and column/text()='1']" /> 
				<param name="IfXmlPath:0" value="/root/return/record[column/@name='SERVICES' and column/text()='8717' and column/@name='OPER' and column/text()='1']" /> 
			</parameterslist>
		</node>			
		<node name="ExitDummyResponderNode" object="DummyResponder">
			<resultlist>
				<result value="" node="ResponseAssembler"/>
			</resultlist>
		<parameterslist>
			<param name="OverallResult" value="SUCCESS"/>
			<param name="DetailedResult" value="SUCCESS"/>
			<param name="Delay" value="0"/>
		</parameterslist>
		</node>			
		<node name="ExitDummyResponderERRNode" object="DummyResponder">
			<resultlist>
				<result value="" node="ResponseAssembler"/>
			</resultlist>
		<parameterslist>
			<param name="OverallResult" value="FAILURE"/>
			<param name="DetailedResult" value="FAILURE"/>
			<param name="Delay" value="0"/>
		</parameterslist>
		</node>			
		<node name="ResponseAssembler" object="ResponseAssembler">
			<resultlist>
				<result value="" node="BySubscriberRequestDequeuer"/>
			</resultlist>
		</node>
	</automat>
