<?xml version="1.0" encoding="UTF-8"?>
<scenario name="DeleteSubscriberOnQS">
	<objectList>
		<object name="BySubscribersSplitter" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.RequestSplitterBySubscribers" assembly="FORIS.SPA.MmlProcessing.dll"/>
		<object name="RequestDequeuer" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.RequestDequeuer" assembly="FORIS.SPA.MmlProcessing.dll"/>
		<object name="Commander" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.Processors.RequestProcessor" assembly="FORIS.SPA.MmlProcessing.dll"/>
		<object name="ResponseAssembler" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.ResponseAssembler" assembly="FORIS.SPA.MmlProcessing.dll"/>
		<object name="Switch" class="FORIS.ScenarioProcessing.CommonUnits.ParametersSwitch" assembly="FORIS.SPA.SPF.dll"/>
		<object name="DummyResponder" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.DummyResponder" assembly="FORIS.SPA.MmlProcessing.dll" />
		<object name="DataBaseProcessor" class="FORIS.ScenarioProcessing.CommonUnits.DataBaseProcessor" assembly="FORIS.SPA.SPF.dll" />	
		<object name="XmlNodesCopier" class="FORIS.ScenarioProcessing.CommonUnits.XmlNodesCopier" assembly="FORIS.SPA.SPF.dll" />
		<object name="IfCommander" class="FORIS.ServiceProvisioning.MmlProcessing.SPF.Processors.RequestProcessorConditional" assembly="FORIS.SPA.MmlProcessing.dll" />	
		<object name="IfMultipleXmlConditionsResolver" class="FORIS.ScenarioProcessing.CommonUnits.IfMultipleXmlConditionsResolver" assembly="FORIS.SPA.SPF.dll" />		
	</objectList>
	<automat>
		<node name="BySubscribersSplitter" object="BySubscribersSplitter">
			<resultlist>
				<result value="0" node="BySubscriberRequestDequeuer"/>
			</resultlist>
			<parameterslist>
				<param name="queueName" value="requestsSplittedBySubscriberQueue"/>
			</parameterslist>
		</node>
		<node name="BySubscriberRequestDequeuer" object="RequestDequeuer">
			<resultlist>
				<result value="0" node="IfFRBLKENDNode"/>
				<result value="1" node=""/>
			</resultlist>
			<parameterslist>
				<param name="queueName" value="requestsSplittedBySubscriberQueue"/>
			</parameterslist>
		</node>
		<node name="IfFRBLKENDNode" object="IfMultipleXmlConditionsResolver">
			<resultlist>
				<result value="0" node="DummyResponderNode"/>
				<result value="1" node="SwitchNDCNode"/>
			</resultlist>
			<parameterslist>
				<param name="OrXmlPath:0" value="ExtendedRequestMessage/AdditionalData/RegisteredList/FRBLKEND"/>
				<param name="OrXmlPath:1" value="ExtendedRequestMessage/AdditionalData/RegisteredList/FR9103"/>
			</parameterslist>
		</node>
		<node name="SwitchNDCNode" object="Switch">
			<resultlist>
				<result value="1" node="DeleteSubscriberOnQPNode"/>
				<result value="2" node="DataBaseProcessor"/>
			</resultlist>
			<parameterslist>
				<param name="XmlPath" value="xmlsubstr(&quot;ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN&quot;,0,5)"/>
				<param name="Value:2" value="37500"/>
			</parameterslist>
		</node>	
		<node name="DataBaseProcessor" object="DataBaseProcessor">
			<resultlist>
				<result value="" node="ExtractUSERNAMEFTTX" /> 
			</resultlist>
			<parameterslist>
				<param name="DataBaseName" value="FORIS.SPA.SDP" /> 
				<param name="FunctionName" value="select nvl(max(a1.att_value),0) as USERNAMEFTTX
       from cust.terminal_device@billing_crm td
       left join lop_converg_att a1 on td.terminal_device_id = a1.tdc_id and a1.att_id = 5 and sysdate between a1.fd and nvl(a1.td,sysdate)
      where 1=1
       and td.msisdn = {0}
	   and td.tariff_plan_id = 8000
       and td.date_to is null" /> 
				<param name="FunctionType" value="customQuery" /> 
				<param name="OutXml" value="db_get_username" /> 
				<param name="DbParameterName:0" value="i_customer" /> 
				<param name="DbParameterDirection:0" value="Input" /> 
				<param name="DbParameterType:0" value="NVarchar2" /> 
				<param name="DbParameterXpathValue:0" value="ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN" /> 
			</parameterslist>
		</node>
		<node name="ExtractUSERNAMEFTTX" object="XmlNodesCopier">
			<resultlist>
				<result value="" node="IFUSERNAMENode" /> 
			</resultlist>
			<parameterslist>
				<param name="SrcXml" value="db_get_username" /> 
				<param name="DstMessage" value="xmlrequest" /> 
				<param name="CopyContentType" value="InnerText" /> 
				<param name="SrcXmlPath:0" value="root/return/record/column[@name='USERNAMEFTTX']" /> 
				<param name="DstXmlPath:0" value="ExtendedRequestMessage/AdditionalData/UserName" /> 
			</parameterslist>
		</node>
		<node name="IFUSERNAMENode" object="Switch">
			<resultlist>
				<result value="1" node="ReleaseSessionUserNameNode"/>
				<result value="2" node="DeleteSubscriberOnQPNode"/>
			</resultlist>
			<parameterslist>
				<param name="XmlPath" value="ExtendedRequestMessage/AdditionalData/UserName"/>
				<param name="Value:2" value="0"/>
			</parameterslist>
		</node>	
		<node name="ReleaseSessionUserNameNode" object="Commander">
			<resultlist>
				<result value="0" node="DeleteSubscriberOnQPNode"/>
				<result value="" node="ResponseAssembler"/>
			</resultlist>
			<parameterslist>
				<param name="Command" value="ReleaseSessionUserName"/>
				<param name="Script" value="Command"/>
				<param name="ErrorDecodingType" value="ByString"/>
				<param name="CommandSource" value="Dictionary"/>
				<param name="DynamicObject" value="(empty)"/>
			</parameterslist>
		</node>		
		<node name="DeleteSubscriberOnQPNode" object="Commander">
			<resultlist>
				<result value="" node="ResponseAssembler"/>
			</resultlist>
			<parameterslist>
				<param name="Command" value="DeleteSubscriber"/>
				<param name="Script" value="Command"/>
				<param name="ErrorDecodingType" value="ByString"/>
				<param name="CommandSource" value="Dictionary"/>
				<param name="DynamicObject" value="(empty)"/>
			</parameterslist>
		</node>
		<node name="DummyResponderNode" object="DummyResponder">
			<resultlist>
				<result value="" node="ResponseAssembler"/>
			</resultlist>
		<parameterslist>
			<param name="OverallResult" value="SUCCESS"/>
			<param name="DetailedResult" value="SUCCESS"/>
			<param name="Delay" value="0"/>
		</parameterslist>
		</node>			
		<node name="ResponseAssembler" object="ResponseAssembler">
			<resultlist>
				<result value="" node="BySubscriberRequestDequeuer"/>
			</resultlist>
		</node>
	</automat>
</scenario>
