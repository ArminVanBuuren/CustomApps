<?xml version="1.0" encoding="UTF-8"?>
<Command Name="ChangeMSISDNonHL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
	<Description>Change</Description>
	<Author Name="YAI" Date="2015-02-17+15:50"/>
	<Properties GenerationType="Database" />
	<Request Template="function  spa_func.f_add_request({0},{1},{2},MSISDN={3};TDC_ID={4}) return number">
		<Item Name="MSISDN" Type="DatabaseParameter"/>
		<Item Name="ServiceID" Type="LocalDatabaseParameter"/>		
		<Item Name="Action" Type="LocalDatabaseParameter"/>		
		<Item Name="NewMSISDN" Type="LocalDatabaseParameter"/>	
		<Item Name="TDC_ID" Type="LocalDatabaseParameter"/>
	</Request>
	<DatabaseParametersList>
		<DatabaseParameter Name="ServiceID">
			<Properties DatabaseArgumentName="service_id" DatabaseArgumentType="number" />
				<Source Path="dummy" AbsenceValue="50" />
			<PossibleValues StringToKeyTable="true"/>
		</DatabaseParameter>
		<DatabaseParameter Name="Action">
			<Properties DatabaseArgumentName="event_id" DatabaseArgumentType="number" />
				<Source Path="dummy" AbsenceValue="1" />
			<PossibleValues StringToKeyTable="true"/>
		</DatabaseParameter>
		<DatabaseParameter Name="NewMSISDN">
			<Properties DatabaseArgumentName="Additional_param" DatabaseArgumentType="varchar2" />
			<Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList//NewMSISDN"/>
			<PossibleValues StringToKeyTable="true"/>
		</DatabaseParameter>
		<DatabaseParameter Name="TDC_ID">
			<Properties DatabaseArgumentName="Additional_param" DatabaseArgumentType="varchar2" />
			<Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList//TerminalDeviceId"/>
			<PossibleValues StringToKeyTable="true"/>
		</DatabaseParameter>		
	</DatabaseParametersList>	
	<ErrorsList CodePage="ASCII"/>
</Command>
