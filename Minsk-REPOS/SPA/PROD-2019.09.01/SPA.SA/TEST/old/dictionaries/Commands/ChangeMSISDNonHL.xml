<?xml version="1.0" encoding="UTF-8"?>
<Command Name="ChangeMSISDNonHL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
	<Description>Change</Description>
	<Author Name="YAI" Date="2015-02-17+15:50"/>
	<Properties GenerationType="Database" />
	<Request Template="select billing.spa_func.f_add_request({0},{1},{2},'MSISDN_NEW:{3};TDC_ID:{4};') from dual">
		<Item Name="MSISDN" Type="LocalParameter"/>
		<Item Name="ServiceID" Type="LocalParameter"/>		
		<Item Name="Action" Type="LocalParameter"/>		
		<Item Name="NewMSISDN" Type="LocalParameter"/>
		<Item Name="TDI" Type="LocalParameter" /> 
	</Request>
	<ParametersList>
		<Parameter Name="MSISDN">
			<Source Path="ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN"/>
			<PossibleValues StringToKeyTable="true"/>
		</Parameter>	
		<Parameter Name="ServiceID">
			<Source Path="dummy" AbsenceValue="50" />
			<PossibleValues StringToKeyTable="true"/>
		</Parameter>	
			<Parameter Name="Action">
			<Source Path="dummy" AbsenceValue="1" />
			<PossibleValues StringToKeyTable="true"/>
		</Parameter>
		<Parameter Name="NewMSISDN">
			<Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList//NewMSISDN"/>
			<PossibleValues StringToKeyTable="true"/>
		</Parameter>	
		<Parameter Name="TDI">
			<Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList//TerminalDeviceId"/>
			<PossibleValues StringToKeyTable="true"/>
		</Parameter>		
	</ParametersList>		
	<ErrorsList CodePage="ASCII">
		<Error Name="&quot; value=&quot;1&quot; /&gt;" Value="SUCCESS"/>
		<Error Name="&quot; value=&quot;0&quot; /&gt;" Value="SUCCESS"/> 
		<Error Name="&quot; value=&quot;-1&quot; /&gt;" Value="ResourceLimitation"/> 		
		<Error Name="&quot; value=&quot;-2&quot; /&gt;" Value="FAILURE"/> 	
		<Error Name="&quot; value=&quot;-3&quot; /&gt;" Value="FAILURE"/> 	
	</ErrorsList>
</Command>
