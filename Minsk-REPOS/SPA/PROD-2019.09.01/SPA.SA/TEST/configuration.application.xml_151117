<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<serviceInstances>
		<serviceInstance hardwareID="HL:5555">
			<scenarioSource type="FORIS.ServiceProvisioning.ServiceActivator.ScenarioSource.FileScenarioSource, FORIS.SPA.ServiceActivator.Core">
				<fileScenarioSource dir=".\Test\Scenarios"/>
			</scenarioSource>
			<socketList poolSize="8" maxConnections="4" minConnections="1" averageTime="1000" deltaAverageTime="4000" maxCommandsPerPeriod="50" periodDuration="10" autoconnect="1" reconfigurationTimeout="0">
				<socket type="FORIS.ServiceProvisioning.Communication.Script.ManagedScriptSocket, FORIS.SPA.Communication">
					<config workingmode="wmGeneral">
						<socket type="FORIS.ServiceProvisioning.Communication.HttpSocket, FORIS.SPA.Communication">
							<config>
								<ParametersList>
									<Parameter Key="Host" Value="172.24.220.65"/>  
									<Parameter Key="Port" Value="8080"/>
									<Parameter Key="Url" Value="ua/soap"/>
									<Parameter Key="MethodName" Value="POST"/>
									<Parameter Key="Method" Value="POST"/>
									<Parameter Key="UserName" Value=""/>
									<Parameter Key="UserPwd" Value=""/>
									<Parameter Key="ContentType" Value="application/soap+xml; charset=utf-8"/>
									<Parameter Key="ParameterName" Value=""/>
								</ParametersList>
							</config>
						</socket>
						<Emulation Success="AddResult&gt;true&lt;/AddResult&gt;" Failure="DeleteResult&gt;false&lt;/DeleteResult&gt;" RandomizePrintout="false" OverwriteCommandPrintout="true"  Lifetime="00:00:00.00"/>
						<Timers IdleTimeout="6000"/>
						<PredefinedScripts Login="Login" Command="Command" Idle="" Logoff=""/>
						<ScriptsList> 
							<Script Name="Login" Path=".\Test\Dictionaries\Scripts\Login.xml"/>
							<Script Name="Logoff" Path=".\Test\Dictionaries\Scripts\logoff.xml"/>
							<Script Name="Command" Path=".\Test\Dictionaries\Scripts\Command.xml"/>
							<Script Name="NoSleep" Path=".\Test\Dictionaries\Scripts\nosleep.xml"/>
						</ScriptsList> 
					</config> 
				</socket> 
			</socketList>
			<context>
				<object name="dictionary" type="FORIS.ServiceProvisioning.Dictionary.ServiceActivatorDictionary, FORIS.SPA.Dictionary">
					<config path=".\Test\Dictionaries" xml="Dictionary.xml"/>
				</object>
				<object name="repetitionTable" type="FORIS.ServiceProvisioning.MmlProcessing.SPF.RepetitionTable, FORIS.SPA.MmlProcessing">
					<fileTableSource path=".\Test\Misc" xml="RepetitionTable.xml"/>
				</object>					
			</context>
		</serviceInstance>
	</serviceInstances>
</Configuration>
