 <node name="GetServicePD" object="DataBaseProcessor">
 <resultlist>
  <result value="" node="IfCheckSRVNode" /> 
  </resultlist>
 <parameterslist>
  <param name="DataBaseName" value="FORIS.SPA.BILLING" /> 
  <param name="FunctionName" value="select r.service_code as SERVICEPD
from cust.terminal_device_service s, cust.terminal_device t, rd.service r
where s.terminal_device_id=t.terminal_device_id
      and t.date_to is null
      and r.service_id=s.service_id
      and t.msisdn = {0}
      and r.service_code in ('CB410153','FR511001','CB412703','FR1058','FR7480','FR812','FR9484')
     AND sysdate between s.date_from  and nvl(s.date_to, sysdate)" /> 
  <param name="FunctionType" value="customQuery" /> 
  <param name="OutXml" value="db_get_service_PD" /> 
  <param name="DbParameterName:0" value="i_customer" /> 
  <param name="DbParameterDirection:0" value="Input" /> 
  <param name="DbParameterType:0" value="NVarchar2" /> 
  <param name="DbParameterXpathValue:0" value="ExtendedRequestMessage/IdentifiersList/Identifier/MSISDN" /> 
  </parameterslist>
  </node>	
  
- <node name="IfCheckSRVNode" object="IfMultipleXmlConditionsResolver">
- <resultlist>
  <result value="0" node="ExitDummyResponderNode" /> 
  <result value="1" node="IfCheckSRV2Node" /> 
  </resultlist>
- <parameterslist>
  <param name="OrXmlPath:0" value="ExtendedRequestMessage/AdditionalData/RegisteredList/CB410153" /> 
  </parameterslist>
  </node>
  - <node name="IfCheckSRV2Node" object="IfMultipleXmlConditionsResolver">
- <resultlist>
  <result value="0" node="ExitDummyResponderNode" /> 
  <result value="1" node="" /> 
  </resultlist>
- <parameterslist>
  <param name="XmlNameInContext" value="$db_get_service_PD" />
  <param name="OrXmlPath:0" value="//CB410153" /> 
  </parameterslist>
  </node>
- <node name="ExitDummyResponderNode" object="DummyResponder">
- <resultlist>
  <result value="" node="ResponseAssembler" /> 
  </resultlist>
- <parameterslist>
  <param name="OverallResult" value="SUCCESS" /> 
  <param name="DetailedResult" value="SUCCESS" /> 
  <param name="Delay" value="0" /> 
  </parameterslist>
  </node>		
  <node name="DateFromCopy" object="XmlNodesCopier">
            <resultlist>
            <result value="" node=""/>
         </resultlist>
        <parameterslist>
            <param name="SrcXml" value="db_get_service_PD"/>
            <param name="DstMessage" value="xmlrequest"/>
            <param name="CopyContentType" value="InnerText"/>   
            <param name="SrcXmlPath:0" value="root/return/record/column[@name='SERVICEPD']"/>
            <param name="DstXmlPath:0" value="ExtendedRequestMessage/AdditionalData/RegisteredList"/>
        </parameterslist>
        </node>		
		<node name="ResponseAssembler" object="ResponseAssembler">
