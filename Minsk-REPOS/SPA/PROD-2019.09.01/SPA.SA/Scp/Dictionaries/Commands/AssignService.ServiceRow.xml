<?xml version="1.0" encoding="UTF-8"?>
<Command Name="AssignService.ServiceRow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>AssignService (services table)</Description>
  <Author Name="Oleg Safonov" Date="2011-02-08+12:35"/>
  <Properties GenerationType="MmlXml" />
  <Request>
    <RequestTemplate>
      <![CDATA[<Row
SERVICE_ROW_ID="{9}"
REQUEST_ID="reversecut({6},4)-{8}"
TERMINAL_DEVICE_CONTRACT_ID="{0}" 
SERVICE_CODE="xmldecodename({1})" 
{2} {3} {4} {5}
SERVICE_ACTION="A"/>]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="TerminalDeviceID" Type="Wellknown"/>
    <Item Name="Service" Type="LocalParameter"/>
    <Item Name="DateFrom" Type="LocalParameter"/>
    <Item Name="CounterCoefficient" Type="LocalParameter"/>
    <Item Name="DiscountType" Type="LocalParameter"/>
    <Item Name="DiscountValue" Type="LocalParameter"/>
    <Item Name="org.id" Type="Wellknown"/>
    <Item Name="id" Type="Wellknown"/>
    <Item Name="GlobalUid" Type="Wellknown" />
    <Item Name="ServiceIdentifier" Type="Wellknown"/>
  </Request>
  <ParametersList>
    <Parameter Name="Service">
      <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList/*" AbsenceValue=""/>
      <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList/FavoritNumberList/*" AbsenceValue=""/>
    </Parameter>
    <Parameter Name="DateFrom">
      <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList/*[1]/DateFrom[string-length(.) > 0]"
              AbsenceValue="" PostFormat="DATE_FROM=&quot;{0}&quot;"/>
    </Parameter>
    <Parameter Name="CounterCoefficient">
      <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList/*[1]/CounterCoefficient[string-length(.) > 0]"
              AbsenceValue="" PostFormat="COUNTER_COEFFICIENT=&quot;{0}&quot;"/>
    </Parameter>
    <Parameter Name="DiscountType">
      <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList/*[1]/DiscountType[string-length(.) > 0]"
              AbsenceValue="" PostFormat="DISCOUNT_TYPE=&quot;{0}&quot;"/>
    </Parameter>
    <Parameter Name="DiscountValue">
      <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList/*[1]/DiscountValue[string-length(.) > 0]"
              AbsenceValue="" PostFormat="DISCOUNT_VALUE=&quot;{0}&quot;"/>
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="ascii"/>
</Command>
