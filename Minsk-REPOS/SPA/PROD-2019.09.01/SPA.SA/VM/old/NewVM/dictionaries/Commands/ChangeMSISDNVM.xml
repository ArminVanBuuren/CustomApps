<?xml version="1.0" encoding="UTF-8"?>
<Command Name="ChangeMSISDNVM" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
	<Description>Замена номер VM </Description>
	<Author Name="YAI" Date="2016-03-21+12:50"/>
	<Properties GenerationType="Database" />
	<Request Template="function spa_func.f_change_msisdn_voicemail({0},{1}) return number">
		<Item Name="MSISDN" Type="DatabaseParameter"/>
		<Item Name="NewMSISDN" Type="LocalDatabaseParameter" />
	</Request>
	<DatabaseParametersList>
		<DatabaseParameter Name="NewMSISDN">
			<Properties DatabaseArgumentName="newmsisdn" DatabaseArgumentType="varchar2" />
			<Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList//NewMSISDN"/>
			<PossibleValues StringToKeyTable="true"/>
		</DatabaseParameter>
	</DatabaseParametersList>	
	<ErrorsList CodePage="ASCII">
		<Error Name="&lt;returned value=&quot;10&quot; /&gt;" Value="SUCCESS"/> 
		<Error Name="&lt;returned value=&quot;11&quot; /&gt;" Value="SUCCESS"/> 
	</ErrorsList>
</Command>
