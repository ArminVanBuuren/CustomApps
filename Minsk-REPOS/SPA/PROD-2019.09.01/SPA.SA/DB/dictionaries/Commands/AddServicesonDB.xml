<?xml version="1.0" encoding="UTF-8"?>
<Command Name="AddServicesonDB" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
	<Description>Добавление сервисов</Description>
	<Author Name="YAI" Date="2018-02-08+16:50"/>
	<Properties GenerationType="Database" />
	<Request Template="function spa_func.f_prov_misc_service({0},{1},{2}) return number">
		<Item Name="MSISDN" Type="DatabaseParameter"/>
		<Item Name="ServiceID" Type="LocalDatabaseParameter"/>		
		<Item Name="Action" Type="LocalDatabaseParameter"/>		
	</Request>
	<DatabaseParametersList>
		<DatabaseParameter Name="ServiceID">
			<Properties DatabaseArgumentName="service_id" DatabaseArgumentType="number" />
				<Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList" />
			<PossibleValues StringToKeyTable="false">
				<Item Key="FR15329" StringValue="65395"/>
				<Item Key="FR15636" StringValue="66499"/>
			</PossibleValues>	
		</DatabaseParameter>
		<DatabaseParameter Name="Action">
			<Properties DatabaseArgumentName="flag" DatabaseArgumentType="number" />
				<Source Path="dummy" AbsenceValue="1" />
			<PossibleValues StringToKeyTable="true"/>
		</DatabaseParameter>		
	</DatabaseParametersList>	
	<ErrorsList CodePage="ASCII"/>
</Command>
