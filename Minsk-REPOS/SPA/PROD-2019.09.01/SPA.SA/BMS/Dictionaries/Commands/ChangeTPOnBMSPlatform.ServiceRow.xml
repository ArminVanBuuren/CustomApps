<?xml version="1.0" encoding="utf-8"?>
<Command Name="ChangeTPOnBMSPlatform.ServiceRow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Description>Change Tariff Plan (services table)</Description>
  <Author Name="Oleg Safonov" Date="2011-02-08+12:35"/>
  <Properties GenerationType="MmlXml" />
  <Request>
    <RequestTemplate>
      <![CDATA[<Row 
TerminalDeviceID="{0}" 
PersonalAccountNumber="{1}" 
Msisdn="{2}"
TariffPlanId="{3}"
TariffPlanIdFlag="true"
DateFrom="{4}"/>]]>
    </RequestTemplate>
    <EmulationPrintoutTemplate><![CDATA[]]></EmulationPrintoutTemplate>
    <Item Name="TerminalDeviceID" Type="Wellknown"/>
    <Item Name="PersonalAccount" Type="Wellknown"/>
    <Item Name="msisdn" Type="Wellknown"/>
    <Item Name="TariffPlanId" Type="LocalParameter"/>
    <Item Name="DateFrom" Type="LocalParameter"/>
  </Request>
  <ParametersList>
    <Parameter Name="DateFrom">
      <Source Path="ExtendedRequestMessage/AdditionalData/RegisteredList/BMSList//DateFrom"/>
      <PossibleValues StringToKeyTable="true"/>
    </Parameter>
    <Parameter Name="TariffPlanId">
      <Source Path="ExtendedRequestMessage/AdditionalData/ProvisionList/ChangeTP/NewTariffPlanId"/>
      <PossibleValues StringToKeyTable="true"/>
    </Parameter>
  </ParametersList>
  <ErrorsList CodePage="ascii"/>
</Command>
