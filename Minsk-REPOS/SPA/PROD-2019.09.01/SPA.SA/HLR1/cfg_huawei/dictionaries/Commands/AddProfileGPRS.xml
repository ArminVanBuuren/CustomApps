<?xml version="1.0" encoding="UTF-8"?>
<Command Name="AddProfileGPRS" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Command.xsd">
	<Description>>Добавление GPRS профиль</Description>
	<Author Name="YAI" Date="2011-09-14+12:18"/>
	<Request Template="MOD OPTGPRS: ISDN=&quot;{0}&quot;, PROV=ADDPDPCNTX, APN_TYPE=PS_APN, APNTPLID=1, QOSTPLID=1, PDPTYPE=IPV4, ADDIND=DYNAMIC, VPLMN=FALSE, CHARGE=NORMAL;">
		<Item Name="msisdn" Type="Wellknown"/>
	</Request>
	<ErrorsList CodePage="ASCII">
		<Error Name="ERR3197" Value="SUCCESS" />
	</ErrorsList>
</Command>
