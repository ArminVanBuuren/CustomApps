<?xml version="1.0" encoding="utf-8" ?>
<plugin name="RouterPlugin">
	<queues>
		<queue refName="a1" name=".\router_1" />
		<queue refName="a2" name=".\router_2" />
		<queue refName="a3" name=".\router_3" />
	</queues>
	<condition expression="/Message/@initiator" match="number1">
		<route-to ref="a1" />
	</condition>
	<condition expression="/Message/@initiator" match="number3">
		<route-to ref="a3" />
	</condition>
	<condition expression="/Message/@initiator" match="number_odd">
		<route-to ref="a1" />
		<route-to ref="a3" />
	</condition>
	<condition expression="/Message/@initiator" match="number_all">
		<route-to ref="a1" />
		<route-to ref="a1" />
		<route-to ref="a3" />
	</condition>
	<condition expression="/Message/@initiator" match="number_none">
	</condition>
</plugin>
