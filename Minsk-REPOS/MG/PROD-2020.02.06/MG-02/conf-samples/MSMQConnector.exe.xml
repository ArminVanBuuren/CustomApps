<?xml version="1.0" encoding="utf-8" ?>
<Application configClass="FORIS.MessagingGateway.MSMQConnector.MSMQConnectorConfiguration" type="MSMQCON">

  <Queues>
    <InputQueue path=".\test_input" />
    <OutputQueues>
      <OutputQueue path=".\test_output" name="MOSCOW" timeToBeReceived="1000" timeToReachQueue="10" recoverable="false" />
      <OutputQueue path=".\test_output2" name="OREL" timeToBeReceived="1000" timeToReachQueue="10" recoverable="false" />
    </OutputQueues>  
  </Queues>

  <Settings>
    <Item name="ModuleType" value="MSMQCON" />
    <Item name="ModuleName" value="MSMQCON_01" />
    <Item name="CRMCacheCleanInterval" value="100" />
    <Item name="StoringInterval" value="20" />
    <Item name="StoringHWM" value="200" />
    <Item name="FetchLimit" value="200" />
  </Settings>

  <TemplateProcessors>
    <Processor dll="DefaultTemplates.dll" class="MSMQTemplateProcessor" config="MSMQTemplateProcessor.xml" />
  </TemplateProcessors>

</Application>
