<Application configClass="FORIS.MessagingGateway.SMSConnector.SMSConnectorConfiguration" type="SMSCON">
  <Settings>
    <Item name="ModuleType" value="SMSCON" />
    <Item name="ModuleName" value="RBTCON_01" />
    <Item name="CRMCacheCleanInterval" value="300" />
    <Item name="StoringInterval" value="20" />
    <Item name="StoringHWM" value="200" />
    <Item name="SMSCBindRetryDelay" value="30" />
    <Item name="EnquireLinkInterval" value="60" />
    <Item name="DefaultEncoding" value="ascii" />
    <Item name="RegisteredDelivery" value="false" />
    <Item name="SrcTon" value="1" />
    <Item name="SrcNpi" value="1" />
    <Item name="DstTon" value="1" />
    <Item name="DstNpi" value="1" />
    <Item name="SubmitEsmClass" value="0" />
    <Item name="SubmitUssdServiceOp" value="0" />
    <Item name="SplittingType" value="None" />
    <Item name="ContentField" value="ShortMessage" />
    <Item name="Window" value="200" />
    <Item name="WindowExceededDelay" value="60" />
    <Item name="ResponseTimeout" value="6000" />
    <Item name="SuccessResponseCodes" value="0,40" />
    <Item name="FetchLimit" value="100" />
  </Settings>
  <SMSCenters>
    <!-- Connection host="172.17.134.40" port="5000" systemType="rbt" systemId="mg" pass="mg" / -->
    <Connection host="localhost" port="5000" systemType="rbt" systemId="admin" pass="admin" />
  </SMSCenters>
  <Anumbertransformations />
  <TemplateProcessors defaultConfig="RBTTemplateProcessor.xml">
    <Processor dll="DefaultTemplates.dll" class="SMSTemplateProcessor" config="RBTTemplateProcessor.xml" />
  </TemplateProcessors>
  <Encodings>
    <Table name="iso-8859-1" encoding="0" />
    <Table name="ascii" encoding="1" />
    <Table name="ascii" encoding="2" />
    <Table name="iso-8859-1" encoding="3" />
    <Table name="ascii" encoding="4" special="7bit" />
    <Table name="shift-jis" encoding="5" />
    <Table name="iso-8859-5" encoding="6" />
    <Table name="iso-8859-8" encoding="7" />
    <Table name="unicodefffe" encoding="8" />
  </Encodings>
</Application>