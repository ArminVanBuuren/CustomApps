<Application configClass="FORIS.MessagingGateway.SMSConnector.SMSConnectorConfiguration" type="SMSCON">
  <Settings>
    <Item name="ModuleType" value="SMSCON" />
    <Item name="ModuleName" value="SMSCON_LOAD_02" />
    <Item name="CRMCacheCleanInterval" value="1000" />
    <Item name="SessionCleanInterval" value="5" />
    <Item name="SMSCBindRetryDelay" value="30" />
    <Item name="DefaultCustomerSource" value="111" />
    <Item name="ShortMessageMaxLength" value="0" />
    <Item name="DirectDeliveryTemplate" value="23" />
    <Item name="ErrorDelay" value="600" />
    <Item name="Protocol" value="smpp" />
    <Item name="EnquireLinkInterval" value="30" />
    <Item name="DefaultEncoding" value="unicodefffe" />
    <Item name="Window" value="0" />
    <!-- width of buffer where messages wait submit_resp -->
    <Item name="WindowExceededDelay" value="30" />
    <!-- if window is full after this number of seconds message will be rescheduled -->
    <Item name="ResponseTimeout" value="2000" />
    <!-- message will be dropped from buffer after this number of miliseconds of submit_resp waiting -->
    <Item name="SuccessResponseCodes" value="0,40" />
    <Item name="FetchLimit" value="5" />
    <Item name="SynchroMode" value="false" />
    <Item name="MessagesLimit" value="5" />
    <!--  number of Messages to fetch from DB. If zero, than all -->
    <Item name="StatusToResend" value="20, 88" />
    <!-- resend if submit_resp status code-->
    <Item name="RegisteredDelivery" value="true" />
    <Item name="SrcTon" value="0" />
    <Item name="SrcNpi" value="1" />
    <Item name="DstTon" value="1" />
    <Item name="DstNpi" value="1" />
    <Item name="SubmitEsmClass" value="0" />
    <!-- new setting V3.1 -->
    <Item name="SubmitUssdServiceOp" value="0" />
    <Item name="EnableUdhSplitting" value="false" />
    <!-- new setting V3.1 -->
    <Item name="SplittingType" value="None" />
    <!-- None, UDH-->
    <Item name="ContentField" value="ShortMessage" />
    <!--  PayLoad, ShortMessage  -->
    <Item name="SplitAfter" value="140" />
    <Item name="SplitBy" value="134" />
    <Item name="StoringInterval" value="40" />
    <!-- MG Cache . Save to DB after sec -->
    <Item name="StoringHWM" value="500" />
    <!-- MG Cache. Save DB after count records -->
    <Item name="HealthAlgo" value="Simple" />
    <Item name="HealthParam" value="500" />
    <!--  -->
    <Item name="UtcOffset" value="16+" />
    <!-- winter 12+ , summer 16+   -->
    <Item name="ThreadMultiplifier" value="1" />
    <Item name="SendSpeedLimit" value="5" />
    <!--Item name="ExposeSmsLogService" value="true" /-->
    <Item name="SmsLogging" value="enable" />
  </Settings>
  <SMSCenters>
    <!--Connection host="172.16.160.193" port="3001" systemType="502" systemId="admin" pass="admin" /-->
    <Connection host="10.84.176.246" port="4002" systemType="502" systemId="admin" pass="admin" />
  </SMSCenters>
  <Anumbertransformations>
    <!-- Configuration for St-Peterburg region -->
    <!-- <Anumbertransformation prefix="0000" newprefix="510000" /> -->
    <!-- <Anumbertransformation prefix="+34567" newprefix="35124" /> -->
    <!-- <Anumbertransformation prefix="45" newprefix="55" /> -->
    <!-- <Anumbertransformation prefix="78" newprefix="6" /> -->
    <!-- <Anumbertransformation prefix="+7" newprefix="12311" /> -->
  </Anumbertransformations>
  <TemplateProcessors defaultConfig="SMSTemplateProcessor_LoadTest.xml">
    <!-- Processor dll="DefaultTemplates.dll" class="SMSTemplateProcessor" config="SMSTemplateProcessor4SOAP.xml" /-->
    <Processor dll="DefaultTemplates.dll" class="SMSTemplateProcessor" config="SMSTemplate4GOODOK.xml" />
    <Processor dll="DefaultTemplates.dll" class="SMSTemplateProcessor" config="SMSTemplateProcessor.xml" />
    <!--
  <Processor dll="DefaultTemplates.dll" class="SMSTemplateProcessor" config="SMSTemplateProcessor_Partial.xml" /> -->
  </TemplateProcessors>
  <BlackList propertyName="Sender" regExpBan="^88">
    <Ban>603221981</Ban>
    <Ban>555715</Ban>
  </BlackList>
  <Encodings>
    <!-- These values are set according to SMPP specification -->
    <!-- This line shoudl be set according to SMSC default encoding -->
    <Table name="ascii" encoding="0" />
    <!-- ASCII encoding -->
    <Table name="ascii" encoding="1" />
    <!-- 8-bit binary encoding -->
    <Table name="ascii" encoding="2" />
    <!-- Latin1 (iso-8859-1) encoding -->
    <Table name="iso-8859-1" encoding="3" />
    <!--- 8-bit binary encoding -->
    <Table name="ascii" encoding="4" special="7bit" />
    <!-- Japanese encoding -->
    <Table name="shift-jis" encoding="5" />
    <!-- Cyrillic (iso-8859-5) encoding -->
    <Table name="iso-8859-5" encoding="6" />
    <!-- Larin/Hebrew (iso-8859-8) encoding -->
    <Table name="iso-8859-8" encoding="7" />
    <!-- Unicode encoding 
							There could be 2 types of encoding according to byte order
                                                    - "utf-16" classical (little endian) unicode encoding
                                                    - "unicodeFFFE" big endian unicode encoding -->
    <Table name="unicodefffe" encoding="8" />
    <!-- Values bigger than 8 have different meaning (it is not encoding at all) -->
  </Encodings>
