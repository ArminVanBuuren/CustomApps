		<HeartBeat periodSec="10" />
		<NoActivity defaultThresholdSec="240">
		<MonitoredInterval thresholdSec="60">
				<day from="1" to="5" />
				<hour from="8" to="19" />
			</MonitoredInterval>
		</NoActivity>
		<NoDBConnection  periodSec="30" />
		<NoDeviceConnection  periodSec="10" />
	</Monitors>  
-->
</Application>