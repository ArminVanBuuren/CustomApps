<Application>
   <Queues>
      <!--All queues have to be a non-transactional !-->
      <InputQueue name=".\telcrm_output" />
      <OutputQueue name=".\crmfilter_input" timeToBeReceived="1000" timeToReachQueue="10" recoverable="false">
         <Override connector="SMSCON_01" timeToBeReceived="20" />
         <Override connector="SMSCON_02" timeToBeReceived="20" />
         <Override connector="SMSCON_03" timeToBeReceived="20" />
      </OutputQueue>
   </Queues>
   <Settings>
      <Item name="MaxThreadCount" value="20" />
      <Item name="ModuleName" value="CRMCON_01" />
      <Item name="ModuleType" value="CRMCON" />
      <Item name="CRMCacheCleanInterval" value="600" />
      <Item name="StoringInterval" value="120" />
      <Item name="StoringHWM" value="200" />
      <Item name="ThreadMultiplifier" value="1" />
      <Item name="FetchLimit" value="1000" />
      <Item name="XmlEncoding" value="utf-8" />
      <Item name="SupportLargeMessages" value="true" />
   </Settings>
   <TemplateToConnector>
      <Map template_id="sms -2" connector="SMSCON_02" template_name="DeliveryRequestIssa" />
      <Map template_id="bms -2" connector="SMSCON_02" template_name="DeliveryRequestBMS" />
      <Map template_id="CUGsend" connector="SMSCON_02" template_name="DeliveryRequest" />
      <Map template_id="DirectSendSMS" connector="SMSCON_02" template_name="DeliveryRequestIssa" />
      <Map template_id="SMSAutoInformation" connector="SMSCON_02" template_name="DeliveryRequest" />
      <Map template_id="AutoInformationOfBalance" connector="SMSCON_02" template_name="DeliveryRequest" />
      <Map template_id="SMSAutoInformationPtP" connector="SMSCON_02" template_name="DeliveryRequestPtP" />
     
   </TemplateToConnector>
</Application>